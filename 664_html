    $(document).ready(function() {
		var flg = true;
		$('#usa_image_map area').hover(function (){
			hide = false
			$(this).parent().next('.hover-btn').find('a').text($(this).data('title'));
			$(this).parent().next('.hover-btn').css('visibility','visible');
			if($(this).parent().next('.hover-btn').css('display') == 'none' || $(this).parent().next('.hover-btn').css('margin-top') != ''){
				$(this).parent().next('.hover-btn').slideDown('fast',function(){});
			}
		}, function (){
			hide = true
			timeout = setTimeout(function(){
				clearTimeout(timeout);
				if(hide == true){
					//$('.hover-btn').parent().next('.hover-btn').find('a').text('Bousse');
					$('.hover-btn').slideUp('fast',function(){});
				}
			}, 1000);
		});
    });
    
