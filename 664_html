		<script>
		function sizing() {
			
				var divWidth = $('main .home_actus article').width() - 32; 
				$('#slider .owl-wrapper-outer .owl-wrapper .owl-item .item .item_caption').width(divWidth);
				
				var divWidth2 = $('main .alaune').width()/2; 
				$('main .alaune .alaune_img_content .alaune_img_content_bg').height(divWidth2);
				
				var divWidth3 = $('main .alaune').width()/2; 
				$('main .alaune .home_actus_right').height(divWidth3);
				
				var divWidth4 = $('main .alaune').width()/3-10.6666667; 
				$('main .home_actus article').height(divWidth4);
				$('main .home_actus article .home_actus_right').height(divWidth4);
				
				var windowSize = $(window).width();

				if (windowSize <= 599) {
					var divWidth5 = $('main .home_actus article').width(); 
					var WinWidth = jQuery(window).width()/2;
					var RightPosPag = WinWidth - divWidth5/2 - 0;		
					$(".owl-theme .owl-controls").css('right', RightPosPag);
					var divWidthalaune = $('main .alaune').width(); 		
					$('main .home_actus article').height(divWidthalaune);
					$('main .home_actus article .home_actus_right').height(divWidth2);
				}
				else if (windowSize <= 1040) {
					var divWidth5 = $('main .home_actus article').width(); 
					var WinWidth = jQuery(window).width()/2;
					var RightPosPag = WinWidth - divWidth5/2 - 0;				
					$(".owl-theme .owl-controls").css('right', RightPosPag);
				}
				else if (windowSize <= 1470) {
					var divWidth5 = $('main .home_actus article').width(); 
					var WinWidth = jQuery(window).width()/2;
					var RightPosPag = WinWidth - divWidth5/2 - 35;				
					$(".owl-theme .owl-controls").css('right', RightPosPag);
				}
				else if (windowSize <= 1700) {
					var divWidth5 = $('main .home_actus article').width(); 
					var WinWidth = jQuery(window).width()/2;
					var RightPosPag = WinWidth - divWidth5 - 52;				
					$(".owl-theme .owl-controls").css('right', RightPosPag);
				}
				else if (windowSize <= 2300) {
					var divWidth5 = $('main .home_actus article').width(); 
					var WinWidth = jQuery(window).width()/2;
					var RightPosPag = WinWidth - divWidth5 - 52;				
					$(".owl-theme .owl-controls").css('right', RightPosPag);
				}
				else if (windowSize >= 2300) {
					var divWidth5 = $('main .home_actus article').width(); 
					var WinWidth = jQuery(window).width()/2;
					var RightPosPag = WinWidth - divWidth5/2 - divWidth5 - 67;				
					$(".owl-theme .owl-controls").css('right', RightPosPag);
				}
			
		};
		$(document).ready(sizing);
		$(window).resize(sizing);
		</script>
