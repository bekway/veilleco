		<script>
	$('.menu').hover(function(){
		$('.menu_current').removeClass('active');
	}, function(){
		$('.menu_current').addClass('active');
	});
	
	$('#tabs-nav button').bind('click', function(){
		var active = $(this).parent();
		
		active.siblings().removeClass('active');
		active.addClass('active');
		
		$('.wpcf7').removeClass('active');
		$('.wpcf7').eq(active.index()).addClass('active');
	});
	
	$(document).ready(function(){
		$('#tabs-nav button').first().addClass('active');
		$('.wpcf7').first().addClass('active');
	});
    
    $('.svg-map a').bind('mouseenter', function(){
        $('.map-label').html($(this).attr('title')).addClass('active');
        var polygon = $(this).find('polygon');
        var color = polygon.data('color');
        polygon.attr('fill', color);
    });
    
    $('.svg-map a').bind('mouseleave', function(){
        $('.map-label').removeClass('active');
        var polygon = $(this).find('polygon');
        polygon.attr('fill', '#3f3f3f');
    });
		</script>
		<!-- Banniere Coockies -->
