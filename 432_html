	<script type="text/javascript">
//fix ie8 et +
var imgElements = document.body.getElementsByTagName("img");
for(var i=0;i < imgElements.length;i++){
	var current =imgElements[i];
	var styleToAdd =null;

	var width = current.getAttribute("width");
	if(width != undefined)
		styleToAdd +=";width:"+width+"px";
	var height = current.getAttribute("height");
	if(height != undefined)
		styleToAdd +=";height:"+height+"px";

	var style = current.getAttribute("style");
	if(styleToAdd != undefined){
		if(style != undefined)
			current.setAttribute('style', style+styleToAdd);
		else{
			current.setAttribute('style',styleToAdd);
		}
	}
}
	</script>
