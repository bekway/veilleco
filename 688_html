	jQuery(function($){
   // Your jQuery code here, using the $
   
   $(".mapcontainer").mapael({
			        map: {
                    name: "france_departments",
                    defaultArea: {
                        attrs: {
                            stroke: "#fff",
                            "stroke-width": 1
                        },
                        attrsHover: {
                            "stroke-width": 2
                        }
                    }
                },
                legend: {
                    area: {
                        title: "Population of France by department",
                        slices: [
