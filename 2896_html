		                        function showBanner()
		                        {
		                            if (ametysCookieConsentListener.length > 0)
		                            {
		                                
        var html = '<div class="cookiebanner-wrapper" id="cookie-banner">'
        + '<div class="cookiebanner">'
        + '<div class="cookiebanner-info">'
