							<script type="text/javascript"><!--

				function sendByMail_form_switch_7150485630903166()
				{
					var form = document.getElementById('sendByMail-7150485630903166');
					var submit = document.getElementById('sendByMail-submit-7150485630903166');
					form.style.display = (form.style.display == 'none') ? '' : 'none';
					submit.style.display = (submit.style.display == 'none') ? '' : 'none';
				}
				sendByMail_form_switch_7150485630903166();
				
				function sendByMail_form_popup_7150485630903166()
				{
					refresh_sendByMail_7150485630903166();
					$j("#sendByMail-form-7150485630903166 script").remove();
					$j("#sendByMail-form-7150485630903166").dialog({
				      resizable: false,
				      height:'auto',
				      width: 'auto',
				      modal: true,
				      title: 'Envoyer par mail',
				      buttons: {
				        Envoyer: function() {
				          sendByMail_form_submit_7150485630903166();
				        },
				        Annuler: function() {
				          $j( this ).dialog( "close" );
				        }
				      }
				    });
				    
					$j('#sendByMail-7150485630903166-email-to').val('');
					$j('#sendByMail-7150485630903166-text').val('');
					$j('#sendByMail-7150485630903166-captcha').val('');
				}
				
				function sendByMail_form_submit_7150485630903166()
				{
					var form = document.getElementById('sendByMail-7150485630903166');
					
					var pageId =  $j('#sendByMail-7150485630903166-id');
					var nameInput = $j('#sendByMail-7150485630903166-name');
					var emailInputBy = $j('#sendByMail-7150485630903166-email-by');
					var emailInputTo = $j('#sendByMail-7150485630903166-email-to');
					var contentInput = $j('#sendByMail-7150485630903166-text');
					var captchaKey = $j('#sendByMail-7150485630903166-captcha-key');
					var captchaInput = $j('#sendByMail-7150485630903166-captcha');
					
					var errors = false;
					
					if (/^\s*$/.test(nameInput.val()))
					{
						errors = true;
						nameInput.parentsUntil('.field').parent().addClass('error');
					}
					else
					{
						nameInput.parentsUntil('.field').parent().removeClass('error');
					}
					
					if (!/^([a-z0-9._-]+@[a-z0-9.-]{2,}[.][a-z]{2,})?$/.test(emailInputBy.val()) || /^\s*$/.test(emailInputBy.val()))
					{
						errors = true;
						emailInputBy.parentsUntil('.field').parent().addClass('error');
					}
					else
					{
						emailInputBy.parentsUntil('.field').parent().removeClass('error');
					}
					
					if (!/^([a-z0-9._-]+@[a-z0-9.-]{2,}[.][a-z]{2,})?(\s*,\s*([a-z0-9._-]+@[a-z0-9.-]{2,}[.][a-z]{2,})?)*$/.test(emailInputTo.val()) || /^\s*$/.test(emailInputTo.val()))
					{
						errors = true;
						emailInputTo.parentsUntil('.field').parent().addClass('error');
					}
					else
					{
						emailInputTo.parentsUntil('.field').parent().removeClass('error');
					}
					
                    
					if (!/^[a-z]{6}$/.test(captchaInput.val()))
					{
						errors = true;
						captchaInput.parentsUntil('.field').parent().addClass('error');
					}
					else
					{
						captchaInput.parentsUntil('.field').parent().removeClass('error');
					}
                    

					if (!errors)
					{
						// send it in ajax
						var data = {
							"page-id": pageId.val(),
							"name": nameInput.val(),
							"email": emailInputBy.val(),
							"emailTo": emailInputTo.val(),
							"text": contentInput.val(),
							"captcha-key": captchaKey.val(),
							"captcha-value": captchaInput.val(),
							"ajax": "true"
						};

						function result(data)
						{
							var s = "";
							$j('errors error', data).each(function () { s += $j(this).text() + "\n"; });
							
							if (s == "")
							{
								var errors = $j('error > error', data);
                                var mailErrors = $j('mailError > mailError', data);
                                
								if (errors.length == 0 && mailErrors.length == 0)
								{
									$j("#sendByMail-form-7150485630903166").dialog("close");
									alert("Le mail a bien été envoyé");
								}
								else if (errors.length > 0)
								{
                                    var m = '';
                                    $j('error > error', data).each(function () { 
                                        m += "\n*" + $j(this).text();
                                    });
									alert("Votre formulaire contient les erreurs suivantes :" + m);
                                    refresh_sendByMail_7150485630903166();
								}
                                else if (mailErrors.length > 0)
                                {
                                    var m = '';
                                    $j('mailError > mailError', data).each(function () { 
                                        m += "\n" + $j(this).text();
                                    });
                                    
                                    alert("L'envoi du mail a échoué pour les adresses email suivantes :" + m);
                                    refresh_sendByMail_7150485630903166();
                                }
							}
							else
							{
                                // Could really append ??
								alert(s);
								refresh_sendByMail_7150485630903166();
							}
						}
                        
                        
                        function handleError(jqXHR, textStatus, errorThrown)
                        {
                            alert("Une erreur inattendue est survenue: l'envoi de mail a échoué.");
                        }
						
                        $j.ajax({
                            url: "/plugins/socialnetworking/send-mail",
                            data: data,
                            type: 'post',
                            success: result,
                            error: handleError
                        });
					}

					return false;
				}
			
// -->
							</script>
						</div>
					</div>
