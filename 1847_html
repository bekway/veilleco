				'Tharaux',
				'Allègre-les-Fumades',
				'Rivières',
				'Méjannes-le-Clap',
				'Navacelles'
			];
			
			var map_config = {
				'default':{
					'borderColor': '#BEC4CB',
					'mapShadow':'#fff',
					'shadowOpacity':'35',
					'hoverShadow':'#666666',
					'lakesColor':'#66CCFF',
					'namesColor':'#919191'
				},
			};
			
			for (var i = 1, max = map_cities.length; i <= max; i++) {
				map_config['map_' + i] = {
					'hover': map_cities[i-1],
					'upColor':'#ffffff',
					'overColor':'#678dc6', /* MODIF-couleurs regions survol */
					'downColor':'#678dc6', /* MODIF-couleurs regions survol */
					'enable':true,
				};
				addEvent('map_' + i);
			}
			
			var pin_config = {
				'default':{
					'pinShadow':'#000',
					'pinShadowOpacity':'50',
				},
				'points':[]
			}
		
