							<script language="javascript">

var nb_background1 = 4;
var background_en_cours1 = 1;

var nb_background2 = 10;
var background_en_cours2 = 1;

function change_bloc1(id){
	if(background_en_cours1==nb_background1){
		background_en_cours1 = 1;
		$("#"+id+nb_background1).fadeOut(2000);
		$("#"+id+background_en_cours1).fadeIn(1000);
		
	}else{
		$("#"+id+background_en_cours1).fadeOut(2000);
		$("#"+id+(background_en_cours1+1)).fadeIn(1000);
		background_en_cours1++;
	}
}

function change_bloc2(id){
	if(background_en_cours2==nb_background2){
		background_en_cours2 = 1;
		$("#"+id+nb_background2).fadeOut(2000);
		$("#"+id+background_en_cours2).fadeIn(1000);
		
	}else{
		$("#"+id+background_en_cours2).fadeOut(2000);
		$("#"+id+(background_en_cours2+1)).fadeIn(1000);
		background_en_cours2++;
	}
}

$(document).ready(function(){
	$(function() {
		setInterval( "change_bloc1('bloc1image')", 7000 );
		setInterval( "change_bloc2('bloc2image')", 5000 );
	});
});
							
							</script>
