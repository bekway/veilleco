$().ready(function () {

$("#menu").mouseenter(function(){
    clearTimeout($(this).data('timeoutId'));
}).mouseleave(function(){
    var someElement = $(this);
    var timeoutId = setTimeout("closeallssmenu()", 500);
    //set the timeoutId, allowing us to clear this trigger if the mouse comes back over
    someElement.data('timeoutId', timeoutId); 
});

	$("#m2 .N2toggle>span").each(function(index){
		if($(this).siblings().length>0){
			$(this).html("<span class='plus'>+" + $(this).html());
		}
	})

	$(".N3").hide(0);
	
	$("#m2 .N2toggle>span").on("click",".plus",function(){
		$(".moins").html("+")
		$(".moins").addClass("plus");
		$(".moins").removeClass("moins");
		
		
		
		$(this).html("-")
		$(this).removeClass("plus");
		$(this).addClass("moins");
		var monN3 = $(this)
		$(".N3").hide(0,function(){
			monN3.parent().parent().find(".N3").show(0);
		});
	});
	$("#m2 .N2toggle>span").on("click",".moins",function(){
		$(this).html("+")
		$(this).removeClass("moins");
		$(this).addClass("plus");
		var monN3 = $(this)
		monN3.parent().parent().find(".N3").hide(0);
	});

    $('div#b1a').mouseover(function () {
		$('div#m1').slideDown('medium');
		$('div#m2').slideUp('medium');
