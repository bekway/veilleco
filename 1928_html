		<div id="drapeaux"><a href="/Accessibilite-Handicap"><div class="drapeau drapeau-handicap"></div></a> <a class="scrollto" href="#contact"><div class="drapeau drapeau-contact"></div></a> <br><br> <a href="/carte"><div class="drapeau drapeau-carte"></div></a></div>
		<script>

	$(document).ready(function(){
		//Fermeture de la pop-pup et du fond
		$('body').on('click', '#fade', function() { //Au clic sur le bouton ou sur le calque
			$('#fade, .popup_contact').fadeOut(function() {
				$('#fade, a.close').remove(); //..ils disparaissent ensemble
			});
			return false;

		});
	});

	function envoiFormulaire()
	{

		if (window.XMLHttpRequest)
		{
			xmlhttp=new XMLHttpRequest();
		}
		else
		{
			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
		}

		xmlhttp.onreadystatechange = function()
		{
			
			if (xmlhttp.readyState==4 && xmlhttp.status==200)
			{
				document.getElementById("popup_contact").style.display = 'table';
				$('#popup_contact').fadeIn().css({'z-index' : '9999', 'height': "100px", 'width': "300px", 'top' : "20%", 'margin-left' : -popMargLeft}).prepend('');

				//Effet fade-in du fond opaque
				$('body').append('<div id="fade">'); //Ajout du fond opaque noir
				//Apparition du fond - .css({'filter' : 'alpha(opacity=80)'}) pour corriger les bogues de IE
				$('#fade').css({'filter' : 'alpha(opacity=80)'}).fadeIn();

				var popMargLeft = ($('#popup_contact').width() + 80) / 2;
				//Apply Margin to Popup
				$('#popup_contact').css({
					'margin-left' : -popMargLeft
				});

				var Reponse = xmlhttp.responseText.trim();
				
				if (Reponse == "ok")
				{
					document.getElementById("txtResultEnvoi").innerHTML="Votre message a été transmis...";						
				}
				else
				{
					document.getElementById("txtResultEnvoi").innerHTML="Un problème est survenu pendant l'envoi du mail, merci d'essayer à nouveau dans quelques intants ou contactez-nous par téléphone.";
				}
			}
		}

		var fd = new FormData();
		fd.append('contactnom', document.getElementById("contactnom").value);
		fd.append('contactprenom', document.getElementById("contactprenom").value);
		fd.append('contactmail', document.getElementById("contactmail").value);
		fd.append('contacttel', document.getElementById("contacttel").value);
		fd.append('contactmessage', document.getElementById("contactmessage").value);
		fd.append('g-recaptcha-response', document.getElementById("g-recaptcha-response").value);

		xmlhttp.open("POST", '/inc/sendmail.php');
		xmlhttp.send(fd);
		


	}
		</script>
		<script src="https://www.google.com/recaptcha/api.js"></script>
