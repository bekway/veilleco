				$(document).ready(function() {
					$("#alethouarsais").click(function (){
						if ($(this).parent().hasClass("active"))
						{
							$(".menu-niv-1").removeClass("active");
						}
						else
						{
							$(".menu-niv-1").removeClass("active");
							$(this).parent().addClass("active");	
						}
					});

					$("#aauquotidien").click(function (){
