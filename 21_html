            /* Close Menu smartphone niv1 */
            $(".menu_niv1 > li > a").click(function() {
                var href = $(this).attr("href");
                var id = href.replace("#", "");
                $(".menu_niv1 ul.in").each(function() {
                    if ($(this).attr("id") !== id) {
                        $(this).collapse('hide');
                    }
                });
                $(".menu_niv1 > li > a").each(function() {
                    if ($(this).attr("href") !== href)
                        $(this).addClass("collapsed");
                });
            });

            /* Close Menu smartphone niv2 */
            $(".menu_niv2 > li > a").click(function() {
                var href = $(this).attr("href");
                var id = href.replace("#", "");
                $("ul.menu_niv3.in").each(function() {
                    if ($(this).attr("id") !== id) {
                        $(this).collapse('hide');
                    }
                });
                $(".menu_niv2 > li > a").each(function() {
                    if ($(this).attr("href") !== href)
                        $(this).addClass("collapsed");
                });
            });

            /* En ce moment : truncate chapeau */
            function truncateEnCeMoment() {
                $("p.totruncate").each(function() {
                    var bloc = $(this).parent();
                    var a = bloc.parent();
                    if (a.hasClass("small")) {
                        var width = bloc.width();
                        if (width > 0) {
                            var title = $(this).text();
                            if (width <= 213) {
                                if (title.length > 35) {
                                    var icone = $(this).find("i");
                                    title = title.replace(icone, "");
                                    title = title.substring(0, 35);
                                    $(this).text(title + "...");
                                    $(this).append('<i class="' + icone.attr("class") + '">');
                                }
                            } else {
                                if (title.length > 55) {
                                    var icone = $(this).find("i");
                                    title = title.replace(icone, "");
                                    title = title.substring(0, 55);
                                    $(this).text(title + "...");
                                    $(this).append('<i class="' + icone.attr("class") + '">');
                                }
                            }
                        }
                    }
                });
                $("p.chapeau").each(function() {
                    var bloc = $(this).parent();
                    var a = bloc.parent();
                    if (!a.hasClass("small")) {
                        var width = bloc.width();
                        if (width > 0) {
                            var chapeau = $(this).text();
                            if (width <= 455) {
                                if (chapeau.length > 140) {
                                    chapeau = chapeau.substring(0, 140);
                                    $(this).text(chapeau + "...");
                                }
                            }
                        }
                    }
                });
            }
            truncateEnCeMoment();
        });
        function loadDynamicalHrefForI18n() {
            $('a.locales-switcher-link').each(function(){

                var route = 'path_render_page';
                var params = {"_locale":"fr","url":"le-conseil-municipal"};
                params['_locale'] = $(this).attr('data-locale');
                $(this).attr('href', Routing.generate(route, params));
            });
        }

