            if (navigator.userAgent.match(/iPad/i) !== null) {
                var disableScroll = false;
                $('.nav.navbar-nav > li').on('shown.bs.dropdown', function() {
                    disableScroll = true;
                });
                $('.nav.navbar-nav > li').on('hidden.bs.dropdown', function() {
                    disableScroll = false;
                });
                $(document).bind('touchmove', function(e) {
                    if (disableScroll) {
                        e.preventDefault();
