            $(".stylish-select2").select2({dropdownCssClass: "dropdown-stylish-select2-blue", minimumResultsForSearch: 10});
            $('.stylish-datepicker').datepicker({format: 'dd/mm/yyyy', language: 'fr'});
            $('.datepicker').datepicker({format: 'dd/mm/yyyy', language: 'fr'});
            $('.form-datepicker').datepicker({format: 'dd/mm/yyyy', language: 'fr'});

            $("area").hover(
                function() {
                    $(".list-lieux").find("a[rel='" + $(this).attr("rel") + "']").css("background-color", "white");
                },
                function() {
                    $(".list-lieux").find("a[rel='" + $(this).attr("rel") + "']").css("background-color", "#FFCE00");
                    $(".list-lieux").find("a[rel='" + $(this).attr("rel") + "']").hover(
