		<script type="text/javascript">
        $(document).ready(function(){
            $('div.block-render').each(function(){
                if (navigator.appVersion.indexOf("MSIE 9.") !== -1) {
                    $(this).html('<div class="spinner-block spinner-block-ie">');
                } else {
                    $(this).html('<div class="spinner-block"><div class="rect1"><div class="rect2"><div class="rect3"><div class="rect4"><div class="rect5">');
                }
                var $block = $(this);
                var params = {
                    _locale: 'fr',
                    id: $block.attr('data-id'),
                    parent: $block.attr('data-parent'),
                    universe: $block.attr('data-universe'),
                    pageId: $block.attr('data-pageid'),
                    lazy: true
                };
                $.post(Routing.generate('path_render_block_page_id', params), function(res){
                    $block.fadeOut(0);
                    $block.html(res)
                    $block.fadeIn(700);
                });
            });
        });
    
		</script>
