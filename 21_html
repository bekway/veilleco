						<script type="text/javascript">
        window.onload = function(){
            $('.dropdown-toggle').mouseup(function(){
                setTimeout(function(){
                    var tallerHeight = 0;
                    $('div.navbar-nav > li.open div.row-menu > div').each(function(){
                        tallerHeight = tallerHeight > $(this).find('.content-wrapper').height() ? tallerHeight : $(this).find('.content-wrapper').height();
                    });
                    $('div.navbar-nav > li.open div.row-menu > div > div.content-wrapper').height(tallerHeight);
                }, 233);
            })
        };
    
						</script>
