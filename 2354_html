        var JQready = function(){
            for(var fct in onJQReady)
                    onJQReady[fct]();
        };
        if ( !window.jQuery ) {
            yepnope({
                load: '/resource/js/jquery-1.8.2.js',
                complete: function () {
                    JQready();
                }
            });
        }else
                JQready();
    }
}
