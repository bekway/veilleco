            $("#button").click(function ()
                               {
                $.ajax({
                    url: 'https://www.cherbourg.fr/institution/vie-politique/les-conseils-municipaux-et-communaux/les-conseils-municipaux-de-cherbourg-en-cotentin-1352.html?email=&type=98654&tx_cimnewsletter_newsletter%5Baction%5D=switch&tx_cimnewsletter_newsletter%5Bcontroller%5D=Newsletter',
                    method: 'POST',
                    data: $("#subscribeNewsletter").serialize()
                }).success(function (response)
                           {
                    var result = $.parseJSON(response);
                    if	(typeof result.error !== "undefined"){
                        $('#error').html("")
                        $('<p>'+ result.error+'').appendTo('#error');
                    }
                    else{
                        $('#bloc_newsletter').html("<p class=\"msg_confirm\">Un email de confirmation vient de vous être envoyé automatiquement. Pour finaliser votre inscription, merci de cliquer sur le lien présent dans cet email.<br/><b>Si vous n'avez pas reçu l'email, vérifiez votre dossier courrier indésirable ou spam.")
                    }
                }).error(function ()
                         {
                    console.log("error");
                })
                ;
            });
        
																</script>
