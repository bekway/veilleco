						<script>
	
	(function($){	
	
	$(window).ready(function(){		
		detectMenuInterne(2184);
	}); 
	
	
	function detectMenuInterne(pageId){
		
		var leLien = $('#menu-interne .id-'+pageId);
		leLien.addClass('active');
		
		$('#menu-interne .li-top-level').hide();
		
		if(leLien.hasClass('li-top-level')){
			leLien.show();
		}
		
		if(leLien.hasClass('li-second-level')){
			console.log(leLien.parent().parent().parent().show());
			leLien.parent().parent().show();
		}
		
		if(leLien.hasClass('li-third-level')){
			leLien.parent().parent().parent().parent().show();
		}
	}    

	
	

})(jQuery);

		
						</script>
