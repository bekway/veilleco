    if (document.getElementById("ctl00_lblMessage")) {
        if (document.getElementById("ctl00_lblMessage").innerText || document.getElementById("ctl00_lblMessage").innerHTML) {
            if (document.getElementById("ctl00_lblMessage").innerText != "" || document.getElementById("ctl00_lblMessage").innerHTML != "") {
                document.getElementById('isoDragEnvoiAmi').style.display = 'block';
                MM_showHideLayers('isoDragEnvoiAmi', '', 'show', 'isoPopEnvoiAmi', '', 'show');
                document.location.href = document.location.href + "#envoiami";
            }
        }
    }
											
											</script>
										</div>
