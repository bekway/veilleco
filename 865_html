            $(window).load(function() {
                $("#circle").click(function(){
                    document.getElementById('popup').style.display='none';
                    document.getElementById('header').style.top='0px';
                });
              
//                var isHidden = document.getElementById("popup").style.display == "none";
//                if(isHidden){
//                    document.getElementById('header').style.top='0px';
//                } 
            });
        });
    });   
