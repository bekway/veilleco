		<script type="text/javascript">
    var $buoop = {vs:{i:8,f:20,o:17,s:6},reminder:0};
    $buoop.ol = window.onload;
    window.onload=function(){
    try {if ($buoop.ol) $buoop.ol();}catch (e) {}
    var e = document.createElement("script");
    e.setAttribute("type", "text/javascript");
    e.setAttribute("src", "//browser-update.org/update.js");
    document.body.appendChild(e);
    }

    $(document).ready(function() {
    	var $grid = $('.events').isotope({
	    itemSelector: '.post'
	});
        
        $('#searchcategory').on( 'change', function() {
          var filterValue = this.value;
          if (filterValue != "*"){
              filterValue = "." + filterValue;
          }
          $grid.isotope({ filter: filterValue });
        });
                        
        var $grid2 = $('.events').isotope({
            itemSelector: '.post'            
        });

        var chosendate = null;
        var saturdayweek = null;
        var sundayweek = null;

        $('#searchbuttonagenda').on('click', function() {

        chosendate = $( "#searchdate option:selected" ).val();
        chosenkw = $( "#searchkw" ).val();
        //console.log(chosendate);
        saturdayweek = moment().day(6);
        sundayweek = moment().day(7);

        if (chosendate == "*") {
          $grid2.isotope({
              filter: function() {
                var keywords = $(this).find('.motscles').text();
                console.log("foobar");
                if(keywords != ""){
                    
                    if (keywords.indexOf(chosenkw) > -1){
                        return true;
                    } else {
                        return false;
                    }
                } else {
                    return true;
                }
            }
          });
        }

        if (chosendate == "today") {
          $grid2.isotope({
              filter: function() {
                console.log("filter today");
                
                var datedebut = $(this).find('.datedebut').text();
                var datefin = $(this).find('.datefin').text();
                var today = moment(new Date()).format('YYYY-MM-DD');
                // console.log(datedebut);
                // console.log(datefin);
                // console.log(today);
                // si juste date début
                if (datefin == "") {
                    var validdate = moment(datedebut).isSame(new Date(), "day");
                } 
                // si date début/fin
                else {
                    if (datefin == today) {
                        var validdate = today;
                    } else {
                        var validdate = moment(today).isBetween(datedebut, datefin);
                    }
                }
                //console.log(iscurrentDate);
                return validdate;
            }
          });
        }
        
        if (chosendate == "weekend") {
            $grid2.isotope({
                filter: function() {
                    saturdayweek = moment().day(6);
                    sundayweek = moment().day(7);
                    console.log(saturdayweek);

                    var datedebut = $(this).find('.datedebut').text();
                    var datefin = $(this).find('.datefin').text();
                    //console.log(datedebut + " " + datefin);
                    // si juste date début
                    if (datefin != "") {
                        var validdate = moment(new Date(datefin)).isBetween(saturdayweek, sundayweek, null, '[]');
                    } else {
                        var validdate = moment(new Date(datedebut)).isBetween(saturdayweek, sundayweek, null, '[]');
                    }
                    if(validdate == false){
                        if(datefin != ""){
                            validdate = moment(new Date(datefin)).isSame(saturdayweek,'day');
                            if(validdate == false){
                                validdate = moment(new Date(datefin)).isSame(sundayweek,'day');
                            }
                        }
                        else{
                            validdate = moment(new Date(datedebut)).isSame(saturdayweek, 'day');
                            if(validdate == false){
                                validdate = moment(new Date(datedebut)).isSame(sundayweek, 'day');
                            }
                        }
                    }
                    console.log(validdate);
                    return validdate;
                    }
                });
        }
        
        if (chosendate == "7days") {
            $grid2.isotope({
                filter: function() {
                    
                    console.log("filter 7days");
                    var datedebut = $(this).find('.datedebut').text();
                    var datefin = $(this).find('.datefin').text();
                    console.log(datedebut + " " + datefin);
                    // si juste date début
                    if (datefin != "") {
                        var validdate = moment(new Date(datefin)).isBetween(new Date(), moment(new Date()).add(7, 'days'));
                    }
                    else {
                        var validdate = moment(new Date(datedebut)).isBetween(new Date(), moment(new Date()).add(7, 'days'));
                    }
                    return validdate;
                    }
                });

        }

        if (chosendate == "30days") {
            //filter 30 days
            $grid2.isotope({
                filter: function() {
                    
                    console.log("filter 30days");
                    var datedebut = $(this).find('.datedebut').text();
                    var datefin = $(this).find('.datefin').text();
                    // si juste date début
                    if (datefin != "") {
                        var validdate = moment(new Date(datefin)).isBetween(new Date(), moment(new Date()).add(30, 'days'));
                    }
                    else {
                        var validdate = moment(new Date(datedebut)).isBetween(new Date(), moment(new Date()).add(30, 'days'));
                    }
                    return validdate;
                    }
                });

        }

        });
        
    });
   
		</script>
		<script>
    jQuery(function($) {
        $(document).ready(function(){
            $(window).load(function() {
                $("#searchbutton").click(function(){
                    var dateurl = $( "#searchdate option:selected" ).val();
                    var dateurl2 = dateurl.substr(dateurl.length -8);
                    var dateurl3 = dateurl2.substring(0,7);
                    var dateurl4 = dateurl3.replace("/","-");
                    var dateurl5 = ".d" + dateurl4;

                    if ($( "#searchcategory option:selected" ).val() == "*" && $( "#searchdate option:selected" ).val() == "*") {
                        $( ".actus" ).isotope({ filter: $( "#searchcategory option:selected" ).val() });
                    } else {
                        if ($( "#searchdate option:selected" ).val() == "*") {
                            dateurl5 = "";
                        }
                        if ($( "#searchcategory option:selected" ).val() != "*") {
                            var categoryslug = "." + $( "#searchcategory option:selected" ).val();
                        } else {
                            var categoryslug = "";
                        }
                        $( ".actus" ).isotope({ filter: categoryslug + dateurl5 });
                    }
                    console.log(categoryslug + dateurl5);
                });

                
                $( ".events" ).isotope({ filter: "*" });

                
                            });
        });
            
    });
		</script>
		<script>
    //si page ancre
    //alors scroll to ancre
    //ouvrir panel
    jQuery(function($) {
        $(document).ready(function(){
            $(window).load(function() {
        
                var anchor = $(location).attr('hash');
                if (anchor){
                    $(anchor).collapse("show");

                    $(anchor).on('shown.bs.collapse', function() {
                      $( "html" ).scrollTop($( "html" ).scrollTop()-300 );  
                    });

                }

                $(window).bind('hashchange', function () {
                    var anchor = $(location).attr('hash');
                    $(anchor).collapse("show");

                });

                $(anchor).on('shown.bs.collapse', function() {
                  $( "html" ).scrollTop($( "html" ).scrollTop()-300 );  
                });
            });

            $(function(){
              jQuery(".slide-video").YTPlayer();
            });
        });
    });    
		</script>
