													<!-- Gabarit : media/images/incmodcarousel2.tpl -->
													<script type="text/javascript" src="https://www.champsaur-valgaudemar.fr/openinfoliveRessources/javascript/gabarits/jquery.carousel.pack.js"></script>
													<script type="text/javascript" src="https://www.champsaur-valgaudemar.fr/openinfoliveRessources/javascript/gabarits/jquery.easing.compatibility.js"></script>
													<script type="text/javascript">


$(document).ready(function() {
	$("#A1 .carousel_boutons").each(function(i){   
			$(this).click(function(){
				$("#A1 .carousel_boutons").removeClass('itemselected');
				var id=this.id;
				$(this).addClass('itemselected');
				pausecomp('100');
				
				
			});
		}
	);

	

	
	
	
	$("#A1 .container_carousel_contenu").jCarouselLite({
	 	btnNext: "#A1 .next",
        btnPrev: "#A1 .prev",
		visible: 1,
		auto :600000,
		speed :2000,
		vertical :false,
		circular :true,
		easing: 'easeOut',
		beforeStart: function(a) {
				
				
			  	$('#A1 .carouseltitre').hide();
				$('#A1 .carouselresume').hide();
				$('#A1 .carouselsuite').hide();
				

	    },
		afterEnd: function(a) {

		
			myid=$(a).attr('id');
			myid=myid.replace('meta', '');
			
			
			$('#A1 .carouseltitre').html($('#A1 #titre'+myid).html()).show();
			$('#A1 .carouselsuite').html($('#A1 #suite'+myid).html()).show();
			$('#A1 .carouselsuite').addClass("suite"+myid);
			
			$('#A1 .carouselresume').html($('#A1 #resume'+myid).html()).show();
			
			$('#A1 .itemselected').removeClass('itemselected');
			$('#A1 #carousel_'+myid).addClass('itemselected');
						
	    },

		scroll :1
				
		,btnGo:
		 [						
	   	"#A1 .carousel_1",		
								
	   	"#A1 .carousel_2",		
								
	   	"#A1 .carousel_3",		
								
	   	"#A1 .carousel_4",		
								
	   	"#A1 .carousel_5",		
								
	   	"#A1 .carousel_6",		
								
	   	"#A1 .carousel_7",		
								
	   	"#A1 .carousel_8",		
								
	   	"#A1 .carousel_9",		
								
	   	"#A1 .carousel_10"		
				]		
		
	

    });
	
	

	

	
		
	$("#A1 .container_carousel_contenu > *").css('visibility', 'visible');
	$("#A1 .container_carousel_titreresume").css('visibility', 'visible');
	$("#A1 .container_carousel_lirelasuite").css('visibility', 'visible');
	$("#A1 .container_carousel_contenu").css('z-index', '');/*
	$("#A1 .container_carousel_contenu ul").css('z-index', '');*/
	
	

});
													</script>
