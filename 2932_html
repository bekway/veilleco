$(".menuTop").mouseenter(function() { 
	$(".active").removeClass("active");
	$(this).addClass("active");
	var contenu = $(this).find('ul').html();
	var classSecteur =  $(this).attr('id');	
	$('#ssmenugen').html("<ul class='subnav "+classSecteur+"'>"+ contenu +"<li class='clear'>");
	$('#ssmenugen').slideDown('fast');
});

 
$(".nav").mouseleave(function (){
	$(".active").removeClass("active");
  	$('#ssmenugen').slideUp('fast');
 	$('#ssmenugen').html('');

});		
