				var addy79277fd6bc80776b06484a2f7315b07e = 'c&#117;lt&#117;r&#101;' + '&#64;';
				addy79277fd6bc80776b06484a2f7315b07e = addy79277fd6bc80776b06484a2f7315b07e + 'l&#97;l&#111;nd&#101;l&#101;sm&#97;&#117;r&#101;s' + '&#46;' + 'fr';
