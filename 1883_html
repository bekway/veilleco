										<script>
 		 		(function ( $ ) {
				    $.fn.turbotabs20 = function(options){
				        // setting the defaults
				        var settings20 = $.extend({
				            mode: 'horizontal',
				            width: '100%',
				            animation: 'Scale',
				            cb_after_animation: function(){},
				        },options);
				        if( settings20.deinitialize === 'true' ){
				            return
				        }
				        var tabs20 = this.find('.Rich_Web_Tabs_tt_tabs20');
				        var container20 = this.find('.Rich_Web_Tabs_tt_container20');
				        var tab20=this.find('.Rich_Web_Tabs_tt_container20 .Rich_Web_Tabs_tt_tab20'); 
				       
				        var tabLength20 = tab20.length;
				        // console.log(tab20);
				        var sel20 = this;
				        var random20 =  'tab20-' + Math.floor( Math.random() * 100 ); // for assigning random class (tobe used for hover effect)
				        var animation = settings20.animation;
				        var animationIn = '';
				        var animationOut = '';
				        var once20 = 0;
				        var primWidth20 = []; // create an array that will store the primary widths, before resizing (used in responsive function)
				        var tabsResponsive20 = false;
				        var timer20 = 340;
				        var tabHeights20 = '';
				        var maxHeight20 = '';

				        setTimeout(function(){ // delay setting the heigh for small interval, giving it a time to collect right value
				        calcHeight20();
				        },150);
				        if( settings20.mode == 'vertical' ){ // check if mode is set to vertical
				            sel20.addClass('vertical');
				        }
				        // applying the color, background and other options to the actual tab20				        this.css({width: settings20.width,});
				        /*==============================================
				                            ANIMATIONS
				        ================================================*/
				        function return_animation20(animation){
				            if( 'Scale' === animation ){
				                animationIn = 'zoomIn';
				                animationOut = 'zoomOut';
				            }
				            else if( 'FadeUp' === animation ){
				                animationIn = 'fadeInUp';
				                animationOut = 'fadeOutDown';
				            }
				            else if( 'FadeDown' === animation ){
				                animationIn = 'fadeInDown';
				                animationOut = 'fadeOutUp';
				            }
				            else if( 'FadeLeft' === animation ){
				                animationIn = 'fadeInLeft';
				                animationOut = 'fadeOutLeft';
				            }
				            else if( 'FadeRight' === animation ){
				                animationIn = 'fadeInRight';
				                animationOut = 'fadeOutRight';
				            }
				             else if( 'SlideUp' === animation ){
				                animationIn = 'slideInUp';
				                animationOut = 'slideOutUp';
				                timer20 = 80;
				            }
				            else if( 'SlideDown' === animation ){
				                animationIn = 'slideInDown';
				                animationOut = 'slideOutDown';
				                timer20 = 80;
				            }
				            else if( 'SlideLeft' === animation ){
				                animationIn = 'slideInLeft';
				                animationOut = 'slideOutLeft';
				                timer20 = 80;
				            }
				            else if( 'SlideRight' === animation ){
				                animationIn = 'slideInRight';
				                animationOut = 'slideOutRight';
				                timer20 = 80;
				            }
				            else if( 'ScrollDown' === animation ){
				                animationIn = 'fadeInUp';
				                animationOut = 'fadeOutUp';
				            }
				            else if( 'ScrollUp' === animation ){
				                animationIn = 'fadeInDown';
				                animationOut = 'fadeOutDown';
				            }
				            else if( 'ScrollRight' === animation ){
				                animationIn = 'fadeInLeft';
				                animationOut = 'fadeOutRight';
				            }
				            else if( 'ScrollLeft' === animation ){
				                animationIn = 'fadeInRight';
				                animationOut = 'fadeOutLeft';
				            }
				            else if( 'Bounce' === animation ){
				                animationIn = 'bounceIn';
				                animationOut = 'bounceOut';
				            }
				            else if( 'BounceLeft' === animation ){
				                animationIn = 'bounceInLeft';
				                animationOut = 'bounceOutLeft';
				            }
				            else if( 'BounceRight' === animation ){
				                animationIn = 'bounceInRight';
				                animationOut = 'bounceOutRight';
				            }
				            else if( 'BounceDown' === animation ){
				                animationIn = 'bounceInDown';
				                animationOut = 'bounceOutDown';
				            }
				            else if( 'BounceUp' === animation ){
				                animationIn = 'bounceInUp';
				                animationOut = 'bounceOutUp';
				            } 
				            else if( 'HorizontalFlip' === animation ){
				                animationIn = 'flipInX';
				                animationOut = 'flipOutX';
				            }
				            else if( 'VerticalFlip' === animation ){
				                animationIn = 'flipInY';
				                animationOut = 'flipOutY';
				            }
				            else if( 'RotateDownLeft' === animation ){
				                animationIn = 'rotateInDownLeft';
				                animationOut = 'rotateOutDownLeft';
				            }
				            else if( 'RotateDownRight' === animation ){
				                animationIn = 'rotateInDownRight';
				                animationOut = 'rotateOutDownRight';
				            } 
				            else if( 'RotateUpLeft' === animation ){
				                animationIn = 'rotateInUpLeft';
				                animationOut = 'rotateOutUpLeft';
				            }
				            else if( 'RotateUpRight' === animation ){
				                animationIn = 'rotateInUpRight';
				                animationOut = 'rotateOutUpRight';
				            } 
				            else if( 'TopZoom' === animation ){
				                animationIn = 'zoomInUp';
				                animationOut = 'zoomOutUp';
				            }
				            else if( 'BottomZoom' === animation ){
				                animationIn = 'zoomInDown';
				                animationOut = 'zoomOutDown';
				            }
				            else if( 'LeftZoom' === animation ){
				                animationIn = 'zoomInLeft';
				                animationOut = 'zoomOutLeft';
				            }
				            else if( 'RightZoom' === animation ){
				                animationIn = 'zoomInRight';
				                animationOut = 'zoomOutRight';
				            }
				        }

				        /*==============================================
				                       Initialize Tabs
				        ===============================================*/
				        
				        return_animation20(animation);
				        var $tag20;
				        function resp20(p){
				        	$tag20 = $(p).parent();
				        }
				        resp20;
				        jQuery(window).resize(function(){
				            resp20;
				        })
				        jQuery(".Rich_Web_Tabs_tt_tabs20>li>span").on('click', function(){
				            resp20(this);
				            if( true === tabsResponsive20 ){
				                if( !$tag20.hasClass("active") ) {
				                	$tag20.addClass('active').find('.Rich_Web_Tabs_tt_tab20').slideDown(400, settings20.cb_after_animation).parent().siblings().removeClass('active').find('.Rich_Web_Tabs_tt_tab20').slideUp();
				                	jQuery('.Rich_Web_Tabs_tt_tab20').each(function(){
				                		jQuery(this).removeClass("active")
				                	})
				                	$tag20.addClass('active').find('.Rich_Web_Tabs_tt_tab20').addClass("active")
				                } else {
				                    $tag20.removeClass('active').find('.Rich_Web_Tabs_tt_tab20').slideUp();
				                    $tag20.removeClass('active').find('.Rich_Web_Tabs_tt_tab20').removeClass("active")
				                }// else
				            } else {

				                if( !$tag20.hasClass("active") ) {
				                    if( 'random20' === animation ){
				                        var animations_array20 = Array("Scale","Bounce","FadeUp","FadeDown","FadeLeft","FadeRight","SlideUp","SlideDown","SlideLeft","SlideRight","ScrollUp","ScrollDown","ScrollLeft","ScrollRight","BounceUp","BounceDown","BounceLeft","BounceRight","HorizontalFlip","VerticalFlip","RotateDownLeft","RotateDownRight","RotateUpLeft","RotateUpRight","TopZoom","BottomZoom","LeftZoom","RightZoom");
				                        var rand_animation20 = animations_array20[Math.floor(Math.random()*animations_array20.length)];
				                        return_animation20(rand_animation20);
				                    }
				                    var index20 = $tag20.index();
				                    var current20 = $tag20;
				                    $tag20.parent().find("li.active").removeClass("active");
				                    $tag20.addClass("active");
				                    $tag20.closest(sel20).find("div.Rich_Web_Tabs_tt_tab20.active").attr('class', 'Rich_Web_Tabs_tt_tab Rich_Web_Tabs_tt_tab20 Rich_Web_Tabs_animated ' + animationOut); 
				                     setTimeout(function(){
				                        current20.closest(sel20).find("div.Rich_Web_Tabs_tt_tab20").eq(index20).attr('class', 'Rich_Web_Tabs_tt_tab Rich_Web_Tabs_tt_tab20 active Rich_Web_Tabs_animated '+ animationIn);
				                        settings20.cb_after_animation();
				                    },timer20);
				                }// if
				            }// else
				        });
				        /*==============================================
				                        RESPONSIVENESS
				        ===============================================*/
				        // create variables that will store values that will be added later
				            var tabsWidth20 = 0;
				            var currWidth20 = 0;
				            var conWidth20 = 0;
				            var mobile20 = true;
				            var tabW20 = 0;
				            var called20 = 0;
				            var resized20 = 0;
				            primWidth20['resized20'] = 0;           
				            calcWidth20(); 
				        if( settings20.mode != 'accordion' ) {   

				            if( settings20.mode != 'vertical' ) {
				               
				                if( tabW20 < tabsWidth20 + 20 ){ // if starting from small screen transform it to accordion now
				                    resize20(); 

				                    mobile20 = true;
				                }
				                
				                $(window).resize(function(){
				                	
				                    // resp20(this);
				                    var windowWidth20 = parseInt( $(window).outerWidth() ); // check for device width;
				                    calcWidth20(); //callback

				                    if( !mobile20) { // if viewed on larger screen and then resized20 to smaller one 
				                       
				                        if( true === tabsResponsive20 && currWidth20 > primWidth20['container20'] ||  tabs20.width() > primWidth20['container20'] ){
				                           resize20(); 
				                           $("div.Rich_Web_Tabs_tt_tab20").removeClass('Rich_Web_Tabs_animated');
				                        } else if( false === tabsResponsive20 && tabW20 < ( tabsWidth20 + 10 ) ){
				                           
				                           reset20(); 
				                        } else if( primWidth20['resized20'] != 0 ){
				                            
				                            if( currWidth20 > primWidth20['resized20'] + 40 ) {

				                                resize20(); 
				                            }

				                        }

				                    } else { 
				                        
				                       // if starting from small screen
				                       if( windowWidth20 <= 553 ) {
				                       //alert(111);
				                           //if(  true === tabsResponsive20 && currWidth20 > primWidth20['container20']  * 1.5 ) { 
				                             
				                                reset20(); 
				                                $("div.Rich_Web_Tabs_tt_tab20").removeClass('Rich_Web_Tabs_animated');
				                                setTimeout(function(){
				                                calcHeight20();    
				                                if( 1 === once20 ){
				                                    primWidth20['disposal'] = tabW20 + 130;
				                                } //if
				                                },120);
				                                
				                            //} //if 
				                            
				                        } else if( windowWidth20 >= 553  ){
				                              
				                            var zbr = tabs20.find('li').length * 170; // calculate approximate width for each tab20 nav
				                            if( currWidth20 > zbr ) {
				                                //alert(2)
				                                resize20();
				                            } else {
				                                //alert(22)
				                                 resize20();
				                            }
				                        }    
				                    }//else
				                }); //window.resize()
				            } else { // if vertical mode 
				                var windowWidth20 = parseInt( $(window).outerWidth() );
				                
				                if( windowWidth20 < 760 ){ // if starting from small screen transform it to accordion now
				                       reset20();
				                       mobile20 = true;
				                       $("div.Rich_Web_Tabs_tt_tab20").removeClass('Rich_Web_Tabs_animated');
				                }

				                $(window).resize(function(){
				                    // resp20(this);
				                    windowWidth20 = parseInt( $(window).outerWidth() ); //  updatedevice width;
				                    calcWidth20(); 
				                    if( !mobile20 ) { // if viewed on larger screen and then resized20 to smaller one
				                        if( windowWidth20 < 553 ){
				                            reset20(); 
				                        } else {
				                            resize20(); 
				                        }  
				                    } else {

				                        if( windowWidth20 > 553 ){
				                            resize20();
				                            setTimeout(function(){
				                                calcHeight20();    
				                            },120);
				                        } else {
				                            reset20();
				                        }//else

				                    }//else
				                });//window.resize()
				            } // else (is vertical)
				        } else {
				            // alert(111);
				            reset20();
				        }//else( is accordion)
				        
				        /*==============================================
				                        HELPER FUNCTIONS
				        ===============================================*/
				        function calcWidth20(){
				            // resp20(this);
				             // reset20 variables before adding new values
				             tabsWidth20 = 0;
				             currWidth20 = 0;
				             conWidth20 = 0;
				             // get the widths of both navigations and container20				             currWidth20 = parseInt( tabs20.find('li').first().outerWidth(true) ); // get current20 width of resized20 tab20 li
				             conWidth20 = parseInt( container20.outerWidth(true) );
				             if( tabsResponsive20 === false ){
				                 tabs20.find('li').each(function(){ // loop through navs and add width to variable
				                    tabsWidth20 += parseInt( $(this).outerWidth(true) );
				                 }); //if
				            } else {
				                tabsWidth20 = primWidth20['tabs20'];
				            }//else
				            // use the array created in the beginning to store primary widths
				            //make sure that this process is done only once20 (preventing the new values to override the old ones)
				            if( 0 === once20 ) {
				                once20++ ;
				                primWidth20['tabs20'] = tabsWidth20 + 10;
				                primWidth20['container20'] = conWidth20;
				            } else if ( 0 === once20 && mobile20 ){
				                primWidth20['container20'] = sel20.find('.Rich_Web_Tabs_tt_tabs20 li.active .Rich_Web_Tabs_tt_tab20').width();
				            }
				            tabW20 = parseInt( $('.Rich_Web_Tabs_tt_tab20').width() );
				        }// calcWidth20()

				        function calcHeight20(){
				            //seting the the tab20 content height to the tallest tab20 content
				            // src = http://stackoverflow.com/questions/6781031/use-jquery-css-to-find-the-tallest-of-all-elements
				            // Get an array of all element heights
				            // console.log(tab20);
				            tabHeights20 = tab20.map(function() {
				            return $(this).outerHeight(true);
				            }).get();
				            // console.log(tab20);
				            // Math.max takes a variable number of arguments
				            // `apply` is equivalent to passing each height as an argument
				            maxHeight20 = Math.max.apply(null, tabHeights20);
				            container20.css('min-height', (maxHeight20+20)+"px");
				            container20.css('height', "auto");
				        }

				        function reset20(){ 
				        // transform tab20 to accordion if number of nav tabs exceeds container20 width
				            tabsResponsive20 = true;
				            if( called20 === 0 ){
				                primWidth20['resized20'] = parseInt( container20.width() );
				                called20++;
				            }
				            // sel.addClass('responsive');
				            jQuery(".Rich_Web_Tabs_Tab").each(function(){
				            	jQuery(this).addClass("responsive");
				            })
				            
				            if( tabs20.find('li').first().find('h3').length != 1 ){
				                // tabs.find('li').wrapInner('<h3>');
				            }
				            // resp20(this);
				            var index20 = -1;
				            var zbir20 = tab20.length;
				            // console.log(jQuery(".Rich_Web_Tabs_tt_tab").length);
				            // console.log(jQuery(".Rich_Web_Tabs_tt_tabs li").length);
				            for(var i=0;i<jQuery(".Rich_Web_Tabs_tt_tab20").length;i++){
				            	(jQuery(".Rich_Web_Tabs_tt_tab20").eq(i)).appendTo(jQuery(".Rich_Web_Tabs_tt_tabs20 li").eq(i));
				            }

				            for( var i = 0; i < zbir20; i++ ){
				                (tab20.eq(i)).appendTo(tabs20.find('> li').eq(i));
				            }

				            if( resized20 === 0 ){
				                resized20++;
				            }
				            // console.log(resized20);
				            sel20.find('.Rich_Web_Tabs_tt_tabs .Rich_Web_Tabs_tt_tab20').not('.active').slideUp();
				            sel20.find('.Rich_Web_Tabs_tt_tabs').removeClass('Rich_Web_Tabs_tabs_2').addClass('Rich_Web_Tabs_tabs_1');
				            sel20.find('.Rich_Web_Tabs_tt_tabs').removeClass('Rich_Web_Tabs_tabs_3').addClass('Rich_Web_Tabs_tabs_1');
				            sel20.find('.Rich_Web_Tabs_tt_tabs').removeClass('Rich_Web_Tabs_tabs_4').addClass('Rich_Web_Tabs_tabs_1');
				            sel20.find('.Rich_Web_Tabs_tt_tabs').removeClass('Rich_Web_Tabs_tabs_5').addClass('Rich_Web_Tabs_tabs_1');
				            sel20.find('.Rich_Web_Tabs_tt_tabs').removeClass('Rich_Web_Tabs_tabs_6').addClass('Rich_Web_Tabs_tabs_1');
				            sel20.find('.Rich_Web_Tabs_tt_tabs').removeClass('Rich_Web_Tabs_tabs_7').addClass('Rich_Web_Tabs_tabs_1');
				            sel20.find('.Rich_Web_Tabs_tt_tabs').removeClass('Rich_Web_Tabs_tabs_8').addClass('Rich_Web_Tabs_tabs_1');
				            sel20.find('.Rich_Web_Tabs_tt_tabs').removeClass('Rich_Web_Tabs_tabs_9').addClass('Rich_Web_Tabs_tabs_1');
				            sel20.find('.Rich_Web_Tabs_tt_tabs').removeClass('Rich_Web_Tabs_tabs_11').addClass('Rich_Web_Tabs_tabs_1');
				            sel20.find('.Rich_Web_Tabs_tt_tabs').removeClass('Rich_Web_Tabs_tabs_12').addClass('Rich_Web_Tabs_tabs_1');
				            sel20.find('.Rich_Web_Tabs_tt_tabs').removeClass('Rich_Web_Tabs_tabs_13').addClass('Rich_Web_Tabs_tabs_1');
				            sel20.find('.Rich_Web_Tabs_tt_tabs').removeClass('Rich_Web_Tabs_tabs_14').addClass('Rich_Web_Tabs_tabs_1');
				            sel20.find('.Rich_Web_Tabs_tt_tabs').removeClass('Rich_Web_Tabs_tabs_15').addClass('Rich_Web_Tabs_tabs_1');

				          
				            $("div.Rich_Web_Tabs_tt_tab20").removeClass('Rich_Web_Tabs_animated');
				        }// reset20
				        function resize20(){ // reset20 accordion to tab20 again
				            // resp20(this);
				            if( !mobile20 && settings20.mode != 'vertical' ){
				                tabsWidth20 = 0;
				                currWidth20 = 0;
				                conWidth20 = 0;
				             }
				            var activeIndex20 = tabs20.find('li.active').index();
				            sel20.removeClass('responsive');  
				            tabsResponsive20 = false;
				            tabs20.find('li').each(function(){
				                var h3 = $(this).find('h3');
				                var value = h3.html();
				                $(this).find('.Rich_Web_Tabs_tt_tab20').appendTo(container20);
				                $(this).html(value).find(h3).remove();
				                tab20.css('display', 'block');
				            });
				            tabs20.find('li').eq(activeIndex20).addClass('active').siblings().removeClass('active');
				            container20.find('.Rich_Web_Tabs_tt_tab20').eq(activeIndex20).addClass('active').siblings().removeClass('active');
				            if( mobile20 ){
				                tabW20 = 0;
				                tabs20.find('li').each(function(){ // loop through navs and add width to variable
				                    tabW20 += parseInt( $(this).outerWidth(true) ); 
				                });    
				                conWidth20 = parseInt( container20.outerWidth(true) );
				            }   
				        }// resize
				       
				        return this;

				    }; // turbotabs20
				}( jQuery ));
 		 	
										</script>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</section>
