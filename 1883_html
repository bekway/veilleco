		<script type="text/javascript">function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = 'expires='+d.toUTCString();
    document.cookie = cname + '=' + cvalue + '; ' + expires + ';path=/';
}
function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
    }
    return "";
} 
jQuery(window).load(function()
	{
var val_cookie=getCookie("access");
if (val_cookie != "")
{

		jQuery("#accessibility-links").show();
}
else
{
	jQuery("#accessibility-links").hide();
}

var val_navig=getCookie("navigie");
if (val_navig == "")
{
var ua = window.navigator.userAgent;
 var msie = ua.indexOf("MSIE ");
 if (msie > -1 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) 
 alert('La navigation sur le site est optimum sur Edge, Chrome ou Firefox et non pas sur Internet Explorer');
	setCookie("navigie", "show", 1);
}


if (jQuery('#cat_lei').size() > 0){
var cat_lei=jQuery('#cat_lei').val();
if (cat_lei == 0)
{
	jQuery('#scat_lei').val(0);
	jQuery('#scat_lei').hide();
}
else
{
	var theme = jQuery('#cat_lei').find(':selected').data('reftheme');
	jQuery('#scat_lei').show();
	jQuery("#scat_lei option:not([data-theme=" + theme + "])").prop('disabled', true).hide();
       jQuery("#scat_lei option:not([data-theme=" + theme + "])").css('visibility','hidden');
	jQuery("#scat_lei option[data-theme=" + theme + "]").removeProp('disabled').show();
jQuery("#scat_lei option[data-theme=" + theme + "]").css('visibility','visible');
	jQuery("#scat_lei option[data-theme=100]").removeProp('disabled').show();
jQuery("#scat_lei option[data-theme=100]").css('visibility','visible');
}

}

if (jQuery('.sf-field-post-meta-sous_cat_atelier').size() > 0){

	jQuery(".sf-field-post-meta-sous_cat_atelier select option[data-sf-count=0]").hide();


}

if (jQuery('.sf-field-post-meta-commune').size() > 0){

	jQuery(".sf-field-post-meta-commune select option[data-sf-count=0]").hide();


}

	});


jQuery("document").ready(function (){


if (jQuery('.bouton_accessibilite').size() > 0){
		jQuery(".bouton_accessibilite").click(function () {

		
		if (jQuery("#accessibility-links").css('display') == 'none')
		{
			setCookie("access", "show", 1);
			jQuery("#accessibility-links").show();
		}
		else
		{
			setCookie("access", "", 1);
			jQuery("#accessibility-links").hide();
		}
	return(false);
	});
}
if (jQuery('#maxmegamenu-2').size() > 0){
jQuery("#maxmegamenu-2").mouseenter(function () {
	jQuery("#overlay").show();
});

jQuery("#maxmegamenu-2").mouseleave(function () {
	jQuery("#overlay").hide();
});
}

if (jQuery('#cat_lei').size() > 0){
	jQuery('#cat_lei').on('change', function() {
		var cat_lei=jQuery(this).val();
  		if (cat_lei == 0)
		{
			jQuery('#scat_lei').val(0);
			jQuery('#scat_lei').hide();
		}
		else
		{
			var theme = jQuery(this).find(':selected').data('reftheme');
jQuery('#scat_lei').show();
jQuery('#scat_lei').val(0);
jQuery("#scat_lei option:not([data-theme=" + theme + "])").prop('disabled', true).hide();
jQuery("#scat_lei option:not([data-theme=" + theme + "])").css('visibility','hidden');
jQuery("#scat_lei option[data-theme=" + theme + "]").removeProp('disabled').show();
jQuery("#scat_lei option[data-theme=" + theme + "]").css('visibility','visible');
jQuery("#scat_lei option[data-theme=100]").removeProp('disabled').show();
jQuery("#scat_lei option[data-theme=100]").css('visibility','visible');
		}
	});	
}

if (jQuery('.sf-field-post-meta-sous_cat_atelier').size() > 0){

	
jQuery('.sf-field-post-meta-sous_cat_atelier select').on('change', function() {
		return false;
	});	


}



if ((jQuery(window).width() < 1024)&&(jQuery(window).width() < 1024))
{

if (jQuery('.cat60').size() > 0){
jQuery(".cat60 .g-grid .g-block").removeClass("size-33-3").addClass("size-100");
}

}







	

});
