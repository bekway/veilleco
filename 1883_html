						    border-left: 15px solid transparent !important;
						    border-top: 30px solid rgba(0, 0, 0, 0) !important;
						    content: "" !important;
						    position: absolute !important;
						    display: block !important;
						    left: 0 !important;
						    opacity: 1 !important;
						    top: 0 !important;
						    z-index: 2 !important;
						}
						.Rich_Web_Tabs_Tab_20 .Rich_Web_Tabs_tabs_30 li:last-child:hover:before {
							border-bottom: 30px solid rgba(0, 0, 0, 0) !important;
						    border-left: 15px solid #ececec !important;
						    border-top: 30px solid rgba(0, 0, 0, 0) !important;
						    content: "" !important;
						    position: absolute !important;
						    display: block !important;
						    left: 0 !important;
						    opacity: 1 !important;
						    top: 0 !important;
						    z-index: 1 !important;
						}
						.Rich_Web_Tabs_Tab_20 .Rich_Web_Tabs_tabs_30 :last-child.active:before {
							border-bottom: 30px solid rgba(0, 0, 0, 0) !important;
						    border-left: 15px solid #ececec !important;
						    border-top: 30px solid rgba(0, 0, 0, 0) !important;
						    content: "" !important;
						    position: absolute !important;
						    display: block !important;
						    left: 0 !important;
						    opacity: 1 !important;
						    top: 0 !important;
						    z-index: 1 !important;
						}
