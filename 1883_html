										<script type="text/javascript">
			    	var arr = ["safa", "sodf"];
			    	var startVal = arr.length;
			    	arr[11] = "osdfc";
			    	

				    var el_height_32 = jQuery('.Rich_Web_Tabs_Tab_20 .Rich_Web_Tabs_tabs_32').height();
				    if(el_height_32 > 65) {
				    	jQuery('.Rich_Web_Tabs_Tab_20 .Rich_Web_Tabs_tabs_32 :first-child').css({"border-radius" : "25px 0 0 0"});
				    	jQuery('.Rich_Web_Tabs_Tab_20 .Rich_Web_Tabs_tabs_32 :last-child').css({"border-radius" : "0 0 25px 0"});
				    }
				    var el_height_18 = jQuery('.Rich_Web_Tabs_Tab_20 .Rich_Web_Tabs_tabs_18').height();
				     if(el_height_18 > 65) {
				     	jQuery('.Rich_Web_Tabs_Tab_20 .Rich_Web_Tabs_tabs_18').css({"border-radius" : "25px"});
				     }
				      var el_height_26 = jQuery('.Rich_Web_Tabs_Tab_20 .Rich_Web_Tabs_tabs_26').height();
				      if(el_height_26 < 65) {
				     	jQuery('.Rich_Web_Tabs_Tab_20 .Rich_Web_Tabs_tabs_26 div.rich_web_tab_ul_line').css({"position" : "absolute", "border" : "1px solid #000", "border-left" : "0", "border-right" : "0", "width" : "100%", "transform" : "translateY(-50%)", "top" : "50%", "border-top" : "none"});
				     }
				     if(el_height_26 > 65) {
				     	jQuery('.Rich_Web_Tabs_Tab_20 .Rich_Web_Tabs_tabs_26 div.rich_web_tab_ul_line').css({"position" : "absolute", "border" : "1px solid #000", "border-left" : "none", "border-right" : "none", "height" : "50%", "width" : "100%", "transform" : "translateY(-50%)", "top" : "50%"});
				     }
			    	jQuery(document).ready(function(){
					    jQuery('.Rich_Web_Tabs_Tab_20').turbotabs20({
					    	mode: 'horizontal',
					        animation : 'FadeUp',
				            width: '100%',
					    }); 
					});
			    
