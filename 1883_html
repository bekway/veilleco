						   border-radius: 5px 0 0 5px;
						}
						.Rich_Web_Tabs_Tab_20 .Rich_Web_Tabs_tabs_30 :last-child {
						   border-radius: 0 5px 5px 0;
						}
						.Rich_Web_Tabs_Tab_20 .Rich_Web_Tabs_tabs_30 :last-child:before {
						   content: none !important;
						}
						/*****************************/
						/* New Style 16 */
						/*****************************/
						.Rich_Web_Tabs_Tab_20 .Rich_Web_Tabs_tabs_31 {
							border: 0 solid rgba(255,255,255,0);
						}
