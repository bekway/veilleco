		function showMapWindow(id, nom){
			$('.listing_ville').slideUp();
			$('#btn_lst_ville .sprite').removeClass('arrow_top').addClass('arrow_bot');
			google.maps.event.trigger(tabMarker[id], 'click');
			open_lst=!open_lst;
			$('#btn_lst_ville .left').html(nom);
			MyMap.map.setZoom(12);
			MyMap.map.setCenter(tabMarker[id].getPosition());
		}
	}
		
