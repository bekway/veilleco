 Terroir, \u00e9t\u00e9 2019)<\/a><\/p>\n<\/div><\/div><div class=\"icon_list_connector\"  style=\"border-right-width: 1px;border-right-style: dashed;border-color: #ffffff;\"><\/div><a class=\"ulimate-info-list-link\" href=\"https:\/\/www.agglo-sudsaintebaume.fr\/wp-content\/uploads\/2019\/05\/oppos-1.jpg\" title=\"Tribune Libre\" target=\"_blank\" rel=\"nofollow noopener\" ><\/a><\/li><\/ul><\/div><\/div><\/div><\/div><\/div>\n" + '');
$.strRemove = function(theTarget, theString) {
return $("<div/>").append(
$(theTarget, theString).remove().end()
).html();
};
var articleStr = sessionStorage.getItem('h5ab-print-article');
var removeArr = ['video','audio','script','iframe'];
$.each(removeArr, function(index, value){
var processedCode = $.strRemove(value, articleStr);
articleStr = processedCode;
});
var fullPrintContent = articleStr;
sessionStorage.setItem('h5ab-print-article', fullPrintContent);
});
		</script>
