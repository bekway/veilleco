						<script type="text/javascript">

	// version pour survol des dropdown bootstrap :
	var windowWidth = $(window).width();
	if ( windowWidth > 1024 ) { 
		$(document).ready(function(){
			var bodyheight = $('body').height();
			var navpos = $('#nav').offset().top;
			var navheight = $('#nav').height();
			var overlayheight = bodyheight - navpos - navheight;

		    $('ul.nav li.dropdown').hover(function() {
		      $(this).find('.dropdown-menu').stop(true, true).delay(0).fadeIn(100);
		    }, function() {
		      $(this).find('.dropdown-menu').stop(true, true).delay(0).fadeOut(100);
		    });  
			$('.niv1').hover(function() {$('#overlay').css({ height: overlayheight + "px" }); },function(){ $('#overlay').css({ height: "0" }); });
		});
	}
