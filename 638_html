$(document).ready(function() {

	$('body').on('click', function(event)
	{
		if($(event.target).parent().attr('id') === 'recherche')
		{
			$('.asearch').hide();
			$('#SearchText').addClass('showed').focus();
		}
		else
		{
			$('#SearchText').removeClass('showed');
			$('.asearch').show();
		}
	});

});
