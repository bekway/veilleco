    $(function() {
        
        $(".sub_rech").click(function() {
            if ( $("#query").val() != "" ) {
                var keywords = $("#query").val();
                runSearch(keywords);
            }
        });
        
        $("#query").keypress( function(event) {
            console.log("test");
            if (event.keyCode == 13) {    
                var keywords = $("#query").val();
                runSearch(keywords);
                return false;
            }   
        });
           
        
        function runSearch(keywords) {
            console.log('ok')
            var route = Routing.generate("localeo_grc_assistant_search",{"keywords":keywords});
            window.location.href= route;
            
        }
        
    });
    
