											<script type="text/javascript">
        (function($, window, document) {
          $(function() {
            var map_contact;
            var infowindow_geo;
            var markers_geo = [];
            var poly_geo = [];
            var bounds = new google.maps.LatLngBounds();
            var pos = new google.maps.LatLng('46.948275', '4.283607');
              

            var zoom = 15;
            function initialiser_geo_assoc() {
              var options = {
                center: pos,
                zoom: 10,
                mapTypeId: google.maps.MapTypeId.ROADMAP
              };
                                
              map_contact = new google.maps.Map(document.getElementById("map_assoc_1302"),options);  

              
              var marker = new google.maps.Marker({
                map : map_contact,
                position: pos,
                animation: google.maps.Animation.DROP,
                icon: new google.maps.MarkerImage('/fileadmin/templates/master/img/marker.png'),
              });
                
              map_contact.setCenter(pos);

              addMarkerGeo();
            }
            function addMarkerGeo() {
              var marker = new google.maps.Marker({
                map : map_contact,
                position : pos,
                animation: google.maps.Animation.DROP,
                icon: new google.maps.MarkerImage('/fileadmin/templates/master/img/marker.png'),
              });

            }

            function setAllMapGeo(map) {
              for (var i = 0; i < markers_geo.length; i++) {
                markers_geo[i].setMap(map);
              }
            }
            function showOverlaysGeo() {
              setAllMapGeo(map);
            }
            function deleteOverlaysGeo() {
              setAllMapGeo(null);
              markers_geo = [];
            }

            initialiser_geo_assoc();

          });
        }(window.jQuery, window, document));
      
											</script>
