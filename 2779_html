								<script type="text/javascript">
                                function change( el ){
                                    if ( el.value === "Noir & Blanc" )
                                        el.value = "Couleur";
                                    else
                                        el.value = "Noir & Blanc";
                                }
                            
								</script>
								<div class="invert"><button name="invert_button" class="invert_button" id="invert_button" onclick="switch_value(this.id)">Inverser</button></div>
