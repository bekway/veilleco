			<script>
        $(function() {
            $(window).on('load', function(event) {
                $('#layout-navigation').affix({
                    offset: {
                        top: function () {
                            // var slideshowHeight = $('#section-spotlight').outerHeight();
                            // var layoutNavHeight = $('#layout-navigation').outerHeight();
                            if ( $('#section-alert').length > 0 ) {
                                var top = $('#section-alert').outerHeight();
                                return top;
                            } else {
                                return 0;
                            }
                        }
                    }
                });
            });
            
            function hoverMenu() {
                var slideshowHeight = $('#section-spotlight').outerHeight();
                var layoutNavHeight = $('#layout-navigation').outerHeight();
                if ( $('#section-alert').length > 0 ) {
                    var slideshowHeight = slideshowHeight + $('#section-alert').outerHeight();
                }
                slideshowHeight = slideshowHeight - layoutNavHeight;
                if ( $(window).scrollTop() >= slideshowHeight ) {
                    $('#layout-navigation').addClass('active');
                } else {
                    $('#layout-navigation').removeClass('active');
                }
            }


            $(window).on('scroll', throttle( function(){
                hoverMenu();
            }, 150));

            $(window).on('resize', debounce( function(){
                hoverMenu();
            }, 150));

            hoverMenu();
                    });
    
			</script>
