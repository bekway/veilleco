					<script>
			$(function() {

				var $masonry = $('#masonry'),
					msnry = null;

				$(window).on('resize', throttle( function() {
					if ( $(window).width() >= 768 ) {
						if( msnry === null ) {
							msnry = $masonry.masonry({
								itemSelector: '.grid-item',
								columnWidth: 114,
								fitWidth: true,
								initLayout: false,
							});

							msnry.masonry( 'on', 'layoutComplete', function() {
								$masonry .removeClass('loading');
							});

							msnry.masonry();
						}
					} else {
						if ( msnry !== null ) {	
							$masonry.masonry('destroy');
							msnry = null;
						}
					}
				}, 50)).resize();


			});
		
