				            $(function () {
				                $eventsCarousel = $('#js-events-carousel');
				                $eventsCarousel.owlCarousel({
				                	autoplaySpeed: 750,
				                	navSpeed: 300,
				                	dotsSpeed: 300,
				                	autoplay: false,
				                	dots: true,
				                	navText: ['<span><i class="icon-long-arrow-left">', '<span><i class="icon-long-arrow-right">'],
				                	items: 1,
				                	nav: false,
				                	dotsEach: 1,
				                	loop: true,
				                	margin: 20,
				                	responsive: {
				                		768 : { items: 2, nav: false, dots: true, },
				                		992 : { items: 2, nav: true, dots: false, margin: 150, },
				                		1200 : {items: 2, nav: true, dots: false, margin: 116, }
				                	},
				                	onChanged : function (event) {
				                		var $this = $(this)[0].$element;
							            $this.find('.owl-item.main-active').removeClass('main-active');
								        if ( $(this)[0]._breakpoint == 992 || $(this)[0]._breakpoint == 1200 ) {
								            setTimeout(function() {
								                var $activeItem = $this.find('.owl-item.active').eq(0).addClass('main-active');
								            }, 1);
								        }
				                	},
				                	onResized : function (event) {
				                		var $this = $(this)[0].$element;
							            $this.find('.owl-item.main-active').removeClass('main-active');

								        if ( $(this)[0]._breakpoint == 992 || $(this)[0]._breakpoint == 1200 ) {
								            setTimeout(function() {
								                var $activeItem = $this.find('.owl-item.active').eq(0).addClass('main-active');
								            }, 1);
								        }
				                	},
				                });
				            });
				        
									</script>
