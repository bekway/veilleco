					<script type="text/javascript">
$(document).ready(function() {
	$("#btnFermerBandeauAlerte").click(function(){
		$.ajax({
			type: "POST",
			url: SERVER_ROOT + "include/ajax/ajax.alerteInfo.php",
			success: function(){
				var button = $("#btnFermerBandeauAlerte");
				var target = $("#btnFermerBandeauAlerte").attr("aria-controls");
				button.attr('aria-expanded', 'false');
				$('.tpl_alerte').addClass('invisible')
			}
		});
	});
    $("#btnAlertTop").click(function(){
        var button = $("#btnFermerBandeauAlerte");
        var target = $("#btnFermerBandeauAlerte").attr("aria-controls");
        button.attr('aria-expanded', 'true');
        $('#' + target).slideUp().attr('aria-hidden', 'false');
        
        if ($('.tpl_alerte').hasClass('invisible')) {
        	$('.tpl_alerte').removeClass('invisible')
        }else {
        	$('.tpl_alerte').addClass('invisible')
        }
    });
});
					</script>
