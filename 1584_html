							<!-- Begin Content -->
							<ul itemscope itemtype="http://schema.org/BreadcrumbList" class="breadcrumb">
								<li class="active"><span class="divider icon-location"></span></li>
								<li itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem"><a itemprop="item" href="/fr/" class="pathway"> <span itemprop="name"> Accueil </span> </a> <span class="divider"> <img src="/media/system/images/arrow.png" alt=""> </span>
