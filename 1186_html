		<script type="text/javascript">
	jQuery(document).ready(function($) {
			
			/* Open */
//		   	$('.search-form').css('opacity', '0');

			function openNav() {
			  var max_height = "100%" ;
			  jQuery('#searchBar').animate({"height": max_height}, function() {  
			       // Only on Mobile Phone
			       if (window.matchMedia("(max-width: 590px)").matches) {
				   	$( ".search-form" ).fadeIn( 500, function() {} );
				   } 
			  })
			}
			
			/* Close */
			function closeNav() {
			  document.getElementById("searchBar").style.height = "0%";
				// Only on Mobile Phone
				if (window.matchMedia("(max-width: 590px)").matches) {}
			}

			$( ".lk-search" ).click(function() {
				openNav();
				return false;
			});

			$( ".closebtn" ).click(function() {
				closeNav();
				return false;
			});

		// MENU NAV
		var iWindowsSize = $(window).width();
		if (iWindowsSize  > 991 ){
			$('.dropdown-toggle').dropdownHover();
		}
		else {
			$('ul.navbar-nav > li > ul > li.menu-item-has-children > a').on("click", function(e){
			  $(this).next('ul').toggle();
			  e.stopPropagation();
			  e.preventDefault();
			});
//			$( "ul.navbar-nav > li > ul > li.menu-item-has-children a:first-child" ).each(function() {
//				$(this).append( " <i class=\"fa fa-caret-down\" aria-hidden=\"true\">" );
//			});
		}
		// CAROUSEL HOME
		$("#carouselHome").owlCarousel({
			navigation:true,
			navigationText: [
			  "<i class=\"fa fa-angle-left fa-5x\">",
			  "<i class=\"fa fa-angle-right fa-5x\">"
			  ],
		   slideSpeed : 300,
		   paginationSpeed : 400,
		   singleItem:true
		});
		
	// Big Number ----------------------------
	$("#bigNumber").hide();
		$( "#tel" ).click(function() {
			$("#bigNumber").toggle(500);
			return false;
		});
	
		$( "#bigNumber" ).click(function() {
		  $(this).fadeOut( 500 );
		  return false;
		});
		// Affichage et centrage du "BIG NUMBER"
		$(window).load(function() {
		var windowHeight = $(window).height(); 
	 	
	 	var haut = Math.ceil((windowHeight - $("#bigNumber").height()) / 2 + $(window).scrollTop());
	   	function placeNumber(haut){
		    $("#bigNumber").css('top', haut);
		   }
		$(window).resize(function() {
		   	var windowHeight = $(window).height();
	 		var haut = Math.ceil((windowHeight - $("#bigNumber").height()) / 2 + $(window).scrollTop());
		    placeNumber(haut);
		 });
		  	placeNumber(haut);
		 });
		
	});
		</script>
