			<script type="text/javascript">
          jQuery(function ($)
          {
            $('#proposition-evenement input#date-debut-manif, #proposition-evenement input#date-fin-manif').inputmask('99/99/9999', { 'placeholder': 'JJ/MM/AAAA' })

              
                          // Function pour générer l'adresse "humaine" via des coordonnées
            // ------------------------------------------------------------------------
            genererAdresse = function (coordonnee, callback)
            {
              var geocoder = new google.maps.Geocoder()
              var coordonneeLatLngString = coordonnee.split(',')
              var coordonneeLatInt = parseFloat(coordonneeLatLngString[0])
              var coordonneeLngInt = parseFloat(coordonneeLatLngString[1])
              geocoder.geocode({
                'latLng': new google.maps.LatLng(coordonneeLatInt, coordonneeLngInt)
              }, function (results, status)
              {
                if (status == google.maps.GeocoderStatus.OK) {
                  if (results[0]) {
                    street_number = ''
                    route = ''
                    locality = ''
                    postal_code = ''
                    jQuery.each(results[0].address_components, function (index, value)
                    {
                      if (value.types[0] == 'street_number') {
                        street_number = value.long_name	// Récupérer le numéro de l'adresse
                      } else if (value.types[0] == 'route') {
                        route = value.long_name			// Récupérer le nom de la route
                      } else if (value.types[0] == 'locality') {
                        locality = value.long_name			// Récupérer la ville
                      } else if (value.types[0] == 'postal_code') {
                        postal_code = value.long_name		// Récupérer le nom de la route
                      }
                      adresseFull = results[0].formatted_address
                    })
                    callback(street_number, route, locality, postal_code, adresseFull)
                  } else {
                    console.log('Pas de résultat trouvé')
                  }
                } else {
                  console.log('Geocoder FAIL ! Merci de contacter un administrateur du site. Status : ' + status)
                }
              })
            }

            // Function pour récupérer les coordonnées au "dragend"
            // ------------------------------------------------------------------------
            bindDragDropMarker = function (marker)
            {
              google.maps.event.addListener(marker, 'dragend', function (point)
              {
                var LatLngRobotInt = point.latLng.lat() + ',' + point.latLng.lng()
                var LatLngRobotString = LatLngRobotInt.toString()
                addressHumaine = genererAdresse(LatLngRobotString, function (street_number, route, locality, postal_code, adresseFull)
                {
                  $('.bloc-form #adresse-manif').val(street_number + ' ' + route)
                  $('.bloc-form #code-postal-manif').val(postal_code)
                  $('.bloc-form #ville-manif').val(locality)
                  $('.bloc-form #ville-geocode-manif').val(locality)
                  $('.bloc-form #adresse-complete-humaine-manif').val(adresseFull)
                  $('.bloc-form #coordonnees-lieu-manif').val(LatLngRobotString)
                })
              })
            }

            // Charger la Google map
            // ---------------------------------------------------------------
            function initialize ()
            {
              geocoder = new google.maps.Geocoder()

              // Style de la google map
              // --------------------------------------------------------------------------
              newStyle = [{
                'featureType': 'poi.park',
                'elementType': 'geometry.fill',
                'stylers': [{ 'color': '#dddddd' }]
              }]

              // Coordonnée du point
              // --------------------------------------------------------------------------
              var myLatlng = new google.maps.LatLng(47.614689, -0.452445)

              // Carte centrée sur l"adresse
              // --------------------------------------------------------------------------
              myMapOptions = {
                zoom: 16,
                center: myLatlng,
                mapTypeId: google.maps.MapTypeId.ROADMAP,
                styles: newStyle
              }

              // Création de la carte
              // --------------------------------------------------------------------------
              myMap = new google.maps.Map(
                document.getElementById('googlemap-prop'),
                myMapOptions
              )

              // Création d'un marker "par défaut"
              // --------------------------------------------------------------------------
              marker = new google.maps.Marker({
                map: myMap,
                draggable: true,
                position: new google.maps.LatLng(47.614689, -0.452445)
              })

              var isIE11 = !!(navigator.userAgent.match(/Trident/) && navigator.userAgent.match(/rv[ :]11/))
              if (!isIE11) {
                                  marker.setIcon({ url: 'data:image/svg+xml;charset=UTF-8,' + encodeURIComponent('<svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="30px" height="40px" viewBox="0 0 232 308.8" style="enable-background:new 0 0 232 308.8;" xml:space="preserve"><style type="text/css">.marker-pi-icone {fill:#fff;} .marker-pi-fond {fill:#000;}<g><path class="marker-pi-fond" d="M232,116c0,68.6-67.4,145.8-99.4,185c-8.6,10.5-24.5,10.5-33.1,0C67.4,261.8,0,184.6,0,116 C0,51.9,51.9,0,116,0S232,51.9,232,116z"/><path class="marker-pi-icone" d="M79.3,122.4c0,10.6-8.6,19.2-19.2,19.2S41,133,41,122.4s8.6-19.2,19.2-19.2 S79.3,111.8,79.3,122.4z M116,103.3c-10.6,0-19.2,8.6-19.2,19.2s8.6,19.2,19.2,19.2s19.2-8.6,19.2-19.2S126.6,103.3,116,103.3z M171.9,103.3c-10.6,0-19.2,8.6-19.2,19.2s8.6,19.2,19.2,19.2c10.6,0,19.2-8.6,19.2-19.2S182.5,103.3,171.9,103.3z"/>') })
              } else {
                                  marker.setIcon("https://www.ccals.fr/wp-content/plugins/creasit-plan-interactif/images/picto_ie11/autre.png")
              }

              // Ajouter l'adresse humaine et la ville géocode du marker
              var coordonnees_client = '47.614689, -0.452445'
              genererAdresse(coordonnees_client, function (street_number, route, locality, postal_code, adresseFull)
              {
                $('.bloc-form #adresse-complete-humaine-manif').val(adresseFull)
                $('.bloc-form #ville-geocode-manif').val(locality)
              })

              bindDragDropMarker(marker)

              // Faire un resize de la map si on clique sur "Oui"
              // --------------------------------------------------------------------------
              $('.checkboxgmap input').change(function ()
              {
                var valChecked = $(this).val()
                if (valChecked == 'oui') {
                  google.maps.event.trigger(myMap, 'resize')
                  myMap.setCenter(marker.getPosition())
                }
              })

            }

            google.maps.event.addDomListener(window, 'load', initialize)
                        })
        
			</script>
			<div class="clearfix"></div>
		</div>
