	jQuery(function() {
		function chargeSeance()
		{
			jQuery.ajax({
				url: 'http://scripting.brive.fr/brive/deliberations/ajax.php?SEA=' + selected + '&rnd=1594215834',
				context: document.body
			}).done(function(data) {
				jQuery('#div' + selected).html(data);
			});
		}
		var selected = "annee2020";
		jQuery( '.vc_tta-panel-title a' ).click(function() {
			selected = jQuery(this).attr('href').substring(1);
			chargeSeance();
		});
		chargeSeance();
		jQuery('#wait').ajaxStart(function() {
			jQuery(this).show();
		}).ajaxComplete(function() {
			jQuery(this).hide();
		});
	});
