					<path class="marker-pi-fond" d="M232,116c0,68.6-67.4,145.8-99.4,185c-8.6,10.5-24.5,10.5-33.1,0C67.4,261.8,0,184.6,0,116 C0,51.9,51.9,0,116,0S232,51.9,232,116z"></path>
					<path class="marker-pi-icone" d="M79.3,122.4c0,10.6-8.6,19.2-19.2,19.2S41,133,41,122.4s8.6-19.2,19.2-19.2 S79.3,111.8,79.3,122.4z M116,103.3c-10.6,0-19.2,8.6-19.2,19.2s8.6,19.2,19.2,19.2s19.2-8.6,19.2-19.2S126.6,103.3,116,103.3z M171.9,103.3c-10.6,0-19.2,8.6-19.2,19.2s8.6,19.2,19.2,19.2c10.6,0,19.2-8.6,19.2-19.2S182.5,103.3,171.9,103.3z"></path>
				</g>
			</svg>') });} else {marker.setIcon("http://www.coeurdebeauce.fr/wp-content/plugins/creasit-plan-interactif/images/picto_ie11/autre.png");} var prev_infobulle; google.maps.event.addListener(marker, "click", function(){ infobulle = new InfoBubble({ map: myMap, content: this.contenuInfoBulle, // Contenu de l'infobulle position: new google.maps.LatLng(48.202878,1.8799370000000408), // Coordonnées latitude longitude du marker shadowStyle: 0, // Style de l'ombre de l'infobulle (0, 1 ou 2) padding: 20, // Marge interne de l'infobulle (en px) backgroundColor: "#fff", // Couleur de fond de l'infobulle borderRadius: 2, // Angle d'arrondis de la bordure arrowSize: 10, // Taille du pointeur sous l'infobulle borderWidth: 1, // Épaisseur de la bordure (en px) borderColor: "rgba(0, 0, 0, 0.2)", // Couleur de la bordure disableAutoPan: false, // Désactiver l'adaptation automatique de l'infobulle hideCloseButton: false, // Cacher le bouton 'Fermer' arrowPosition: 50, // Position du pointeur de l'infobulle (en %) arrowStyle: 0, // Type de pointeur (0, 1 ou 2) minWidth : 300, // Largeur minimum de l'infobulle (en px) maxWidth : 300 // Largeur minimum de l'infobulle (en px) }); // Si on a déjà une infobulle ouverte, on la ferme if(prev_infobulle){ prev_infobulle.close(); } // La précédent infobulle devient l'infobulle que l'on va ouvrir prev_infobulle = infobulle; // Enfin, on ouvre l'infobulle infobulle.open(); // On centre la map sur l'infobulle myMap.panTo(this.getPosition());google.maps.event.addListener(infobulle, "domready", function(){ // infobubble est notre info-bulle setTimeout(function(){ jQuery("a.itineraire").on("click", function(){ var adresse = jQuery(this).data("adresse"); jQuery("#input-retour").removeAttr("placeholder"); jQuery("#input-aller").removeAttr("placeholder"); if(jQuery(this).hasClass("itineraire-aller")){ jQuery("input#input-aller").val(adresse); jQuery("input#input-retour").val(""); } else if (jQuery(this).hasClass("itineraire-retour")){ jQuery("input#input-retour").val(adresse); jQuery("input#input-aller").val(""); } jQuery("#itineraire-box").slideDown("slow"); var offset_itineraire = jQuery("#itineraire-box").offset(); var offset_itineraire_sn = offset_itineraire.top - 150; jQuery("html,body").animate({scrollTop: offset_itineraire_sn}, "slow"); }) }, 500);});}); bounds.extend(new google.maps.LatLng(48.202878,1.8799370000000408)); markers.push(marker); var mcOptions = {gridSize: 50, maxZoom: 20}; markerCluster = new MarkerClusterer(myMap, markers, mcOptions); myMap.fitBounds(bounds); var listener = google.maps.event.addListener(myMap, "idle", function() { if(markers.length == 1) myMap.setZoom(16); google.maps.event.removeListener(listener); }); // -------------------------------------------------------------------------- // CHAMP ITINÉRAIRE // -------------------------------------------------------------------------- var optionsItineraire = { componentRestrictions: {country: 'fr'} }; var inputFirst = document.getElementById('input-retour'); autocompleteItineraire = new google.maps.places.Autocomplete(inputFirst, optionsItineraire); var inputSecond = document.getElementById('input-aller'); autocompleteItineraireSecond = new google.maps.places.Autocomplete(inputSecond, optionsItineraire); direction = new google.maps.DirectionsRenderer({ map : myMap, panel : document.getElementById('caption'), suppressMarkers : true }); jQuery("a.inverser").click(function(){ var htmlAller = jQuery('input#input-aller').val(); var htmlRetour = jQuery('input#input-retour').val(); jQuery('input#input-aller').val(htmlRetour); jQuery('input#input-retour').val(htmlAller); calculate(); }) jQuery("a.calculer-itineraire, #itineraire-box a.calculer-itineraire").click(function(){ calculate(); return false; }) jQuery(".moyen-de-transport-itineraire a").click(function(){ jQuery(".moyen-de-transport-itineraire a").removeClass("active"); jQuery(this).addClass("active"); calculate(); }) // -------------------------------------------------------------------------- // FONCTION ITINERAIRE // -------------------------------------------------------------------------- calculate = function(){ jQuery('.moyen-de-transport-itineraire a').each(function(){ if(jQuery(this).hasClass('active')){ aActive = jQuery(this).data('moyen'); } }) if(aActive == "default"){ moyenDeTransport = "DRIVING"; } else if(aActive == "voiture"){ moyenDeTransport = "DRIVING"; } else if(aActive == "bus"){ moyenDeTransport = "TRANSIT"; } else if(aActive == "a-pied"){ moyenDeTransport = "WALKING"; } origin = document.getElementById('input-aller').value; // Le point départ destination = document.getElementById('input-retour').value; // Le point d'arrivé if(origin && destination){ var request = { origin : origin, destination : destination, travelMode : google.maps.TravelMode[moyenDeTransport] // Type de transport } var directionsService = new google.maps.DirectionsService(); // Service de calcul d'itinéraire directionsService.route(request, function(response, status){ // Envoie de la requête pour calculer le parcours if(status == google.maps.DirectionsStatus.OK){ direction.setDirections(response); // Trace l'itinéraire sur la carte et les différentes étapes du parcours // Fermeture de la dernière infobulle ouverte if(prev_infobulle){ prev_infobulle.close(); } var _route = response.routes[0].legs[0]; pinA = new google.maps.Marker({ position: _route.start_location, map: myMap, icon: { url: "data:image/svg+xml;charset=UTF-8," + encodeURIComponent('
			<svg version="1.1" xmlns="http://www.w3.org/2000/svg" width="30px" height="40px" x="0px" y="0px" viewbox="0 0 239.6 315.1" style="enable-background:new 0 0 239.6 315.1;" xml:space="preserve">
				<style type="text/css">.marker-pi-icone {fill:#fff;font-size:130px;font-family:"Raleway", sans-serif;} .marker-multiple-pi-fond {fill:#035725;}</style>
				<g>
					<path class="marker-multiple-pi-fond" d="M235.8,119.1c0,68.6-67.4,145.8-99.4,185c-8.6,10.5-24.5,10.5-33.1,0c-32-39.2-99.4-116.3-99.4-185 c0-64.1,51.9-116,116-116S235.8,55.1,235.8,119.1z"></path>
					<text transform="matrix(1 0 0 1 80.0267 175.9853)" class="marker-pi-icone">A</text>
				</g>
			</svg>')}, }), pinB = new google.maps.Marker({ position: _route.end_location, map: myMap, icon: { url: "data:image/svg+xml;charset=UTF-8," + encodeURIComponent('
			<svg version="1.1" xmlns="http://www.w3.org/2000/svg" width="30px" height="40px" x="0px" y="0px" viewbox="0 0 239.6 315.1" style="enable-background:new 0 0 239.6 315.1;" xml:space="preserve">
				<style type="text/css">.marker-pi-icone {fill:#fff;font-size:130px;font-family:"Raleway", sans-serif;} .marker-multiple-pi-fond {fill:#035725;}</style>
				<g>
					<path class="marker-multiple-pi-fond" d="M235.8,119.1c0,68.6-67.4,145.8-99.4,185c-8.6,10.5-24.5,10.5-33.1,0c-32-39.2-99.4-116.3-99.4-185 c0-64.1,51.9-116,116-116S235.8,55.1,235.8,119.1z"></path>
					<text transform="matrix(1 0 0 1 80.0267 175.9853)" class="marker-pi-icone">B</text>
				</g>
			</svg>')}, }); } }); } };</div>
