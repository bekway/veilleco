		<script src="/typo3conf/ext/e_magineurs/res/sartrouville/ressources/js/slick.min.js?1593498916" type="text/javascript"></script>
		<script src="/typo3conf/ext/e_magineurs/res/sartrouville/ressources/js/jquery.fancybox.pack.js?1593498916" type="text/javascript"></script>
		<script src="/typo3conf/ext/e_magineurs/res/sartrouville/ressources/js/jquery.mousewheel-3.0.6.pack.js?1593498916" type="text/javascript"></script>
		<script src="/typo3conf/ext/e_magineurs/res/sartrouville/ressources/js/script.js?1593498916" type="text/javascript"></script>
		<script src="/typo3conf/ext/e_magineurs/res/sartrouville/ressources/js/socialshareprivacy.min.js?1593498916" type="text/javascript"></script>
		<script src="/typo3conf/ext/e_magineurs/res/sartrouville/ressources/js/modules/fbshare.js?1593498916" type="text/javascript"></script>
