		<script>function resize () {
            var largeur = (document.body.clientWidth);
            var myDiv = document.getElementsByClassName('tp-parallax-wrap'); 
            var myButton = document.getElementById('commander');
            
            if (largeur > 767 && largeur < 959){ 
                //alert ('1');
                for (var i = 0; i < myDiv.length; i++) {

                    myDiv[i].style.marginLeft = '-50px';
                    
                }
                myButton.style.marginLeft = '140px';
                myButton.style.marginTop = '40px';
            }
            else if (largeur > 479 && largeur < 767 ){ 
                 //alert ('2');
                for (var i = 0; i < myDiv.length; i++) {

                    myDiv[i].style.marginLeft = '100px';
                    
                }
                myButton.style.marginLeft = '-140px';
                myButton.style.marginTop = '60px';
            }
            else if (largeur < 479){ 
                 //alert ('3');
                for (var i = 0; i < myDiv.length; i++) {

                    myDiv[i].style.marginLeft = '-50px';
                    
                }
                myButton.style.marginLeft = '140px';
                myButton.style.marginTop = '60px';
            }
        }
            window.onload = resize;
            window.onresize = resize;
		</script>
