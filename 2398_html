		 /* ONLY page accueil : cibler if mobile pour ajouter class iframe video  */
		 
/* test a lancer pour ensuite cibler supports :
alert(navigator.userAgent.toLowerCase());
*/

var isMobile = /mobile/i.test(navigator.userAgent.toLowerCase());

if (isMobile)
{
  /*alert("Mobile true");*/  	
jQuery( "iframe#acso-video" ).addClass( "onlyMobile" );
}
else{
 /* alert("Mobile false");*/
  jQuery( "iframe#acso-video" ).addClass( "noMobile" );
}

