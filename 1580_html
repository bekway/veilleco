		<script src="fileadmin/v2/js/menu.js" type="text/javascript"></script>
		<script type="text/javascript">	//menu accessible
jQuery(document).ready( function() {

	function masque_tout() {
		jQuery('#mblmenu > ul > li > ul').addClass('masque');
		jQuery('#mblmenu > ul > li > ul > li > ul').addClass('masque');
		jQuery('#mblmenu > ul > li').removeClass('opened');	
		jQuery('#mblmenu > ul > li > ul > li').removeClass('opened');
	}

	function masque_niveau_2() {
		jQuery('#mblmenu > ul > li > ul > li > ul').addClass('masque');
		jQuery('#mblmenu > ul > li > ul > li').removeClass('opened');
	}
	
	masque_tout();

	jQuery('a.firstli').click( function(ev) {
		jQuery(this).toggleClass("opened");
		ul = jQuery(this).next();
		if(ul.hasClass('masque')) {
			ul.removeClass('masque');
		}
		else
		{
			ul.addClass('masque');
		}
		return false;
	});
	
	// mblmenu Click sur +/- niveau 1	
	jQuery('#mblmenu > ul > li > a > em').click( function(ev) {
	
		var ul = jQuery(this).parent().next();
		if(ul.size() != 0) {
			ev.preventDefault();
			var li = jQuery(this).parent().parent();
			// Si affiché, 
			if(!ul.hasClass('masque')) {
				//ul.addClass('masque');
				li.removeClass('opened');
				masque_tout();
			}
			// Si masqué, afficher
			else 
			{
				masque_tout();
				ul.removeClass('masque');
				li.addClass('opened');
			}
		}
	});

	// mblmenu Click sur +/- niveau 2
	jQuery('#mblmenu > ul > li >  ul > li > a > em').click( function(ev) {
	
		var ul = jQuery(this).parent().next();
		if(ul.size() != 0) {
			ev.preventDefault();
			var li = jQuery(this).parent().parent();
			// Si affiché, masquer
			if(!ul.hasClass('masque')) {
				ul.addClass('masque')
				li.removeClass('opened');
				//masque_niveau_2();
			}
			// si masqué, masquer les niveaux 2 et afficher
			else 
			{
				masque_niveau_2();
				ul.removeClass('masque');
				li.addClass('opened');
			}
		}

	});
	
	//aide demarches
	jQuery('.switch-button').click(function(ev) {
		ev.preventDefault();
		jQuery(this).parent().toggleClass('open');
	});
	
	/*
	jQuery('#menu > ul > li > ul > li > a').click( function(ev) {
		jQuery(this).parent().parent().find('ul').addClass('masque');
		if(jQuery(this).find('+ul').size() != 0) {
			ev.preventDefault();
			jQuery(this).next().removeClass('masque');
		}
	});
	*/
		
	jQuery('#mb-acces-compte > .myspace').addClass('masque')
		.parent().bind('focusin', function()
		{
			jQuery(this).find('.btUser').removeClass('masque');
		}).bind('focusout', function()
		{
			jQuery(this).find('.btUser').addClass('masque');
		});
});
		</script>
		<script type="text/javascript">
function afficher_cacher(id)
{
	if (jQuery('#'+id).is(':visible')) {
		jQuery('#'+id).hide();
		jQuery('#bouton_'+id).html('<img src="fileadmin/v2/images/mobile/picto-mon-compte.png" alt="" />');
	} else {
		jQuery('#'+id).show();
		jQuery('#bouton_'+id).html('<img src="fileadmin/v2/images/mobile/picto-mon-compte-on.png" alt="" />');
	}
	return true;
}
		</script>
