			 function animfade($id){
			
	  			$('#'+animatetab[$id]).addClass('selected');
				if((parseInt($id)+1)<=$i) return (parseInt($id)+1);
				else return '0';
			  }
			var $item=animfade(0);
			var clearInt = setInterval(function(){$oe_menu_items.removeClass('selected');$item=animfade($item);},2000);
			
			
                $oe_menu_items.bind('mouseenter',function(){
					var $this = $(this);
					$this.addClass('slided selected');
					$this.children('div').css('z-index','9999').stop(true,true).slideDown(200,function(){
						$oe_menu_items.not('.slided').children('div').hide();
						$this.removeClass('slided');
					});
				}).bind('mouseleave',function(){
					var $this = $(this);
					$this.removeClass('selected').children('div').css('z-index','1').hide();
				});

				$oe_menu.bind('mouseenter',function(){
					var $this = $(this);
					$this.addClass('hovered');
				}).bind('mouseleave',function(){
					var $this = $(this);
					$this.removeClass('hovered');
					$oe_menu_items.children('div').hide();
				});
				$('.oe_wrapper .nav-child ').each(function(){
					var divparent=$(this).parent();
