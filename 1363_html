}
} else {
lsjQuery("#layerslider_1").layerSlider({sliderVersion: '6.6.8', type: 'fullwidth', responsiveUnder: 1140, hideUnder: 0, hideOver: 100000, pauseOnHover: 1, twoWaySlideshow: true, skin: 'fullwidth', navStartStop: false, navButtons: false, hoverBottomNav: true, showCircleTimer: false, thumbnailNavigation: 'disabled', tnWidth: 80, skinsPath: 'https://www.cc-paysdebitche.fr/wp-content/themes/paysdebitche/config-layerslider/LayerSlider/static/layerslider/skins/'});
}
});
