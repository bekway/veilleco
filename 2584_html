									<script type="text/javascript">
	$("#menu_a_78").click(function(){
		//$("#bouton_recherche_bandeau").hide();
		if($("#recherche").hasClass("active")) {
			$("#recherche").hide();
			$("#recherche").removeClass("active");
			$("#bouton_recherche_bandeau").removeClass("active");
		} else {
			$("#recherche").stop().animate({height:'show'},'normal');
			$("#recherche").addClass("active");
			$("#bouton_recherche_bandeau").addClass("active");
		}
	});
