		$(document).ready(function() {

			$("#rendu-album-page_138_photos_gauche .item > a").click(function(e) {
				e.preventDefault();e.stopPropagation();
				// Initializes and opens PhotoSwipe
				gallery_page_138_photos_gauche = new PhotoSwipe( pswpElement, PhotoSwipeUI_Default, items_page_138_photos_gauche, {
					index: parseInt($(this).attr("data-index"))-1,
					pinchToClose: false,
					closeOnScroll: false,
					closeOnVerticalDrag: false,
					escKey: true
					});
				gallery_page_138_photos_gauche.init();
				return false;
			});

		});
		
