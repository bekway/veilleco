function loadmedia(){

$('._media').media({mp3Player: 'https://www.mairie-saintvallier.fr/openinfoliveRessources/outils/player/mediaplayer.swf',params: { wmode: 'transparent', allowfullscreen: 'true'},attrs:     { allowfullscreen:  'true' }} );
	return false;
}
function loadmedia1(id,autoplay){

$('._'+id+' ._media').media({
	mp3Player: 'https://www.mairie-saintvallier.fr/openinfoliveRessources/outils/player/player1.swf', 
	flvKeyName: 'file', 
	flvPlayer: 'https://www.mairie-saintvallier.fr/openinfoliveRessources/outils/player/player1.swf',
	attrs:     { allowfullscreen:  'true' },	
	autoplay:  autoplay,
	params: { wmode: 'transparent', allowfullscreen: 'true'},
	flashvars:{autostart: autoplay } 		
	});
	
}




function loadModifierArticle(url,redirect){
	if ($("#loadModifierArticle").length==0){
		$('body').append('<div style="display:none;" id="loadModifierArticle">');
		
	}else{
		$("#loadModifierArticle").empty();		
	}		
		url=url+'&redirecturl=';
	
	
	url=url+encodeURIComponent(redirect);
	$("#loadModifierArticle").append('<iframe width="100%" height="98%" id="contentloadModifierArticle" src='+url+'>');
	$("#loadModifierArticle").dialog('destroy');
	$("#loadModifierArticle").dialog({
		title:'Modification rapide',
		bgiframe: true,
		height: 700,
		width: 1050,
		position: 'center',
		zIndex:9999,
		close: function( event, ui ) {window.location.reload();}		
	});	
	
	
	
}

function fermerModeAdministrateur(){
	
	
	$.ajax({
		type: "POST",
		url: urlSite+"index.php/moncompte/fermeModeAdministrateur",		
		success: function(msg){	
					$('#adminfront, .modiffrontarticle').hide('fold',{}, 500);		
			}
	});

}


function supprimer_ajouter(url_video,titre){
			
			if($(".encart_media object").size()>0){
				$('.encart_media object').replaceWith('<a class="media" href="'+url_video+'">');
			}else{
				$('.encart_media embed').replaceWith('<a class="media" href="'+url_video+'">');
			}
			
			$(".media_vignette_fiche .titre span").html(titre);
			
			$('.media').media({
			attrs:     { allowfullscreen:  'true', id:'id_video' },
			autoplay:  true,
			params: { wmode: 'transparent', allowfullscreen: 'true'},
			flashvars:{autostart:'1'} 
			});		
		
		}
		$('.generepdf').live("click", function(){				
		var c=$("#contenu_dyn .container").find("script,noscript,style").remove().end().html();
		var t='LES-COMPTES-RENDUS-DU-CONSEIL-MUNICIPAL';
			
			$.post(urlSite+"index.php/pagetopdf/prepare",
				{ contenu: c, titre: escape("Les séances du Conseil Municipal sont publiques et se tiennent à la salle du Conseil Municipal. Chaque séance fait l'objet d'un compte rendu publié sur le site de la ville et affiché en mairie. Renseignements :Secrétariat de Direction 03 85 67 95 52 ou 03 85 67 91 55 Consulter les comptes-rendus des Conseils Municipaux :Séance du 3 juin 2020 et en annexes du compte-rendu, les synthèses :- de présentation du budget primitif 2020 de la ville et celle concernant le compte administratif 2019 de la ville- de présentation du budget primitif 2020  du CCAS et celle concernant le compte administratif 2019 du CCASSéance du 3 juillet 2020Séance du 27 février 2020Séance du 12 décembre 2019Séance du 3 octobre 2019Séance du 4 juillet 2019Séance du 11 avril 2019Séance du 13 février 2019Séance du 13 décembre 2018Séance du 2 octobre 2018Séance du 2 juillet 2018Séance du 29 mai 2018Séance du 25 avril 2018Séance du 3 avril 2018Séance du 6 février 2018Séance du 14 décembre 2017Séance du 2 octobre 2017Séance du 6 juillet 2017Séance du 12 avril 2017Séance du 16 février 2017Séance du 22 décembre 2016Séance du 20 octobre 2016Séance du 27 septembre 2016Séance du 28 juin 2016 Tous les comptes-rendus antérieurs sont consultables en mairie sur simple demande.") , css: 'css/26333_3.css', keyword : ''},
					function(data){					
						window.open('https://www.mairie-saintvallier.fr//index.php/pageToPDF/display/'+t+'.pdf');
						return false;
				}
			);
		});


$(document).ready(function() {	
	setTimeout("loadmedia()", 1000);
	
	
	 
			
		$('a').live("click", function(){
			var downloadpos=$(this).attr('href').indexOf('/download/');
			
			if (downloadpos>0){ 
				doc=$(this).attr('href').replace("https://www.mairie-saintvallier.fr/", "");					
				_gaq.push(['_trackPageview', doc]);
			}
		});
	
});
