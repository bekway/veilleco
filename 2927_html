													<script>
			var index_calendar_E574B1E1D81045B3983E5C7815DE55D2 = 0;
			var index_calendar_max_E574B1E1D81045B3983E5C7815DE55D2 = 2;
			var isHover_defile_E574B1E1D81045B3983E5C7815DE55D2 = false;
			
			
			$( document ).ready(function() {
					
				$(".calendar-liste-suiv").click(function() {
					$("#li-une-date-agenda-E574B1E1D81045B3983E5C7815DE55D2-"+index_calendar_E574B1E1D81045B3983E5C7815DE55D2).slideToggle( {duration:1000});
					index_calendar_E574B1E1D81045B3983E5C7815DE55D2++;
				  		
				  if(index_calendar_E574B1E1D81045B3983E5C7815DE55D2 > index_calendar_max_E574B1E1D81045B3983E5C7815DE55D2)
						index_calendar_E574B1E1D81045B3983E5C7815DE55D2 = 1;					
							
				  $("#li-une-date-agenda-E574B1E1D81045B3983E5C7815DE55D2-"+index_calendar_E574B1E1D81045B3983E5C7815DE55D2).slideToggle( {duration:1000});
				});	
					
				$(".calendar-liste-prev").click(function() {
				  $("#li-une-date-agenda-E574B1E1D81045B3983E5C7815DE55D2-"+index_calendar_E574B1E1D81045B3983E5C7815DE55D2).slideToggle( {duration:1000});
					index_calendar_E574B1E1D81045B3983E5C7815DE55D2--;
				  	
				  if(index_calendar_E574B1E1D81045B3983E5C7815DE55D2 < 1 )
						index_calendar_E574B1E1D81045B3983E5C7815DE55D2 = index_calendar_max_E574B1E1D81045B3983E5C7815DE55D2;
					$("#li-une-date-agenda-E574B1E1D81045B3983E5C7815DE55D2-"+index_calendar_E574B1E1D81045B3983E5C7815DE55D2).slideToggle( {duration:1000});
				});
					
				$("#defile-E574B1E1D81045B3983E5C7815DE55D2").hover(
				  function() {
				    isHover_defile_E574B1E1D81045B3983E5C7815DE55D2 = true;
				  }, function() {
				    isHover_defile_E574B1E1D81045B3983E5C7815DE55D2 = false;
				  }
				);	show_calendar_E574B1E1D81045B3983E5C7815DE55D2();
    			setInterval("show_calendar_E574B1E1D81045B3983E5C7815DE55D2()", 5000);
   				
			});
			function show_calendar_E574B1E1D81045B3983E5C7815DE55D2(){
					if(isHover_defile_E574B1E1D81045B3983E5C7815DE55D2)
						return;
					
					$("#li-une-date-agenda-E574B1E1D81045B3983E5C7815DE55D2-"+index_calendar_E574B1E1D81045B3983E5C7815DE55D2).slideToggle( {duration:1000});
					
					index_calendar_E574B1E1D81045B3983E5C7815DE55D2++;
										
					if(index_calendar_E574B1E1D81045B3983E5C7815DE55D2 > index_calendar_max_E574B1E1D81045B3983E5C7815DE55D2)
						index_calendar_E574B1E1D81045B3983E5C7815DE55D2 = 1;
										
					$("#li-une-date-agenda-E574B1E1D81045B3983E5C7815DE55D2-"+index_calendar_E574B1E1D81045B3983E5C7815DE55D2).slideToggle( {duration:1000});
					
				}
