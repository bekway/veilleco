		<script src="https://www.ville-dunkerque.fr/typo3conf/ext/e_magineurs/res/dunkerque/ressources/js/jquery.fitvids.js?1548861258" type="text/javascript"></script>
		<script>
jQuery(document).ready(function() {
	var solutions = $('.powermail_fieldwrap_type_text, .powermail_fieldwrap_type_html');
	var solutionsBrut = [];
	// On récupére les solutions avant pour qu'elles n'aient aucun traitement
	$.each( solutions, function( key, value ) {
		//Recuperation d'une solution
		solutionsBrut[key] = value.innerHTML;
	});
	var legend = $('.powermail_fieldset legend');
	legend = legend[0].innerHTML;

	if(legend == "FormulaireSimulation"){
		$('.powermail_fieldset legend').addClass("hidden");
		$('.powermail_input').on("change paste keyup", function(event) {

			var elementsForm = $( ".powermail_fieldwrap input, .powermail_fieldwrap select, .powermail_fieldwrap textarea" );
			var listID = [];
			var solutionsNew = [];
			var listClassSolutions = [];
			var solution = "";

			//Liste les id des champs du formulaire
			$.each( elementsForm, function( key, value ) {
				var elementChildren = $(value).attr('id');
				if (elementChildren !== undefined) {
					listID[key] = elementChildren;
				}
			});

			//Liste les classes des solutions
			$.each( solutions, function( key, value ) {
				var tempClass = $(value).attr('class');
				var tempClass = tempClass.split(" ");
				var classSolution = "";
				$.each( tempClass, function( key, value ) {
					if (value !== ""){
						classSolution += "."+ value;
					}
				});
				listClassSolutions[key] = classSolution;
			});

			solutionsNew = [];
			//Liste les solutions en remplaçant les variables par leur valeur
			$.each( solutionsBrut, function( key, value ) {
				//Recuperation d'une solution
				solution = value;

				//On sépare la solution au [ pour ne remplacer que les valeurs à l'intérieur
				var triSolution = solution.split("[");
				var solutionTraitee = "";

				//On parcourt chaque bout de la solution pour remplacer les valeurs si nécessaire
				$.each( triSolution, function( key2, value ) {
					if ((value.split("]")).length == 1){
						//Contenu extérieur au [] donc aucun traitement dessus
						solutionTraitee += value;
					} else {
						//Contenu interne au [], les variables vont donc être modifiées par leur valeur
						var temp = value.split("]");
						solutionRemplacement = temp[0];

						//Parcourt la liste des id pour que chaque variable (champ du formulaire) soit modifiée
						$.each( listID, function( keyID, valueID ) {
							var id = valueID.slice(16);
							var re = new RegExp(id,"g");
							var valField = $('#'+valueID).val();
							if (valField == ""){
								valField = 0;
							}
							solutionRemplacement = solutionRemplacement.replace(re, valField);

						});
						solutionRemplacement = eval(solutionRemplacement).toFixed(2);
						solutionTraitee += solutionRemplacement;
						solutionTraitee += temp[1];
					}
					if (!($(listClassSolutions[key]).hasClass("solution"))){
							$(listClassSolutions[key]).addClass("solution");
					}
				});
				solutionsNew[key] = solutionTraitee;

			});

			//Remplace les solutions par les solutions modifiées
			$.each( listClassSolutions, function( key, value ) {
				$(value).empty();
				$(value).append(solutionsNew[key]);
			});

    	});
	}

});
		</script>
	</body>
