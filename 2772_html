		<script type="text/javascript">
	$(document).ready(function(){
		$('#c-admin h2').on('click',function(){
			changeClass('','adminDiv');
			var adminMenu = 0;
			if($('#adminDiv').is(':visible')){
				adminMenu = 1;
			}
			
			$.ajax({
				type: "post",
				url: "/admin/ajaxAction.php",
				data: "adminMenu="+adminMenu,
				async: true
			});
		});
		
		changeClass('','adminDiv');	});
		</script>
