				<script type="text/javascript">
    ///<![CDATA[

    var triggerSubmit = function() {
        $('#tx_cimsearch_form input[type=submit]').trigger('click');
    }
    
    var urlLoading = "https://www.agen.fr/nous-connaitre/le-conseil-municipal/documents-du-conseil-municipal-573.html?tx_cimsearchelastic_displaysearch%5Baction%5D=scroll&tx_cimsearchelastic_displaysearch%5Bcontroller%5D=Listing&tx_cimsearchelastic_displaysearch%5Bformat%5D=html";
    var urlLoadingWithoutParams = "https://www.agen.fr/nous-connaitre/le-conseil-municipal/documents-du-conseil-municipal-573.html?type=99992&tx_cimsearchelastic_displaysearch%5Baction%5D=scroll&tx_cimsearchelastic_displaysearch%5Bcontroller%5D=Listing&tx_cimsearchelastic_displaysearch%5Bformat%5D=html";
    var page = 1;
    var shouldLoad = true;
    var sort = '[{"metadata.creation_date":{"order":"desc"}}]';
    var totalRecord = 68;
    var inputs = $("#tx_cimsearch_form").find('input[type="checkbox"],select');
    var isLoading = false;
    
    
                var elementId = 932;
            

    $(document).bind('_page_ready_geo', function() {

        $(inputs).on('change', triggerSubmit);
        $('input[type="text"]').on('keyup', triggerSubmit);

        
        $('#reset_search_popin').on('click',function(e){
            e.preventDefault();
            document.getElementById('tx_cimsearch_form').reset();
            $('#submitForm').trigger('click');
            return false;
        })

        var hasSelectedItems = function() {
            var selectedItems = 0;
            var fieldsets = $('#tx_cimsearch_form').find('fieldset');
            selectedItems = fieldsets.find('select option[value!=""]:selected').length + fieldsets.find('input[type="checkbox"]:checked').length;
            fieldsets.find('input[type="text"]').each(function(){
                if($(this).val() !== ""){
                    selectedItems++;
                }
            });

            return selectedItems > 0;
        }

        var showResetButton = function() {
            $('#reset_search_popin').removeClass('hide');
        }

        var resetForm = function() {
            $('#tx_cimsearch_form')[0].reset();
            $('#reset_search_popin').addClass('hide');
            $('#tx_cimsearch_form input[type=submit]').trigger('click');
        }

        $('#reset_search_popin a').on('click', function(e) {
            resetForm();
            e.preventDefault();
        });

        $('#tx_cimsearch_form input[type=submit]').on('click',function(e){
            e.preventDefault();
            if(hasSelectedItems()) {
                showResetButton();
            }
            $('#zone_results article').remove();
            $('#searchLoader').addClass('loader');
            $('#searchLoader').removeClass('is-hidden');
            page = 0;
            if(!isLoading) {
                isLoading = true;

                if($('#map_search').is(':visible')){
                    listMap.deleteOverlaysGeo();
                    loadPoints();
                }

                $.ajax({
                    url: urlLoadingWithoutParams,
                    type: 'POST',
                    data: $('#tx_cimsearch_form').serialize() + "&" + $.param({
                        "tx_cimsearchelastic_displaysearch[page]": (page),
                        'elementId':elementId,
                        'mapIsDisplay':$('#map_search').is(':visible'),
                        'contentElements':"50",
                        sorting:sort
                    }),
                })
                    .done(function(data) {
                        isLoading = false;
                        var obj = data;

                        if(obj.content != ''){

                            var jObj = $(obj.content);

                            if($('#filters').is(':visible')) {
                                jObj.removeClass('large-3');
                                jObj.addClass('large-4');
                            }

                            $('#searchLoader').before(jObj);
                            timerFunctionListSearch(page)
                            page = page+1;
                            shouldLoad = true;
                            //$('#second_display .sticky').foundation('_calc', true);
                            bindPopin();
                        }else{
                            shouldLoad = false;
                        }

                        $('#searchLoader').removeClass('loader');
                        // update counter
                        if(obj.nb_results > 1){
                            $('.nb_result').html('<span>'+obj.nb_results+'resultats')
                        } else {
                            $('.nb_result').html('<span>'+obj.nb_results+'resultat')
                        }

                        $('#searchLoader').addClass('is-hidden');
                        $('#filters').trigger('off.zf.toggler')
                    })
            }
        })

        $(document).on('_scroll',function(e){
            var bottom = $('#list_items').position().top+$('#list_items').outerHeight(true);
            if(($(window).scrollTop() + $(window).height() >= (bottom+50))){
                listLoadNextContent();
            }
        });
    });

    function listLoadNextContent() {
        if(shouldLoad && !isLoading){
            isLoading = true;
            $('#searchLoader').addClass('loader');
            $('#searchLoader').removeClass('is-hidden');
            $.ajax({
                url: urlLoadingWithoutParams+"&"+$('#tx_cimsearch_form').serialize(),
                type: 'POST',
                data: {"tx_cimsearchelastic_displaysearch[page]": (page),'elementId':elementId,'mapIsDisplay':$('#map_search').is(':visible'),'contentElements':"50",sorting:sort},
            })
            .done(function(data) {
                isLoading = false;
                var obj = data;
                if(obj.content != ''){
                    var jObj = $(obj.content);

                    if($('#filters').is(':visible')) {
                        jObj.removeClass('large-3');
                        jObj.addClass('large-4');
                    }

                    $('#searchLoader').before(jObj);

                    timerFunctionListSearch(page)
                    page = page+1;
                    shouldLoad = true;
                    //$('#second_display .sticky').foundation('_calc', true);
                    bindPopin();
                }else{
                    shouldLoad = false;
                }

                $('#searchLoader').removeClass('loader');
                $('#searchLoader').addClass('is-hidden');
            })
        }
    }
    //]]>
