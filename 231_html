    $(document).bind('_page_ready_geo', function() {
        listMap = new Cimeos('map_search')
        contentElements = 50;
        query = "{"query":{"bool":{"must":[{"bool":{"must":[{"term":{"_type":"fichier"}},{"match":{"folder":"fileadmin\/user_upload_ville\/deliberations_CM\/"}}]}},{"bool":{"should":[{"term":{"site":"www.agen.fr"}},{"bool":{"must_not":[{"exists":{"field":"site"}}]}}]}},{"bool":{"should":[{"bool":{"must":[{"exists":{"field":"endtime"}},{"range":{"endtime":{"gte":"now"}}},{"bool":{"should":[{"range":{"starttime":{"lte":"now"}}},{"bool":{"must_not":[{"exists":{"field":"starttime"}}]}}]}}]}},{"bool":{"must":[{"exists":{"field":"starttime"}},{"range":{"starttime":{"lte":"now"}}},{"bool":{"should":[{"range":{"endtime":{"gte":"now"}}},{"bool":{"must_not":[{"exists":{"field":"endtime"}}]}}]}}]}},{"bool":{"must_not":[{"exists":{"field":"starttime"}},{"exists":{"field":"endtime"}}]}}]}}]}},"sort":[{"metadata.creation_date":{"order":"desc"}}],"size":"20"}";
        loadContent = 1;
        urlAjax = 'https://www.agen.fr/nous-connaitre/le-conseil-municipal/documents-du-conseil-municipal-573.html?type=99991&tx_cimsearchelastic_displaysearch%5Baction%5D=geo&tx_cimsearchelastic_displaysearch%5Bcontroller%5D=Listing';

        var mapDisplay = setInterval(function(){
          if($('#map_search').is(':visible')){
            displayMap();
            clearInterval(mapDisplay);
          }
        },500)

        function displayMap() {
          listMap.displayMap();
          if(loadContent) {
              loadPoints();
          }
        }
        
        loadPoints = function() {
            $.ajax({
                url: urlAjax,
                data: jQuery.extend({'contentElements': contentElements,'elementId':elementId},QueryStringToHash($('#tx_cimsearch_form').serialize()))
            }).done(function(response){
                if(response.documents.length){
                  var type = response.documents[0]["_type"];
                  if(type == "annuaire_commune" || type == "conseil_quartiers"){
                      var kml = listMap.addKml("https://www.agen.fr/"+"/fileadmin/user_upload/kml/"+type+"/all.xml?dummy="+(new Date()).getTime());
                  }
                  $.each(response.documents,function(index,elasticDocument) {
                      var latitude;
                      var longitude;
                      if (elasticDocument.mapLatitude && elasticDocument.mapLongitude) {
                          latitude = elasticDocument.mapLatitude;
                          longitude = elasticDocument.mapLongitude;
                      } else {
                          if (elasticDocument.cimNewsPlace) {
                              if (elasticDocument.cimNewsPlace.mapLatitude
                                  && elasticDocument.cimNewsPlace.mapLongitude) {
                                  latitude = elasticDocument.cimNewsPlace.mapLatitude;
                                  longitude = elasticDocument.cimNewsPlace.mapLongitude;
                              }
                          } else if (elasticDocument.cimNewsOrganizer) {
                              if (elasticDocument.cimNewsOrganizer.mapLatitude
                                  && elasticDocument.cimNewsOrganizer.mapLongitude) {
                                  latitude = elasticDocument.cimNewsOrganizer.mapLatitude;
                                  longitude = elasticDocument.cimNewsOrganizer.mapLongitude;
                              }
                          }
                      }
                      if(latitude && longitude){
						marker = listMap.addMarker({'latitude':latitude ,'longitude':longitude});
                      	marker.setTitle(elasticDocument.title);
                      	marker.addListener('click', function(){
                        getPopinFromMapMarker(50,elasticDocument.uid,elasticDocument.title);
                      })
                      }
                  })
                  listMap.fitBounds();
                }
            })
        }

        function QueryStringToHash  (query) {
          var query_string = {};
          var vars = query.split("&");
          for (var i=0;i<vars.length;i++) {
            var pair = vars[i].split("=");
            pair[0] = decodeURIComponent(pair[0]);
            pair[1] = decodeURIComponent(pair[1]);
                // If first entry with this name
            if (typeof query_string[pair[0]] === "undefined") {
              query_string[pair[0]] = pair[1];
                // If second entry with this name
            } else if (typeof query_string[pair[0]] === "string") {
              var arr = [ query_string[pair[0]], pair[1] ];
              query_string[pair[0]] = arr;
                // If third or later entry with this name
            } else {
              query_string[pair[0]].push(pair[1]);
            }
          } 
          return query_string;
        };
      });
    
