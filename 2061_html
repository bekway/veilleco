        jQuery(document).ready(function() {
            jQuery('#share_article_popup').popup({transition: 'all 1.5s'});
        });
        jQuery('.share-article').click(function(){
            jQuery('#share_article_popup input[name=node_id]').val(jQuery(this).attr('node_id'));
            jQuery('#share_article_popup').popup('show');
        });
        jQuery('.fb-icon').off('click').click(function(){
            var href = jQuery(this).attr('data-href');
            window.open('http://www.facebook.com/sharer.php?u=' + href, '_blank', 'toolbar=0,location=0,menubar=0, width=400, height=400');
        });
        jQuery('.twitter-icon').off('click').click(function(){
            var href = jQuery(this).attr('data-href');
            window.open('http://twitter.com/share?&url=' + href, '_blank', 'toolbar=0,location=0,menubar=0, width=400, height=400');
        });
    
