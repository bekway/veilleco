																						<script>
        var ezxform_token = '@$ezxFormToken@';
    
																						</script>
																						<div id="share_article_popup" class="popup-window">
																							<p class="title">Envoyer ce lien par e-mail à un ami</p>
																							<div id="alert" class="alert" style="display: none"></div>
																							<form>
																								<p><label>Prénom du destinaire</label> <input name="receiver_name" type="text" value=""></p>
																								<p><label>Adresse email du destinataire</label> <input name="receiver_email" type="text" value=""></p>
																								<p><label>Prénom de l'expéditeur</label> <input name="sender_name" type="text" value=""></p>
																								<p><label>Sujet</label> <input type="text" name="subject" value=""></p>
																							</form><input type="hidden" name="ezxform_token" value=""> <input type="hidden" name="node_id" value=""> <button id="share_article_popup_submit">Expédier</button> <button class="share_article_popup_close">Annuler</button></div>
																						<script type="text/javascript">
    function parseErrorCode( code ) {
        switch(code) {
            case 1:
                jQuery('#share_article_popup input[name=receiver_name]').css('border-color','red').css('background-color','#FFEBEB');
                break;
            case 2, 5:
                jQuery('#share_article_popup input[name=receiver_email]').css('border-color','red').css('background-color','#FFEBEB');
                break;
            case 3:
                jQuery('#share_article_popup input[name=sender_name]').css('border-color','red').css('background-color','#FFEBEB');
                break;
            case 4:
                jQuery('#share_article_popup input[name=subject]').css('border-color','red').css('background-color','#FFEBEB');
                break;
        }
    }
    function resetFields() {
        jQuery('#share_article_popup #alert').empty().removeAttr('class').hide();
        jQuery('#share_article_popup input').each(function(){
                  jQuery(this).attr('style','');
        });
    };
    jQuery('#share_article_popup_submit').click(function(event){
        jQuery.ajax({
            type: "POST",
            url: "/layout/set/print/layout/set/print/layout/set/print/layout/set/print/sendmail/share",
            data: {
                ezxform_token:  '@$ezxFormToken@',
                receiver_name:  jQuery('#share_article_popup input[name=receiver_name]').val(),
                receiver_email: jQuery('#share_article_popup input[name=receiver_email]').val(),
                sender_name:    jQuery('#share_article_popup input[name=sender_name]').val(),
                subject:        jQuery('#share_article_popup input[name=subject]').val(),
                node_id:        jQuery('#share_article_popup input[name=node_id]').val()
            },
            cache: "false",
            dataType: "json",
            success: function (response) {
                resetFields();
                if( response.results.length == 0 ) {
                    jQuery('#share_article_popup #alert').html('Votre message a été envoyé!').addClass('alert')
                            .addClass('alert-success').show();
                    jQuery('#share_article_popup').delay(2500).queue(function(){
                        jQuery('#share_article_popup').popup('hide');
                    });
                    jQuery('#share_article_popup input').each(function(){
                        jQuery(this).prop('disabled', true);
                    });
                } else {
                    jQuery('#share_article_popup #alert').html('Merci de vérifier les champs suivants').addClass('alert')
                            .addClass('alert-error').show();
                    jQuery.each(response.results, function(i,val){
                        parseErrorCode(val);
                    });
                }
            },
            error: function() {
                resetFields();
                jQuery('#share_article_popup #alert').html('Sorry! Can\'t parse server response. Please try again later.').addClass('alert')
                        .addClass('alert-error').show();
            }
        });
    });
																						</script>
