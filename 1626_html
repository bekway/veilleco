var wall;
init_freewall();

function init_freewall()
{
    wall = new Freewall("#freewall");
    wall.reset({
    	selector: '.post_block',
    	animate: true,
    	cellW: 330,
    	cellH: 'auto',
        gutterY: 30,
        gutterX: 30,
    	onResize: function() {
    		wall.fitWidth();
    	},
        onComplete: function() {
            $('#freewall').css('visibility', 'visible'); 
        }
    });
    
    wall.fitWidth();
        
    wall.container.find('.post_block img').on("load", function (e) {
    	wall.fitWidth();
    });  
}
