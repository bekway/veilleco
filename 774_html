	function bve_doc_lanceur(key,mode)
	{
		with(document.frm_ged)
		{
			modeappel.value=mode;
			pkkey.value=key;
			pk_menu_fils.value=js_pkmenufils;
		}
		if (mode=="envoyer_avis")
		{
			with(document.frm_ged)
			{
				var message_alert="Vous devez saisir :\n";
				if (anom.value=="") message_alert+="- votre nom\n";
				if (amail.value=="") message_alert+="- votre email\n";
				if (adescription.value=="") message_alert+="- votre commentaire\n";
				if (message_alert!="Vous devez saisir :\n")
				{
					alert(message_alert);
					return;
				}
			}
		}
		else if(mode == 'generer_flippingbook')
		{
			ajax(document.frm_ged, 'ged.php', 'flippingbook', document.getElementById('flippingbook'), function()
			{
				Modalbox.show($('flippingbook'), {title: '', width: 960, height: 720,afterLoad: create_flippingbook, afterHide: destroy_flippingbook});
			});

			return;
		}
		else if(mode == 'generer_liste_document_vue_categorie_article')
		{
			ajax(document.frm_ged, 'ged.php', 'vue_categories_articles_table_documents', document.getElementById('liste_document_categorie_' + key), function()
			{
				Effect.SlideDown('liste_document_categorie_' + key);
			}, AJAX_VOID_MESSAGE);

			return;
		}

		document.frm_ged.submit();
	}

	function create_flippingbook()
	{
		bodyStyle = document.getElementById('flippingbook').style;
		bodyStyle.visibility = "hidden";
		sizeContent();

		var flippingbook_settings = document.getElementById('flippingbook_settings');
		var toeval = "";
		if(flippingbook_settings.hasChildNodes())
		{
			for(var i = 0; i < flippingbook_settings.childNodes.length; i++)
			{
				if(flippingbook_settings.childNodes.item(i).nodeType == 1)
				{
					toeval += "flippingBook." + (flippingbook_settings.childNodes.item(i).name == 'pages' ? '' : 'settings.') + flippingbook_settings.childNodes.item(i).name + " = " + flippingbook_settings.childNodes.item(i).value + ";";
				}
			}
		}
		eval(toeval);

		flippingBook.contents = [
						[ "Cover", 1 ],
						[ "Modern", 4 ]
					];

		flippingBook.create();
	}

	function destroy_flippingbook()
	{
		flippingBook.destroy();
	}

	function reinit()
	{
	    with(document.frm_ged)
	    {
			target="_self";
	        action="./ged.php";
	    }
	}

	function rechercher_tag(tag)
	{ 
		with(document.recherche)
		{
			mot_cle.value = tag;
			submit();
		}
	}

	function generer_liste_document_vue_categorie_article(pk_categorie)
	{
		var DivDocument = document.getElementById('liste_document_categorie_' + pk_categorie);
		if(DivDocument.className == 'opened')
		{
			DivDocument.className = '';
			Effect.SlideUp('liste_document_categorie_' + pk_categorie);
		}
		else
		{
			DivDocument.className = 'opened';
			bve_doc_lanceur(pk_categorie, 'generer_liste_document_vue_categorie_article');
		}
	}
	
	function supprimer_avis(key)
	{
		lancer_ajax(document.frm_ged, 'ged.php', '', null,
						function()
						{
							document.getElementById('avis' + key).style.display="none";
						}
					, 'supprimer_avis', key);
	}
        
        function changer_nombre_resultats_page()
	{
            ajax(document.frm_ged, 'ged.php', 'vue_tableau_pagination', document.getElementById('tab_pagination'), null, AJAX_VOID_MESSAGE);
        }
        
        function rechercher_document()
        {
            ajax(document.frm_ged, 'ged.php', 'vue_tableau_pagination', document.getElementById('vue_tab_doc'), null, AJAX_VOID_MESSAGE);
        }
        
        function changer_nombre_resultats_page_document()
	{
            ajax(document.frm_ged, 'ged.php', 'vue_tableau_pagination', document.getElementById('vue_tab_doc'), null, AJAX_VOID_MESSAGE);
        }
 //]]>       
