		/*			LCS :Fonctions pour le changement de taille du texte T+ T-				 */
		/*************************************************************************************************/
		function GetElementsWithClassName(elementName,elementClassName) 
		{
		    var allElements = document.getElementsByTagName(elementName);
		    var elemColl = new Array();
		    for (var i = 0; i< allElements.length; i++) {
				if (allElements[i].className == elementClassName) {
					elemColl[elemColl.length] = allElements[i];
				}
		    }
			return elemColl;
		}
		
		function getStyle(elt,pro)
		{
			var element = elt;
			if (window.getComputedStyle) // Mozilla Firefox & cie
			{
				var propriete = window.getComputedStyle(element,null).getPropertyValue(pro);
			}
			else if (element.currentStyle) // Microsoft Internet Explorer
			{
			// Formatage (IE) de la propriété CSS
				while (pro.indexOf('-') != -1) 
				{
					var lettresuivtiret = pro.charAt(pro.indexOf('-')+1);
					pro = pro.replace(/-\S{1}/,lettresuivtiret.toUpperCase());
				}
				var propriete = eval('element.currentStyle.'+pro);
			}
			return propriete;
		}
		
		/****************************************************/
		/**               GESTION DU ZOOM                   */
		/****************************************************/
		
		function SizeUp(elmt)
		{
			if(elmt.hasChildNodes())
			{
				var fils = elmt.childNodes;
				for(var i = 0; i < fils.length; i++)
				{
					SizeUp(fils[i]);
				}
				var x = getStyle(elmt,"font-size").replace("px","").replace("pt","");
				elmt.style.fontSize = x*1+2*1+"px";
			}
		}
		
		function SizeDown(elmt)
		{
			if(elmt.hasChildNodes())
			{
				var fils = elmt.childNodes;
				for(var i = 0; i < fils.length; i++)
				{
					SizeDown(fils[i]);
				}
				var x = getStyle(elmt,"font-size").replace("px","").replace("pt","");
				if	(x > 2)
				{
					elmt.style.fontSize = x*1-2*1+"px";
				}
			}
		}
		
		//Partie zoom : on n'autorise que le zoom à 200%
		var zoom = 0;
		
		function doSizeUp()
		{	
			if	 (zoom < 2)
			{
				var className='resizable';
				var resizableElements = GetElementsWithClassName('*',className)
				for (var i = 0; i< resizableElements.length; i++) {
					SizeUp(resizableElements[i]);
				}
				zoom++;
			}
		}
		
		function doSizeDown()
		{
			if	 (zoom > -2)
			{
				var className='resizable';
				var resizableElements = GetElementsWithClassName('*',className)
				for (var i = 0; i< resizableElements.length; i++) {
					SizeDown(resizableElements[i]);
				}
				zoom--;
			}
		}

