																							<script type="text/javascript">
			$(document).ready(function() {
				$('ul.sous_menu > li:not(:has(".current")) > ul').hide();
				$('ul.sous_menu > li:has("ul")').children("a").click(function(){
					if (!$(this).parent().parent().hasClass('l3')) {
						sous_menu=$(this).siblings("ul");
						$('ul.sous_menu > li > ul').not($(sous_menu)).not($(this).parents('ul:first')).not($(sous_menu).children().children('ul.sous_menu')).slideUp();
						$(sous_menu).slideDown();
						$('ul.sous_menu li a').removeClass('active');
						$(this).addClass('active');
						if ($(this).parent().parent().parent().hasClass('l1')) $(this).parent().parent().parent().children('a:eq(0)').addClass('active');
						return false;
					}
				});
				$('.current').parents('li').addClass('current');
			});
		
																							</script>
