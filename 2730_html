    /**
     * Usage basique :
     *          "st-mamet":['Saint Mamet La Salvetat','#anchor']
     *      ou
     *          "st-mamet":['Saint Mamet La Salvetat','http://site.url']
     *
     * si le lien egale uniquement "#" alors rien ne se passeras
     *
     * On peut utiliser des functions a la place d'un lien "classique"
     * ex :
     *          "boisset":['nom',function(){console.log('COUCOU') ; return "//such.boisset.com"}]
     *
     * pour utiliser la function sans lien retourner "#" ou ne rien retourner
     **/

    function showComFancy(url){
        $.fancybox({ autoSize : true,
            width : 980,
            maxWidth: 1920,
            maxHeight: 1080,
            type: 'iframe',
            href: url
        });

	}


    var corrrespArray = {
        "Auriolles":['Auriolles','/annoncebox_auriolles_1_20_1_fr.html'],
        "Caplong":['Caplong','/annoncebox_caplong_2_20_1_fr.html'],
        "Eynesse":['Eynesse','/annoncebox_eynesse_3_20_1_fr.html'],
        "Landerrouat":['Landerrouat','/annoncebox_landerrouat_4_20_1_fr.html'],
        "La-Roquille":['La-Roquille','/annoncebox_la-roquille_5_20_1_fr.html'],
        "Les-Leves-et-Thoumeyragues":['Les-Leves-et-Thoumeyragues','/annoncebox_les-leves-et-thoumeyragues_6_20_1_fr.html'],
        "Ligueux":['Ligueux','/annoncebox_ligueux_7_20_1_fr.html'],
        "Listrac-de-Dureze":["Listrac-de-Dureze",'/annoncebox_listrac-de-dureze_8_20_1_fr.html'],
        "Margueron":['Margueron','/annoncebox_margueron_9_20_1_fr.html'],
        "Massugas":['Massugas','/annoncebox_massugas_10_20_1_fr.html'],
        "Pellegrue":['Pellegrue','/annoncebox_pellegrue_11_20_1_fr.html'],
        "Pineuilh":['Pineuilh','/annoncebox_pineuilh_12_20_1_fr.html'],
        "Port-Sainte-Foy-et-Ponchapt":['Port-Sainte-Foy-et-Ponchapt','/annoncebox_porte-sainte-foy-et-ponchapt_13_20_1_fr.html'],
        "Riocaud":['Riocaud','/annoncebox_riocaud_14_20_1_fr.html'],
        "Saint-Andre-et-Appelles":['Saint-Andre-et-Appelles','/annoncebox_saint-andre-et-appelles_15_20_1_fr.html'],
        "Saint-Avit-de-Soulege":['Saint-Avit-de-Soulege','/annoncebox_saint-avit-de-soulege_16_20_1_fr.html'],
        "Saint-Avit-Saint-Nazaire":['Saint-Avit-Saint-Nazaire','/annoncebox_saint-avit-saint-nazaire_17_20_1_fr.html'],
        "Sainte-Foy-la-Grande":['Sainte-Foy-la-Grande','/annoncebox_sainte-foy-la-grande_18_20_1_fr.html'],
        "Saint-Philippe-du-Seignal":['Saint-Philippe-du-Seignal','/annoncebox_saint-philippe-du-seignal_19_20_1_fr.html'],
        "Saint-Quentin-de-Caplong":['Saint-Quentin-de-Caplong','/annoncebox_saint-quentin-de-caplong_20_20_1_fr.html'],
        
        
    };//*//











    function getName($id){
        var name = corrrespArray[$id];
        if(name === undefined)
            return $id;

        if(name[0] === undefined)
            return $id;
        else
            return name[0];
    }

    function getLink($id){
        var ObjCom = corrrespArray[$id];
        if(ObjCom === undefined)
            return "#";



        if(ObjCom[1] === undefined)
            return "#";
        else{
            if(typeof ObjCom[1] === "function"){
                return ObjCom[1]();
            } else
                return ObjCom[1];
        }

    }

    $( document ).ready(function() {
        
        
        
        
        var $body = $('body');
        var $mouseFollow = $('#mapTooltip');
        var $container = $('div.svg-content-active');


        //activate following of the tooltip
         $container.on('mouseenter','g',function(e){
            $mouseFollow.addClass('animate');
            $container.addClass('hovered');
        });
        //disable it
         $container.on('mouseleave','g',function(e){
            $mouseFollow.removeClass('animate');
            $container.removeClass('hovered');
        });


        //change the text inside the tooltip
         $container.on('mouseover','g',function(){
            if(!$mouseFollow.hasClass('animate'))
                return;

            $('div.mousecontent').text(getName($(this).attr('id')));
        });

        //action on click
         $container.on('click','g',function(){
            if(!$mouseFollow.hasClass('animate'))
                return;

            var r = getLink($(this).attr('id'));
            console.log('link to ' + r);

            if(r !== undefined && r !== "#")
            	showComFancy(r);
            
            return;
            if(r !== undefined && r !== "#")
                window.location.href = r;
        });

        //match tooltip coordinate with mouse cursor
         $container.on('mousemove', function(e){

var parentOffset = $container.offset(); 
   //or $(this).offset(); if you really just want the current element's offset
   var relX = e.pageX - parentOffset.left;
   var relY = e.pageY - parentOffset.top;

            if($mouseFollow.hasClass('animate'))
                $mouseFollow.css('top',(relY+ 10)).css('left',(relX +10) )
        });
        
        
        
        
        
        
    });
												</script>
