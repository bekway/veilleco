					'hoverShadow':'#666666',
					'lakesColor':'#66CCFF',
					'namesColor':'#919191'
				},
			};
			
			for (var i = 1, max = map_cities.length; i <= max; i++) {
				map_config['map_' + i] = {
					'hover': map_cities[i-1],
					'upColor':'#ffffff',
					'overColor':'#84c8a0', /* MODIF-couleurs regions survol */
					'downColor':'#84c8a0', /* MODIF-couleurs regions survol */
					'enable':true,
				};
				addEvent('map_' + i);
			}
			
			var pin_config = {
				'default':{
					'pinShadow':'#000',
					'pinShadowOpacity':'50',
				},
				'points':[]
			}
		
						</script>
					</div>
				</div>
				<div class="clear"></div>
			</div>
			<div class="clear"></div>
		</div>
