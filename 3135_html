		<script type="text/javascript">
	$(document).ready(function(){			
		$('.fancy').fancybox();
		$('.ifancy').fancybox({type: 'iframe',width: 650});		
		$.datepicker.setDefaults( $.datepicker.regional[ "fr" ] );
		$( '#datepicker_ideas, #datepicker_ideas2' ).datepicker({dateFormat: 'yy-mm-dd', onSelect: function(selection){
			$(location).attr('href','/recherche_19.html?sdate='+selection);
		}});
		$( '.datepicker_range' ).datepicker();
		
		$.simpleWeather({
			woeid: '601259',	// http://woeid.rosselliot.co.nz/
			unit: 'c',
			success: function(weather) {
				image='url('+weather.image+')';
				temperature=weather.temp+'°C';
				wind=weather.wind.speed+weather.units.speed+' '+weather.wind.direction;
											
				Soleil = ["19","21","22","31","32","33","34","36"];
				Orage = ["0","1","2","3","4","17","35","37","38","47"];
				Pluie = ["5","6","7","8","9","10","11","12","18","39","40","45","48"];
				Nuage = ["20","23","24","26","27","28","29","30","44"];
				Neige = ["13","14","15","16","25","41","42","43","46"];
				
				//if(Soleil.indexOf(weather.code) > -1)
			   
				$("#bgmeteo").css('background-image',image);
				$("#tempmeteo").append(temperature+'<span>Vent : '+wind+'').show();
			},
			error: function(error) {
			  $("#weather").html('<p>'+error.message+'');
			}
		  });
		
		// fix hauteur iframe video (plugin custom tinymce avec choix largeur : garder ratio 16/9)
		$('.tinymce_video_width_part1, .tinymce_video_width_part2, .tinymce_video_width_part3, .tinymce_video_width_part4').each(function(){
			$(this).height(Math.round($(this).width()*9/16));
		});
	});
		</script>
