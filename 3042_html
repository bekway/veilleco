										<script type="text/javascript">

function filepage(fichier,callback)
{
  if(window.XMLHttpRequest){ // FIREFOX
    xhr_object = new XMLHttpRequest();
  }else if(window.ActiveXObject){ // IE
    xhr_object = new ActiveXObject("Microsoft.XMLHTTP");
  }else{
    return(false);
  }

  xhr_object.onreadystatechange = function(event) {
    if (this.readyState === XMLHttpRequest.DONE) {
      if (this.status === 200 || this.status === 0) {
        if (callback !== undefined) {
          //Avec callback, envoie en param de la fonction
          console.log(this.response);
          callback(this.response);
        }else{
          //Sans callback, retour direct
          console.log(this.responseText);
          return(this.responseText);
        }
      } else {
          console.log("Erreur AJAX query: Status [%d], StatusText[%s]", this.status, this.statusText);
          return(false);
      }
    }
  };
  xhr_object.open("GET", fichier, true);
  xhr_object.send(null);
}


function deletepage(id,delcallback){ if(texte = filepage('librairie/php/interactif_responsive/js_page_delete.php?id='+escape(id),delcallback)){  return(texte);  }}
function delpage(id)
{
	if (window.confirm('Vous allez supprimer cet élément,\n\ \352tes vous s\373r ?'))
	{
		// Lance la requete de supression des données en base ainsi que de la page dans le fichier racine
		var response = deletepage(id,delcallback); // Delete Page
	}
}
function delcallback(queryResult)
{
	var response = $.parseJSON(queryResult);
	console.log('=>' + response.result);
	console.log('=>' + response.numid);
	console.log('=>' + response.redirection);
	console.log('=>' + response.lien);
	console.log('=>' + response.filebase);
	if (response.result>0) {	$( "#page"+response.numid).remove();	}
    var url = response.redirection;
    if(url != '')  { window.location.replace(url);}
}
										</script>
									</ul>
								</li>
							</ul>
