function filepage(fichier,callback)
{
  if(window.XMLHttpRequest){ // FIREFOX
    xhr_object = new XMLHttpRequest();
  }else if(window.ActiveXObject){ // IE
    xhr_object = new ActiveXObject("Microsoft.XMLHTTP");
  }else{
    return(false);
  }

  xhr_object.onreadystatechange = function(event) {
    if (this.readyState === XMLHttpRequest.DONE) {
      if (this.status === 200 || this.status === 0) {
        if (callback !== undefined) {
          //Avec callback, envoie en param de la fonction
          console.log(this.response);
          callback(this.response);
        }else{
          //Sans callback, retour direct
          console.log(this.responseText);
          return(this.responseText);
        }
      } else {
          console.log("Erreur AJAX query: Status [%d], StatusText[%s]", this.status, this.statusText);
          return(false);
      }
    }
  };
  xhr_object.open("GET", fichier, true);
  xhr_object.send(null);
}


