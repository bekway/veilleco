function delcate(id)
{
	if (window.confirm('Vous allez supprimer cet élément,\n\ \352tes vous s\373r ?'))
	{
		var response = deletecategorie_pdf(id); // Delete Block
		$( "#idcate"+id).remove(); // supprime le paragraphes
	}
}
