$("#hrefup"+newid).attr("href", "javascript:blocpushliup_orderdesc('"+tri+"','"+newid+"','"+idliste+"','"+idsousliste+"')");
$("#hrefdown"+id).attr("href", "javascript:blocpushlidown_orderdesc('"+newordre+"','"+id+"','"+idliste+"','"+idsousliste+"')");
