      a.bloc_text_lien:link   {   text-decoration: none; font-weight: normal; display:inline; font-size:14px;color:#222!important;}
      a.bloc_text_lien:visited,
      a.bloc_text_lien:active,
      a.bloc_text_lien:hover  {   text-decoration: underline; font-weight: normal;  display:inline; font-size:14px;color:#222!important;}


