      .bloc_titre a:hover { font-size:100%;font-weight:bold;color:#fff;text-align:left;padding-right:3px;padding-left:3px;padding-top:10px;padding-bottom:10px;text-decoration: underline!important; }

      .bloc_stitre { font-size:100%; font-weight:bold;color:#fff;text-align:left;padding:0;padding-bottom:10px!important;}
      .bloc_stitre a { font-size:100%; font-weight:bold;color:#fff;text-align:left;padding:0;padding-bottom:10px!important;}
      .bloc_stitre a:hover { font-size:100%; font-weight:bold;color:#fff;text-align:left;padding:0;padding-bottom:10px!important;text-decoration: underline!important;}

      .bloc_date {font-size:100%; color:#555;font-weight:bold;text-align:left;padding-top:5px;padding-bottom:5px;}

      .bloc_description {font-size:100%; color:#555;text-align:left;padding:0;padding-top:0px;font-family: calibril;}
