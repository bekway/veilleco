var newordre = tableau[1];
// alert ('New ID = ' + newid);
newid = parseInt(newid);
// alert ('OLD TRI = ' + tri + "- OLD ID = " + id + "------- NEW TRI = " + newtri + "- NEW ID = " + newid);
