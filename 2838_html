				<style>
    #geoloc-map {
        position: absolute;
        height: 25rem;
        width: 100%;
        z-index: 0;
    }
    .map .overlay {
        position: relative;
        z-index: 0;
    }
    .map .btn-container{
        z-index: 23;
    }
				</style>
				<script>
    var myLatLng = [48.17638898277826, 2.3634755937500813];
    var map = L.map('geoloc-map', {
        zoomControl: false,
        maxZoom: 12
    }).setView(myLatLng, 5);

    L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
        maxZoom: 18,
        attribution: false,
        id: 'mapbox.streets'
    }).addTo(map);

    var points = [{"coords":["45.135290","1.301000"],"color":"#000000"}];
    var pointsBounds = [];
    console.log('------POINTS-----');
    console.log(pointsBounds);

    for (var i = 0; i < points.length; i++) {
        var myIcon = L.divIcon({
            className: 'mapMarkers marker_' + i,
            html: '<i class="material-icons mapMarkers" style="color:' + points[i]['color'] + '">location_on',
            iconAnchor: [16, 32]
        });

        pointsBounds.push(points[i]['coords']);

        L.marker(points[i]['coords'], {'icon': myIcon}).addTo(map);
    }
    console.log('-----------');
    console.log(pointsBounds);
    console.log('-----------');
    map.fitBounds(pointsBounds);
				</script>
