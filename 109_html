		<script type="text/javascript">
	
	

	dragIsRun = false;
	jQuery( window ).resize(function() {
		nbSlides = 9;
		$width = 1050;
		if(jQuery(this).width() < $width){
			nbSlides--;
		}
		$width-=100;
		if(jQuery(this).width() < $width){
			nbSlides--;
		}
		$width-=100;
		if(jQuery(this).width() < $width){
			nbSlides--;
		}
		$width-=100;
		if(jQuery(this).width() < $width){
			nbSlides--;
		}
		$width-=100;
		if(jQuery(this).width() < $width){
			nbSlides--;
		}
		$width-=100;
		if(jQuery(this).width() < $width){
			nbSlides--;
		}
		$width-=100;
		if(jQuery(this).width() < $width){
			nbSlides--;
		}
		
		
		if(jQuery(this).width() < 1157){
			
			if(dragIsRun == true){
				jQuery('#divdrag>div:first-child').slick('unslick');
			}
			dragIsRun = true;
			jQuery('#divdrag>div:first-child').slick({
				slidesToShow: nbSlides,
				slidesToScroll:nbSlides,
				nextArrow:"",
				prevArrow:""
			});

		} else {
			if(dragIsRun == true){
				dragIsRun = false;
				jQuery('#divdrag>div:first-child').slick('unslick');
			}
		}
		
	}).resize();
	
	logoOriginal = jQuery("#logo").attr("src");
	logoMini = "https://www.ville-troyes.fr/logo_troyes_bleu/";
	jQuery( window ).resize(function() {
		if(jQuery( window ).width() <= 980){
			jQuery("#logo").attr("src", logoMini);
		} else {
			jQuery("#logo").attr("src", logoOriginal);
		}

	}).resize();
	
		</script>
