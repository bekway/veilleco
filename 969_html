																<fieldset><select style="display:none;" id="nuxeo-search-collectivite"> <option value="cara" selected>Communauté d'Agglomération Royan Atlantique</option> </select>
																	<div><label>Type de document :</label> <select class="nuxeo-search-select" id="nuxeo-search-type"> <option value="">Tous les types</option> </select></div>
																	<div><label>Liste des thèmes :</label> <select class="nuxeo-search-select" id="nuxeo-search-sujet"> <option value="">Tous les sujets</option> </select></div>
																	<div><label>Dates :</label> <select class="nuxeo-search-select" id="nuxeo-search-date"> <option value="">Toutes les dates</option> </select></div>
																</fieldset>
																<fieldset>
																	<div class="search-field"><input type="text" name="query" id="nuxeo-search-query"> <button type="button" id="nuxeo-search-button">Rechercher</button></div>
																</fieldset>
															</div>
															<div id="nuxeo-search-resultats">
																<div id="nuxeo-search-liste"></div>
																<div class="nuxeo-pagination"><span id="nuxeo-search-totalQuery" data-nuxeo-search-total="0"></span>
