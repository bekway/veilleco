														<script>
	var portletNamespace = '_searchnuxeo_WAR_Nuxeoportlet_';
	var searchFileURL = 'https://www.agglo-royan.fr:443/documents-administratifs?p_p_id=searchnuxeo_WAR_Nuxeoportlet&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_cacheability=cacheLevelPage&p_p_col_id=column-1&p_p_col_pos=2&p_p_col_count=3';
	var nuxeoURL = 'https://documents-publics.agglo-royan.fr/nuxeo';
	var query = "";
	var type = "";
	var sujet = "";
	var collectivite = "";
	var dateSeance = "";
	var page = 0;
	var docsPerPage = parseInt("20");
	var serviceFolderPath = "";
	var showDescription = ('false' === 'true');
	var displayTable = ('true' === 'true');
	var enableDocumentAdministratif = ('true' === 'true');

	
	
	$(function() {
		collectivite = $("#nuxeo-search-collectivite option:selected").val();
		searchDatesSeances(portletNamespace, {
			"searchFileURL": searchFileURL, 
			"collectivite": collectivite,
			"type": type, 
			"sujet": sujet
		});
		searchTypeAndSujet(portletNamespace, {
			"searchFileURL": searchFileURL,
			"collectivite": collectivite
		});
		page = 0;
		clearPagination();
		searchDocuments(portletNamespace, {
			"searchFileURL": searchFileURL, 
			"nuxeoURL": nuxeoURL, 
			"collectivite": collectivite,
			"page": page, 
			"docsPerPage": docsPerPage, 
			"showDescription": showDescription,
			"path": serviceFolderPath,
			"displayTable": displayTable,
			"enableDocumentAdministratif": enableDocumentAdministratif
		});
	});
	$("#nuxeo-search-collectivite").on("change", function(event) {
		collectivite = $("#nuxeo-search-collectivite").val();
		type = $("#nuxeo-search-type").val();
		sujet = $("#nuxeo-search-sujet").val();
		searchDatesSeances(portletNamespace, {
			"searchFileURL": searchFileURL, 
			"collectivite": collectivite,
			"type": type, 
			"sujet": sujet
		});
		searchTypeAndSujet(portletNamespace, {
			"searchFileURL": searchFileURL,
			"collectivite": collectivite
		});
	});
	$("#nuxeo-search-type, #nuxeo-search-sujet").on("change", function(event) {
		collectivite = $("#nuxeo-search-collectivite").val();
		type = $("#nuxeo-search-type").val();
		sujet = $("#nuxeo-search-sujet").val();
		searchDatesSeances(portletNamespace, {
			"searchFileURL": searchFileURL, 
			"collectivite": collectivite,
			"type": type, 
			"sujet": sujet
		});
	});
	$("#nuxeo-search-docs").on("click", "#nuxeo-search-button", function() {
		query = $("#nuxeo-search-query").val();
		
		collectivite = $("#nuxeo-search-collectivite").val();
		type = $("#nuxeo-search-type").val();
		sujet = $("#nuxeo-search-sujet").val();
		dateSeance = $("#nuxeo-search-date").val();
		
		page = 0;
		clearPagination();
		searchDocuments(portletNamespace, {
			"searchFileURL": searchFileURL, 
			"nuxeoURL": nuxeoURL, 
			"query": query, 
			"collectivite": collectivite,
			"type": type, 
			"sujet": sujet, 
			"dateSeance": dateSeance, 
			"page": page, 
			"docsPerPage": docsPerPage, 
			"showDescription": showDescription,
			"path": serviceFolderPath,
			"displayTable": displayTable,
			"enableDocumentAdministratif": enableDocumentAdministratif
		});
	});
	
	
	

	function changePage(pagination_page) {
		page = pagination_page - 1;
		searchDocuments(portletNamespace, {
			"searchFileURL": searchFileURL, 
			"nuxeoURL": nuxeoURL, 
			"query": query, 
			"type": type, 
			"collectivite": collectivite,
			"sujet": sujet, 
			"dateSeance": dateSeance, 
			"page": page, 
			"docsPerPage": docsPerPage, 
			"showDescription": showDescription,
			"path": serviceFolderPath,
			"displayTable": displayTable,
			"enableDocumentAdministratif": enableDocumentAdministratif
		});
	}
	$("#nuxeo-search-resultats").on("click", ".link-files, .link-files-annexe", function(event) {

		if (document.body.scrollWidth < 928) {
			return true;
		}
		var documentURL = $(this).attr("href");
		if (documentURL.match(".pdf$")) {
			return !loadDocumentBlob(nuxeoURL, documentURL);
		} else {
			return true;
		}
	}).on("click", ".link-metadata", function(event) {
		var uuid = $(this).data('uuid');
		var url = "https://www.agglo-royan.fr:443/documents-administratifs?p_p_auth=sUuQUBg4&p_p_id=uploadnuxeo_WAR_Nuxeoportlet&p_p_lifecycle=0&p_p_state=pop_up&p_p_mode=view&p_p_col_id=column-1&p_p_col_pos=2&p_p_col_count=3&_uploadnuxeo_WAR_Nuxeoportlet_valid=true&_uploadnuxeo_WAR_Nuxeoportlet_upload-folder-uuid=wwww&_uploadnuxeo_WAR_Nuxeoportlet_upload-mode=metadata";
		url = url.replace("wwww", uuid);
		$("#nuxeo-form-title").html($(this).html());
		$("#nuxeo-form-iframe").attr("src", url);
		$("#nuxeo-form").show();
		$(".nuxeo-glass").show();
		return false;
 	});

	$("#nuxeo-search-liste").on("click", ".list-annexe", function() {
		var _this = $(this);
		var _ul = _this.next();
		_ul.toggle();
		if (_ul.is(':visible')) {
			_this.find('span').css('background-image', 'url(//db.agglo-royan.fr/images/minus.gif)');
		} else {
			_this.find('span').css('background-image', 'url(//db.agglo-royan.fr/images/plus.gif)');
		}
	});
