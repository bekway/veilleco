		// <![CDATA[
			

			function closeIframe() {
	$("#pdf-viewer-iframe").attr("src", "about:blank");
	$(".pdf-viewer-glass").hide();
	$("#pdf-viewer").hide();
}

$(document).ready(function(){ 
