					var form = document.getElementById('sendByMail-6197921808531784');
					var submit = document.getElementById('sendByMail-submit-6197921808531784');
					form.style.display = (form.style.display == 'none') ? '' : 'none';
					submit.style.display = (submit.style.display == 'none') ? '' : 'none';
				}
				sendByMail_form_switch_6197921808531784();
				
				function sendByMail_form_popup_6197921808531784()
				{
					refresh_sendByMail_6197921808531784();
					$j("#sendByMail-form-6197921808531784 script").remove();
					$j("#sendByMail-form-6197921808531784").dialog({
				      resizable: false,
				      height:'auto',
				      width: 'auto',
				      modal: true,
				      title: 'Envoyer par mail',
				      buttons: {
				        Envoyer: function() {
				          sendByMail_form_submit_6197921808531784();
				        },
				        Annuler: function() {
				          $j( this ).dialog( "close" );
				        }
				      }
				    });
				    
					$j('#sendByMail-6197921808531784-email-to').val('');
					$j('#sendByMail-6197921808531784-text').val('');
					$j('#sendByMail-6197921808531784-captcha').val('');
