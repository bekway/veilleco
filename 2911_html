		<script type="text/javascript">
	  $('.unblocmenu').mouseenter(function(){
		$(this).find('.sous_menu').slideDown(200);	
	  });
	  $('.unblocmenu').mouseleave(function(){
		$(this).find('.sous_menu').hide();	
	  });
	  
	  function printPDF(pdfUrl) {
		  var w = window.open(pdfUrl);
		  w.print();
	  }
	  
	  function go_annuaire() {
		var url = $('#select_annuaires').val();
		//console.log(url);
		document.location.href = url;
	  }
	  
	  function go_page_jesuis() {
		var url = $('#sel_cat_jesuis').val();
		document.location.href = url;
	  }
	  
	  function recomail() {
		mail_str = "mailto:?subject=" + document.title;
		mail_str += "&body=Je te suggère de lire cet article : " + document.title + "";
		mail_str += " est disponible a l'adresse : " + location.href;
		location.href = mail_str;
	  }
	  
	  function change_size(type) {
		switch(type) {
		  case 0:
			$('#wrap_contenu *').css('font-size','-=1px');
		  break;
		  case 1:
			$('#wrap_contenu *').css('font-size','+=1px');
		  break;
		  case 2:
			window.location.reload();
		  break;
		}
	  }
		</script>
