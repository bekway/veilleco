		<script type="text/javascript">
	function favoris() {
		if ( navigator.appName != "Microsoft Internet Explorer" ){
			window.sidebar.addPanel("Communauté de Communes <br />Brioude Sud Auvergne","","");
		}
		else {
			window.external.AddFavorite("","Communauté de Communes <br />Brioude Sud Auvergne");
		}
	}

	function afficheMenu(baliseId, baliseId2, color, texte)
	{
		$('.sous-menu').hide();
		$('#menu-princ td').removeClass("active");
		$('#menu-princ td').css("background",'none');
		$('#menu-princ td a').css("color","#887F78");

		if ($(baliseId) && $(baliseId) != null)
		{
			console.log(texte);
			$(baliseId2).css("background",color);
			$(baliseId).css("background",color);
			$(baliseId2+' a').css("color",texte);
			$(baliseId+' .content_link a').css("color",texte);
			$(baliseId2).addClass("active"); 	
			$(baliseId).show();
		}
	}



	$(document).ready(function() {

		$('#nav').mouseleave(function() {
			cacheMenu();
		});

		function afficheMasqueElement(o){
			var IdElement = $(o).attr("id");
			var etatElement = $('#table-article-'+IdElement).css("display");
			if(etatElement){
				if((etatElement=='block')||(etatElement=='table')){
					$('#table-article-'+IdElement).addClass("hidden");	
					$('#'+IdElement+' .icon').text('[+]');
				}else{
					$('#table-article-'+IdElement).removeClass("hidden");	
					$('#'+IdElement+' .icon').text('[-]');	
				}
			}
		}	 

		function cacheMenu()
		{
			$('.sous-menu').hide();
			$('#menu-princ td').removeClass("active");
			$('#menu-princ td').css("background",'none');
			$('#menu-princ td a').css("color","#887F78");
		}
	});
		</script>
		<script type="text/javascript" src="https://www.cc-brivadois.fr/js/tags.js"></script>
		<script type="text/javascript">
    $(function () {
        $('#search_keyword2').tagSuggest({
            url: 'https://www.cc-brivadois.fr//tagging.php',
            delay: 700
        });

        $('a.menu-toggle-btn').bind("click", function(){
	
	    var $lefty = $('#sous-menu-mobile');
    $lefty.animate({
      left: parseInt($lefty.css('left'),10) == 0 ?
        -$lefty.outerWidth() :
        0		
    });
	var posmenu = $lefty.css('left');
	if(posmenu=='0px'){
	$('a.menu-toggle-btn').css('background','url(http://www.brioude.fr/img/mobile/menu_mobile_off.png) no-repeat right');
	$('.sous-sous-menu-mobile').css('left','-300px');
	}else{
	$('a.menu-toggle-btn').css('background','url(http://www.brioude.fr/img/mobile/menu_mobile_on.png) no-repeat right');	
	}

});

$('li.link_arrow').bind("click", function(){
var id_rubrique = $(this).find("a[href]").attr('rel');


	 var $lefty = $('#sous-sous-menu-'+id_rubrique);
    $lefty.animate({
      left: parseInt($lefty.css('left'),10) == 0 ?
        -$lefty.outerWidth() :
        0		
    });

});

$('.menu-back-btn').bind("click", function(){
	 var $lefty = $('.sous-sous-menu-mobile');
    $lefty.animate({
      left: '-300px'
    });

});

$('.icon_right_search').bind("click", function(){
$('.sous-sous-menu-mobile').css('left','-300px');
$('#sous-menu-mobile').css('left','-300px');
			var etatElement = $('#form_recherche_mobile').css("display");
			if(etatElement){
				if((etatElement=='block')||(etatElement=='table')){
					$('#form_recherche_mobile').css("display","none");	
				}else{
					$('#form_recherche_mobile').css("display","block");
				}
			}



});

    });
    
