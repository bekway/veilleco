        var test = new Vue({
            el: ".carte-des-communes",
            data: {
                currentCommune: Math.floor(Math.random() * Math.floor(10)),
                communes: [
                    {
                        nom: "Dommartin-lès-Remiremont",
                        siteInternet: "https://www.dommartin-les-remiremont.fr/",
                        map: "dommartin"
                    },
                    {
                        nom: "Eloyes",
                        siteInternet: "https://www.eloyes.fr/",
                        map: "eloyes"
                    },
                    {
                        nom: "Girmont-Val-d'Ajol",
                        siteInternet: "http://www.girmont.org/",
                        map: "girmont"
                    },
                    {
                        nom: "Le Val-d'Ajol",
                        siteInternet: "http://www.valdajol.fr/",
                        map: "val-ajol"
                    },
                    {
                        nom: "Plombières-les-Bains",
                        siteInternet: "https://www.plombieres-les-bains.fr",
                        map: "plombiere-les-bains"
                    },
                    {
                        nom: "Remiremont",
                        siteInternet: "https://www.remiremont.fr/",
                        map: "remiremont"
                    },
                    {
                        nom: "Saint-Amé",
                        siteInternet: "https://www.saint-ame.fr/",
                        map: "saint-ame"
                    },
                    {
                        nom: "Saint-Etienne-lès-Remiremont",
                        siteInternet: "http://www.ville-st-etienne-remiremont.fr/",
                        map: "saint-etienne"
                    },
                    {
                        nom: "Saint-Nabord",
                        siteInternet: "http://www.saint-nabord.fr/",
                        map: "saint-nabord"
                    },
                    {
                        nom: "Vecoux",
                        siteInternet: "http://www.vecoux.fr/",
                        map: "vecoux"
                    }
                ]
            },
            methods: {
                openWebsite(e) {
                    window.open(this.communes[this.currentCommune].siteInternet,'_blank');
                },
                calculateMap() {
                    var carteZone = document.getElementById("cartezone");
                    var imageMap = document.querySelector(".imagemap");
                    var ratio = (carteZone.offsetWidth - 30) / 1024;
                    imageMap.style.transform = "scale(" + ratio + ")";
                    console.log("je me resize");
                }
            },
            mounted() {
                this.calculateMap();
                window.onresize = event => {
                    this.calculateMap();
                };
            }
        })
        
