jQuery( window ).load(function() {
if (jQuery(window).width() > 980 && jQuery('.home').length == 0 && jQuery('.page-template-default').length == 0 && jQuery('.page-template-actualites').length == 0 && jQuery('.page-template-evenement').length == 0) {
	
	var HauteurFirstDiv = jQuery('.ColonneCentre .TitreSouligne:nth-child(1)').outerHeight(true);
	var HauteurDivDroite = jQuery('.ColonneDroite .TitreSouligne:nth-child(1)').outerHeight(true);
		if(HauteurFirstDiv < HauteurDivDroite) {
			var MarginT	= HauteurDivDroite - HauteurFirstDiv;
			jQuery('.ColonneDroite .TitreSouligne:nth-child(1)').css('margin-top',-MarginT);
		
		}
		else {
			var MarginT	= HauteurFirstDiv - HauteurDivDroite;
			jQuery('.ColonneCentre .TitreSouligne:nth-child(1)').css('margin-top',-MarginT);
		
		}
	}
});	
// Scroll TOP
jQuery("a[href='#'] #BackTop").click(function() {
  jQuery("html, body").animate({ scrollTop: 0 }, "slow"); 
  return false;
});
