		<script language="javascript">
var taille=100;
var taille_original=100;
var limite = 40;
	function size_plus()
	{
		taille = (taille+10);
		if(taille>eval(taille_original+limite))
		{
			taille = (taille-10);
		}
		$('body').css('font-size',taille+'%');
	}
	
	function size_moins()
	{
		taille = (taille-10);
		if(taille<eval(taille_original-limite))
		{
			taille = (taille+10);
		}
		$('body').css('font-size',taille+'%');
	}
		</script>
