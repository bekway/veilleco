			$('.generepdf').on("click", function(){							
			var c=$("#contenu_dyn > .wrap").find("script,noscript,style").remove().end().html();
			if (typeof(c)=='undefined'){				
				c=$("#contenu_dyn").find("script,noscript,style").remove().end().html();
			}

			
			var t='D%C3%A9lib%C3%A9rations';				
				$.post(urlSite+"index.php/pagetopdf/prepare",
					{ contenu: c, titre: escape("Télécharger le compte-rendu sommaire") , css: 'css/59178_2.css', keyword : 'délibération, acte, administratif'},
						function(data){					
							window.open('http://www.ville-douai.fr//index.php/pageToPDF/display/'+t+'.pdf');
							return false;
					}
				);
			});

