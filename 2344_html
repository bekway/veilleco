jQuery(document).ready(function($){
				$("#cookiesck").remove();
				$("body").append("<div id=\"cookiesck\" data-layout=\"layout1\"/>");
				$("body").append("<div id=\"cookiesck_overlay\" />");
				
				$("#cookiesck").append("<div class=\"inner\">");
				$("#cookiesck > .inner").append("<span id=\"cookiesck_text\">En visitant ce site, vous acceptez l'utilisation de cookies afin de vous proposer les meilleurs services possibles.")
					.append("<span id=\"cookiesck_buttons\">");
				$("#cookiesck_buttons").append("<a class=\"cookiesck_button\" id=\"cookiesck_accept\">J'ai compris !")
					.append("<a class=\"cookiesck_button\" href=\"/ccbl/cookies\"  target=\"\" id=\"cookiesck_readmore\">En savoir plus")
					.append("<div style=\"clear:both;\">")
