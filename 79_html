		<main class="site-content">
			<section class="page-section padded-section search">
				<div class="container">
					<header class="section-header text-center">
						<h1 class="section-title d-inline">comptes-rendus-des-seances-du-conseils-municipal</h1><span class="section-title search-result-count">(1361 résultats)</span></header>
					<div class="d-lg-flex justify-content-lg-between">
						<div class="section-header-module ml-sm-auto outils-accessibilite"><button type="button" class="btn btn-secondary btn-readspeaker on show"><span class="icon speaker-icon"></span> Activer la vocalisation</button>
							<div class="readspeaker hide sticky"><button type="button" class="btn rs-play hide"><span class="icon speak-play-icon"></span></button> <button type="button" class="btn rs-pause show"><span class="icon speak-pause-icon"></span></button> <button type="button" class="btn rs-stop show"><span class="icon speak-stop-icon"></span></button> <button type="button" class="btn rs-preview show"><span class="icon speak-preview-icon"></span></button> <button type="button" class="btn rs-next show"><span class="icon speak-next-icon"></span></button> <button type="button" class="btn rs-close show"><span class="icon speak-close-icon"></span></button></div><br class="d-none d-sm-inline d-md-none"> <button type="button" class="btn btn-secondary btn-malentendant"><span class="icon malentendant-icon"></span></button> <button type="button" class="btn btn-secondary btn-minus">A-</button> <button type="button" class="btn btn-secondary btn-plus">A+</button> <button type="button" class="btn btn-reinit">A</button></div>
						<script type="text/javascript">
    var fontSize = 0;
    var contrast = 0;
    var rootUrl = '/accessibility/';
    var canRead = false;
    var tapped = false;
    var indexSpeak = 0;
    var allText, allNodeText = null;
    var parameters = {
        onstart: voiceStartCallback,
        onend: voiceEndCallback,
        rate: 1
    }
    responsiveVoice.setDefaultVoice("French Female");

    $(document).ready(function () {
        voiceloadData();

        $(".btn.rs-play").on("click", function () {
            if (responsiveVoice.isPlaying()) {
                responsiveVoice.resume();
            }
            else {
                launchSpeak();
            }
            $(".btn.rs-play").removeClass("show").addClass("hide");
            $(".btn.rs-pause").removeClass("hide").addClass("show");
        });

        $(".btn.rs-pause").on("click", function () {
            responsiveVoice.pause();
            $(".btn.rs-play").removeClass("hide").addClass("show");
            $(".btn.rs-pause").removeClass("show").addClass("hide");
        });

        $(".btn.rs-stop").on("click", function () {
            $(allNodeText[indexSpeak]).removeClass("highlight");
            $(".btn.rs-play").removeClass("hide").addClass("show");
            $(".btn.rs-pause").removeClass("show").addClass("hide");
            responsiveVoice.cancel();
            indexSpeak = 0;
        });

        $(".btn.rs-preview").on("click", function () {
            if (indexSpeak > 0) {
                $(allNodeText[indexSpeak]).removeClass("highlight");
                indexSpeak--;
                launchSpeak();
            }
        });

        $(".btn.rs-next").on("click", function () {
            if (indexSpeak < allNodeText.length - 1) {
                $(allNodeText[indexSpeak]).removeClass("highlight");
                indexSpeak++;
                launchSpeak();
            }
        });

        $(".btn.rs-close").on("click", function () {
            responsiveVoice.cancel();
            $(allNodeText[indexSpeak]).removeClass("highlight");
            indexSpeak = 0;
            $(".readspeaker").removeClass("show").addClass("hide");
            $(".btn-readspeaker.on").removeClass("hide").addClass("show");
            responsiveVoice.cancel();
        });
    });

    function voiceloadData() {
        $(".btn.rs-play").removeClass("hide").addClass("show");
        $(".btn.rs-pause").removeClass("show").addClass("hide");
        responsiveVoice.cancel();
        indexSpeak = 0;
        var elementToSelect = "body.pre-home div.container p:not(.buttons), body.pre-home main div.container p.buttons a, body main section div.container p:not(.noSpeak), body main section div.container h1, body main section div.container h2, body main section div.container h3,  body main section div.container h4, body main section div.container div.content-block ul:not(.event-list) li, body main section div.container.plan-du-site ul li";
        //
        allText = $(elementToSelect).map(function () {
            if (this.innerHTML != "") {
                //Replace Html balise - \n\r -  
                return this.innerText.replace(/<(?:.|\n)*?>/gm, '').replace(/\s+/gm, ' ').replace(/ /g, ' ').trim();
            }
        }).get();
        
        allNodeText = $(elementToSelect).map(function () {
            if (this.innerHTML != "") {
                return this;
            }
        }).get();
    }

    function voiceStartCallback() {
        var stickyNav = $('nav.navbar');
        var collapsibleSearch = $('.collapsibleSearch');
        var margin = 45;
        var element = $(allNodeText[indexSpeak]);
        element.addClass("highlight");
        
        var scrollto = element.offset().top - stickyNav.outerHeight() - margin;
        if (!stickyNav.hasClass("sticky")) {
            scrollto = scrollto - stickyNav.outerHeight();
        }
        
        if (collapsibleSearch.hasClass("sticky")) {
            scrollto = scrollto - collapsibleSearch.outerHeight();
        }

        $('html,body').animate({
            scrollTop: scrollto
        }, 'slow');
    }

    function voiceEndCallback() {
        $(allNodeText[indexSpeak]).removeClass("highlight");
        //
        if (indexSpeak < allNodeText.length - 1) {
            indexSpeak++;
            launchSpeak();
        }
        else {
            responsiveVoice.cancel();
            indexSpeak = 0;
            $(".btn.play").removeClass("hide").addClass("show");
            $(".btn.pause").removeClass("show").addClass("hide");
        }
    }

    function launchSpeak() {
        var text = allText[indexSpeak];
        if (text != "" && text != null) {
            setTimeout(function () {
                responsiveVoice.cancel();
                responsiveVoice.speak(text, "French Female", parameters);
            }, 100);
        }
        else {
            voiceEndCallback();
        }
    }

    function saveAccessibility(btn, elem, value) {
        if (navigator.cookieEnabled && elem != '') {
            //disabled button
            btn.attr("disabled", "disabled");
            $.ajax({
                type: "POST",
                url: rootUrl + elem + '/' + value,
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                success: function (data) {
                    btn.removeAttr("disabled");
                },
                error: function () {
                    //alert("Error while inserting data");
                }
            });
            return false;
        }
    }

    $(".btn-readspeaker.on").on("click", function () {
        $(".readspeaker").removeClass("hide").addClass("show");
        $(".btn-readspeaker.on").removeClass("show").addClass("hide");

        //Collapse ALL
        $("button[data-toggle='collapse']").removeClass("collapsed");
        $(".collapse").not(".collapsible-search").addClass("show");

        //Fix bug launch one speak null;
        setTimeout(function () {
            responsiveVoice.cancel();
            responsiveVoice.speak(" ", "French Female");
            launchSpeak();
        }, 100);
    });

    //Action Button
    $(".btn-malentendant").on("click", function () {
        window.location.href = "/article/le-module-daccessibilite";
    });

    $(".btn-minus").on("click", function () {
        $("html").removeClass("s1 s2 s3 s4")
        if (fontSize <= 1) {
            fontSize = 0;
            saveAccessibility($(this), "user-fontsize", fontSize);
        }
        else {
            fontSize--;
            $("html").addClass("s" + fontSize);
            saveAccessibility($(this), "user-fontsize", fontSize);
        }
    });
    $(".btn-plus").on("click", function () {
        if (fontSize == 4) return;
        fontSize++;
        $("html").removeClass("s1 s2 s3 s4")
        $("html").addClass("s" + fontSize);
        saveAccessibility($(this), "user-fontsize", fontSize);
    });
    //
    $(".btn-reinit").on("click", function () {
        $("html").removeClass("c1 c2 c3")
        contrast++;
        if (contrast > 3) {
            contrast = 0;
        }
        else {
            $("html").addClass("c" + contrast);
        }
        saveAccessibility($(this), "user-contrast", contrast);
    });
						</script>
