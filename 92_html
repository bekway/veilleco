	$('.profil-photo').css('background-image','url("./plugins/graphisme_auber2.0/_images/profilage/senior.jpg")');
})
$('#senior-hover').mouseleave(function(){
	$('.profil-photo').css('background-image','url("./plugins/graphisme_auber2.0/_images/profilage/jeunes.jpg")');
})

