$(function(){
  $('#enhaut').click(function() {
    $('html,body').animate({scrollTop: 0}, 'slow');
  });

  $(window).scroll(function(){
     if($(window).scrollTop()<900){
        $('#enhaut').fadeOut();
     }else{
        $('#enhaut').fadeIn();
     }
  });
});
