					<script>
        $(function() {
            var $list = $('#js-section-map-list'),
                $map = $('#js-section-map');

            $('#js-section-map, #js-section-map-unactive').find('a').tooltip({
                container: '#fm-group-map'
            });

            $list
                .on('mouseenter', 'a', function () {
                    var id = $(this).parent().attr('id'),
                        id_splited = id.split('-');
                    $map.find('#map-item-' + id_splited[2]).addClass('active');
                })
                .on('mouseleave', 'a', function () {
                    var id = $(this).parent().attr('id'),
                        id_splited = id.split('-');
                    $map.find('#map-item-' + id_splited[2]).removeClass('active');
                });

            $map
                .on('mouseenter', 'path', function () {
                    var id = $(this).attr('id'),
                        id_splited = id.split('-');
                    $list.find('#menu-item-' + id_splited[2]).find('a').addClass('active');
                })
                .on('mouseleave', 'path', function () {
                    var id = $(this).attr('id'),
                        id_splited = id.split('-');
                    $list.find('#menu-item-' + id_splited[2]).find('a').removeClass('active');
                });
        });
    
