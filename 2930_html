			$("#ac-container-content-titre-68").click(function(){
				$(".ac-medium").height(0);
									
				if($("#article-page-68").height() == 0){	
					
					if(isInPopup_16){
					    $("#article-page-68").height("auto");
					} else {
					    $("#article-page-68").height($("#article-page-68").attr("height"));
					}
					$("html, body").animate({scrollTop: $("#ac-container-16").offset().top-50}, 500);
				}		
			});
					$("#article-page-69").attr("height",$("#article-page-69").height());
			
			$("#ac-container-content-titre-69").click(function(){
				$(".ac-medium").height(0);
									
				if($("#article-page-69").height() == 0){	
					
					if(isInPopup_16){
					    $("#article-page-69").height("auto");
					} else {
					    $("#article-page-69").height($("#article-page-69").attr("height"));
					}
					$("html, body").animate({scrollTop: $("#ac-container-16").offset().top-50}, 500);
				}		
			});
					
			
			if(timeout_accordeon != undefined)	
                    clearTimeout(timeout_accordeon);
			
			    timeout_accordeon = setTimeout(function(){ $(".ac-medium").height(0); }, 1000);
			    $("#ac-74").prop("checked", true);
				$("#article-page-74").height($("#article-page-74").attr("height"));
			

			
			});
		
												</script>
											</td>
										</tr>
									</tbody>
								</table>
							</div>
						</div>
					</div>
				</div>
			</div>
