		<script type="text/javascript">
    jQuery(window).load( function() {
        jQuery(document.body).on('submit', '#form_recherche_mini', function(e){
            var currentTarget = jQuery( e.currentTarget );
            if( currentTarget ) {
                var searchTarget = currentTarget.find('#recherche-mini');
                if (searchTarget.val() == "" || searchTarget.val() == "Votre recherche...") {
                    return false;
                }
            }
        });

        jQuery(document.body).on('focus', '#recherche-mini', function(e){
            var currentTarget = jQuery( e.currentTarget );
            if( currentTarget.val() == 'Votre recherche...' ) { currentTarget.val(''); }
        });

        jQuery(document.body).on('blur', '#recherche-mini', function(e){
            var currentTarget = jQuery( e.currentTarget );
            if( currentTarget.val() == '' ) { currentTarget.val('Votre recherche...'); }
        });
    });

    
		</script>
		<script>
	if( typeof flexit != 'undefined' && typeof flexit.trad != 'undefined' ) {
		flexit.trad.search_commune = "Rechercher...";
	}

		$(document).ready(function() {
		$(function(){
			$("#intro-map").mapael({
				"map" : {
					"name" : "ccterredeau",
					"defaultArea": {
						"attrs" : {
							"stroke" : "#22649e",
							"stroke-width" : 1,
							"fill": "#fff",
							"cursor": 'pointer'
						},
						"attrsHover" : {
							"fill" : '#a0d4ad'
						},
						"tooltip": {
							"offset": {"left": 0, "top": -50},
							"overflow": {"right": true, "bottom": false},
							"content" : function ( e ) {
								if( typeof cctdMapInfos != "undefined" && cctdMapInfos.length > 0 ) {
									var targetId = e.id - 1;
									if( typeof cctdMapInfos[ targetId ] != "undefined" && typeof cctdMapInfos[ targetId ] == "object" ) {
										return cctdMapInfos[ targetId ].nom_site;
									}

									return '';
								}
							}
						},
						eventHandlers: {
							click: function (e, id, mapElem, textElem) {
								var targetId = mapElem.id - 1;
								if( typeof cctdMapInfos[ targetId ] != "undefined" && typeof cctdMapInfos[ targetId ] == "object" ) {
									if( cctdMapInfos[ targetId ].url_site != '' ) {
										toggleFlexitLoader(true);
										window.location.href = cctdMapInfos[targetId].url_site;
									}
								}
							}
						}
					},
				},
