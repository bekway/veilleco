									<script>
	function MM_validateForm() { //v4.0
  var i,p,q,nm,TheID,test,num,min,max,errors='',args=MM_validateForm.arguments;
  for (i=0; i<(args.length-2); i+=3) { TheID=args[i+1]; test=args[i+2]; val=MM_findObj(args[i]);
    if (val) { nm=val.name; if ((val=val.value)!="") {
      if (test.indexOf('isEmail')!=-1) { p=verifMail(val);
        if (p<1 || p==(val.length-1)) errors+='- "'+TheID+'" doit contenir une adresse email correcte.\n';
      } else if (test!='R') { num = parseFloat(val);
        if (isNaN(val)) errors+='- "'+TheID+'" doit contenir un nombre.\n';
        if (test.indexOf('inRange') != -1) { p=test.indexOf(':');
          min=test.substring(8,p); max=test.substring(p+1);
          if (num<min || max<num) errors+='- "'+nm+'" doit contenir un nombre entre '+min+' et '+max+'.\n';
    } } } else if (test.charAt(0) == 'R') errors += '- "'+TheID+'" est requis.\n'; }
  } if (errors) alert('Les champs de formulaire cités ci-dessous doivent être remplis :\n'+errors);
  document.MM_returnValue = (errors == '');
}
									</script>
