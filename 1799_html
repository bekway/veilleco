							<script type="text/javascript">
		jQuery(function($){
			jQuery.datepicker.setDefaults($.datepicker.regional['fr']);
			jQuery('#datepicker_1,datepicker_2').mousedown(function(){
				jQuery("#datepicker_1,datepicker_2").datepicker({minDate:'-36500', maxDate:'+36500', dateFormat: 'dd-mm-yy', firstDay:1, cgangeFirstDay : false }).attr("readonly","readonly");
			});

			$('#menu_recherche').submit(function() {
				var action = $(this).attr("action");
				
				if( $('#datepicker_1').val() ) {
					action += '/(startdate)/' + $('#datepicker_1').val().split('/').join('-');
				}
				if( $('#datepicker_2').val() ) {
					action += '/(enddate)/' +  $('#datepicker_2').val().split('/').join('-');
				}

				$(this).attr("action", action );
				return true;
			});
		});
