     var t;			
     t = 1;
    
     function changerTaille(modif) {
      t = t + modif;
      document.getElementsByTagName("body")[0].style.fontSize = t + "em";		
     } 
    
     function resetTaille() {
      document.body.style.fontSize = "1em";		
