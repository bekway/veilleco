						<div class="onoffswitch"><input type="button" name="myonoffswitch" class="onoffswitch-checkbox" id="myonoffswitch" onclick="switch_value(this.id);return change(this);" value="Noir & Blanc" title="passer la page en noir et blanc"></div>
						<script type="text/javascript">
                        function change( el ){
                            if ( el.value === "Noir & Blanc" )
                                el.value = "Couleur";
                            else
                                el.value = "Noir & Blanc";
                        }
                    
						</script>
						<div class="invert"><button name="invert_button" class="invert_button" id="invert_button" onclick="switch_value(this.id)" title="inverser les couleurs de la page">Inverser</button></div>
					</div>
				</div>
