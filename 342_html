  function verifForm(datesesField, titreField){
    if(datesesField || titreField){
      var dateses = jQuery('#DATESES').attr('value');
      var titre = jQuery('#TITRE').attr('value');

      if(dateses == datesesField){
        jQuery('#DATESES').attr('value','');
      }

      if(titre == titreField){
        jQuery('#TITRE').attr('value','');
      }
      jQuery('#formqueryc5d6643f6cdfa88895071defbe449e4b').submit();
    }
  }
  
