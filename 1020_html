/*
|-----------------------------------------------------------------------
|  jQuery Multiple Toggle Script by Matt - www.skyminds.net
|-----------------------------------------------------------------------
|
| Affiche et cache le contenu de blocs multiples.
|
*/
jQuery(document).ready(function() {
    header(".more").hide();
    jQuery('.button-read-more').click(function () {
        header(this).closest('.less').addClass('active');
        header(this).closest(".less").prev().stop(true).slideDown("1000");
    });
    jQuery('.button-read-less').click(function () {
        header(this).closest('.less').removeClass('active');
        header(this).closest(".less").prev().stop(true).slideUp("1000");
    });
});
