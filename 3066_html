	// listen for scroll
	var positionElementInPage = $('#sociaux').offset().top;
		$(window).scroll(
			function() {
				if ($(window).scrollTop() >= positionElementInPage) {
					// fixed
					$('#sociaux').addClass("floatable");
				} else {
					// relative
					$('#sociaux').removeClass("floatable");
				}
			}
