			function newsletter_validation_12B560860E074D9D85058A2226B31A64() {
				var email = $("#inputNewsletter-12B560860E074D9D85058A2226B31A64").val();
				var modele = /^[a-zA-Z0-9\.\-_]+@[a-zA-Z0-9\-]+[a-zA-Z0-9\.\-]+\.[a-zA-Z]{2,5}$/i;
	
				if (!modele.test(email))
				{
				custom_alert ("Veuillez saisir une adresse e-mail valide svp");
			
				} else {
					//code ajax d enregistrement
	
				$.ajax ({
					url: "/modules/newsletter/newsletter_client-ajax.php",
					type: "POST",
					data: {
						action : "insert",
						mail : $("#inputNewsletter-12B560860E074D9D85058A2226B31A64").val(),
						type : $("#selectNewsletter-12B560860E074D9D85058A2226B31A64").val(),
						token : $("#token").val()
					},
					success: function(data) {
					custom_alert (data);
						$("#inputNewsletter-12B560860E074D9D85058A2226B31A64").val("");
					},
				});
	
				}
			}
			
					</script>
