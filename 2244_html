				<script type="text/javascript"><!--

                function feedback_form_switch_9006025647666256()
                {
                	var form = $j("#feedback-9006025647666256");
                    if (form.is(":visible"))
					{
						$j(form).hide(800,function(){$j(form).css('overflow','visible');});
					}
					else
					{
						$j(form).show(800,function(){$j(form).css('overflow','visible');});
					}
                }
                feedback_form_switch_9006025647666256();
                
                function feedback_form_clear_9006025647666256()
                {
                    var form = document.getElementById('feedback-9006025647666256');
                    $j(form).find(':input').each(function () { 
                        switch(this.type) {
                            case 'password':
                            case 'select-multiple':
                            case 'select-one':
                            case 'text':
                            case 'textarea':
                                $j(this).val('');
                                break;
                            case 'checkbox':
                            case 'radio':
                                this.checked = false;
                        }
                    });
                }
                
                function feedback_form_submit_9006025647666256()
                {
                    var form = document.getElementById('feedback-9006025647666256');
                    
                    var pageId =  $j('#path-9006025647666256');
                    var nameInput = $j('#feedback-9006025647666256-name');
                    var emailInput = $j('#feedback-9006025647666256-email');
                    var contentInput = $j('#feedback-9006025647666256-text');
                    var site = $j('#feedback-9006025647666256-site');
                    
                    var errors = false;
                    if (!/^[a-z0-9._-]+@[a-z0-9.-]{2,}[.][a-z]{2,3}$/.test(emailInput.val()) && emailInput.val() != "")
                    {
                        errors = true;
                        emailInput.parentsUntil('.field').parent().addClass('error');
                    }
                    else
                    {
                        emailInput.parentsUntil('.field').parent().removeClass('error');
                    }
                    
                    if (/^\s*$/.test(contentInput.val()))
                    {
                        errors = true;
                        contentInput.parentsUntil('.field').parent().addClass('error');
                    }
                    else
                    {
                        contentInput.parentsUntil('.field').parent().removeClass('error');
                    }
                    
                    if (!errors)
                    {
                        // send it in ajax
                        var data = {
                            "path": pageId.val(),
                            "name": nameInput.val(),
                            "mail": emailInput.val(),
                            "boolean":  $j("input:radio[name=boolean]:checked" ).val(),
                            "comment": contentInput.val(),
                            "site": site.val(),
                            "ajax": "true"
                        };
                        
                        function result(data)
                        {
                            var s = "";
                            $j('errors error', data).each(function () { s += $j(this).text() + "\n"; });
                            if (s == "")
                            {
	                            var errors = $j('error', data);
	                            if (errors.size() == 0)
	                            {
	                                alert("Retour d'information soumis\nVotre retour a bien été envoyé et sera pris en compte par un webmaster.\nMerci pour votre contribution.")
    	                            feedback_form_switch_9006025647666256();
    	                            refresh_feedback_9006025647666256();
	                                feedback_form_clear_9006025647666256();
	                            }
	                            else
	                            {
	                                var errorInfo = "";
	                                $j.each(errors, function(i) {
	                                    errorInfo += "\n* " + $j(errors[i]).text();
	                                }); 
	                                alert("Une erreur est survenue :" + errorInfo)
	                                refresh_feedback_9006025647666256();
	                            }
	                            
                            }
                            else
                            {
                                alert(s);
                                refresh_feedback_9006025647666256();
                            }
                        }
                        
                        function handleError(jqXHR, textStatus, errorThrown)
	                    {
	                        alert("Une erreur est survenue lors de l'envois de votre message. Veuillez réessayer ou contacter l'administrateur.");
	                        refresh_feedback_9006025647666256();
	                    }
	
	                    $j.ajax({
	                        url: "/plugins/user-feedback/send-feedback",
	                        data: data,
	                        type: 'post',
	                        success: result,
	                        error: handleError
	                    });
                    }

                    return false;
                }
            
