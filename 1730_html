        $(function() {
            var url = 'https://www.hopital-saint-joseph.fr/e/saintjo_recherche_globale?f_1%5B0%5D=INPUTVALUE';
            var champ = $('#champ-facette-es_1');
            var searchFct = function() {
                window.location.href = url.replace('INPUTVALUE', encodeURIComponent(champ.val()));
            };
            champ.on('keypress', function(ev) {
                if (ev.which === 13) {
                   searchFct();
                }
            });
            $('#btn_champ-facette-es_1').on('click', function() {
                searchFct();
            });
        });
        
