		<script type="text/javascript">
		
	/**
	 * Animation du sous menu avec Javascript
	**/
	jQuery(document).ready(function($){
		// Images des tuiles cliquables 
		$(".bloc_accueil").on("click", "p", function( ) {
			window.location = $(this).find("a").attr("href");
		});
		// retour haut de page animation douce
		$(".bottom-to-top").click(function(){
			$("html,body").animate({scrollTop: "0px"}, 1500);
		});
		// Clic sur élément pour remonter en haut de l'affichage
		$("h5.et_pb_toggle_title").click(function(){
			var offset_top = $(this).offset().top-$("header").height()-$("div.et_pb_ariane").height()-$("div#titre-1-page").height();
			$("html,body").animate({scrollTop: offset_top}, 500);
		});
		// Sur le slider, ajout d'un bouton pour réduire la taille de celui-ci
		setTimeout(function() {
			$("div.et_pb_slider p > strong").append('<span class="retrecir_slider" onclick="agir_taille_slider($(this));">O');
		},1500);
		
		// Stockage des références des différents éléments dans des variables
		rocket1     = $('.sous-menu-dans-page');
		rocket2     = $('#actus');
		LAST_SCROLL_OFFSET = $(window).scrollTop();
		LAST_SCROLL_TIME   = new Date().getTime();
		
		// On déclenche un événement scroll pour mettre à jour le positionnement au chargement de la page
		$(window).trigger('scroll');
		
		$(window).scroll(function(event){
			// Calcul de la marge entre le haut du document et #rocket_mobile
			fixedLimit = rocket1.offset().top - parseFloat(rocket1.css('marginTop').replace(/auto/,0));
			fixedLimit = $("div.et_pb_section_1:first").offset().top - $("header").height()-$("div.et_pb_ariane").height()-$("div#titre-1-page").height();
		
			// Valeur de défilement lors du chargement de la page
			windowScroll = $(window).scrollTop();
			
			// Mise à jour du positionnement en fonction du scroll
			if( windowScroll >= fixedLimit ){
				rocket1.addClass('fixed');
				rocket2.addClass('fixed');
			} else {
				rocket1.removeClass('fixed');
				rocket2.removeClass('fixed');
			}
			
			// Mise à jour variables
			LAST_SCROLL_OFFSET = windowScroll;
			LAST_SCROLL_TIME   = new Date().getTime();
		});
		
		
	});
		</script>
		<script type="text/javascript">
	$('.sous-menu-dans-page a').click( function() {
		var page = $(this).attr('href'); 
		var speed = 750; // gérer la vitesse de défilement
        
        // Ici on retranche la hauteur du bandeau (200px dans ta feuille de styles)
		$('html, body').animate( { scrollTop: $(page).offset().top - 200 }, speed );
		return false;
	});
		</script>
