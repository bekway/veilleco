																											<script>

$(document).ready(function()
{
	if("3"==1)
	{
		$("#CAT0").trigger('click');
	}
	else
	{
		var cnt=$('body').html();
		$('body').prepend('<div id="popup" onclick="hidePP()">');
		initGotoFancy();
	}
});
  
function detail(ind)
{
	$('#popup').fadeIn();
	gotoAjax("admin/detail.php",{PID:ind},"popup","none","initGotoFancy","");
}

function loadTof(id)
{
	$("#CONT_PHOTO").html("");
	gotoAjax("admin/tofViewer.php",{PID:id},"CONT_PHOTO","tofLoader","initGotoFancy","");
}

function hidePP()
{
	$('#popup').fadeOut();
	$('#popup').html('');
}
