		<script type="text/javascript">
function setColorRound(id) {
    var mesSpan = document.querySelectorAll(".radio-696 > span > span > label > span");
  	for (i=0;i<mesSpan.length;i++) {
    	mesSpan[i].classList.remove("moreCss");
    }
    mesSpan[id].classList.add("moreCss");
}

jQuery(document).ready(function( $ ){
  	var mesSpan = document.querySelectorAll(".radio-696 > span > span > label > span");
	for (i=0; i<mesSpan.length; i++) {
      	mesSpan[i].setAttribute("onclick", "setColorRound("+i+")");
    }
});
		</script>
		<!-- end Simple Custom CSS and JS -->
		<!-- start Simple Custom CSS and JS -->
		<script type="text/javascript">
var note=0;
function updateColor(id) {
    var mesSpan = document.querySelectorAll(".radio-695 > span > span > label > span");
  	for (i=0; i<=id; i++) {
      mesSpan[i].style.color = "#f2b600";
    }
}

function resetColor() {
    var mesSpan = document.querySelectorAll(".radio-695 > span > span > label > span");
  	if (note!=0) { 
        for (i=(note+1); i<mesSpan.length; i++) {
      		mesSpan[i].style.color = "#bbb";
    	}
	} else {
    	for (i=note; i<mesSpan.length; i++) {
      		mesSpan[i].style.color = "#bbb";
    	}
    }
  	
}

function setColor(id) {
  	note=id;
    var mesSpan = document.querySelectorAll(".radio-695 > span > span > label > span");
  	for (i=0; i<=id; i++) {
      mesSpan[i].style.color = "#f2b600";
    }
}

jQuery(document).ready(function( $ ){
  	var mesSpan = document.querySelectorAll(".radio-695 > span > span > label > span");
	for (i=0; i<mesSpan.length; i++) {
    	mesSpan[i].setAttribute("onmouseover", "updateColor("+i+")");
      	mesSpan[i].setAttribute("onmouseout", "resetColor()");
      	mesSpan[i].setAttribute("onclick", "setColor("+i+")");
    }
});

		
		</script>
		<!-- end Simple Custom CSS and JS -->
		<!-- start Simple Custom CSS and JS -->
		<script type="text/javascript">
function updatePriceLabels(id) {
    //avoids slider overlap
    var sliders = document.querySelectorAll(".price-slider input");
    var val1 = parseInt(sliders[0].value);
    var val2 = parseInt(sliders[1].value);

    if (val1 >= val2 && id == 1) {
      sliders[0].value = val2 - 10;
      return;
    }
    if (val2 <= val1 && id == 2) {
      sliders[1].value = val1 + 10;
      return;
    }
    document.querySelector("#sba-rando-left").innerText = sliders[0].value;
    document.querySelector("#sba-rando-right").innerText = sliders[1].value;
}

function sbaReset() {
	
  	document.querySelector("#sba-locomotion-select > option:nth-child(1)").selected = 'selected';
   	document.querySelector("select[name='commune'] > option:nth-child(1)").selected = 'selected';
  	
  	document.querySelector("input[name='min']").value = document.querySelector("input[name='min']").min;
  	document.querySelector("#sba-rando-left").innerText = document.querySelector("input[name='min']").min;
  
  	document.querySelector("input[name='max']").value = document.querySelector("input[name='max']").max;
  	document.querySelector("#sba-rando-right").innerText = document.querySelector("input[name='max']").max;
  
  	document.getElementById("star-5").checked = false;
    document.getElementById("star-4").checked = false;
  	document.getElementById("star-3").checked = false;
  	update();
}

function update() {
  	//sliders[0] = distancemin et sliders[1] = distancemax
	var sliders = document.querySelectorAll(".price-slider input");
  
  	//valueSelect == 0 == apied et == 1 == velo
  	var select = document.querySelector("#sba-locomotion-select");
	var valueSelect = select.options[select.selectedIndex].value;
  	
  	//communes
  	var communeSelect = document.querySelector("select[name='commune']");
  	var commune = communeSelect.options[communeSelect.selectedIndex].text;
  	//difficulte
  	var note = '';
  	if(document.getElementById("star-5").checked) {
      	note = 'Difficile';
    } else if (document.getElementById("star-4").checked) {
      	note = 'Moyen';
    } else if (document.getElementById("star-3").checked) {
      	note = 'Facile';
    }
  
  	//recuperation front
  	//vignette	
  	var distancesObject = document.querySelectorAll(".sba-vignette");
  	//locomotions
  	var locomotions = document.querySelectorAll(".sba-p-locomotion");
  	//distances
 	var distancesP = document.querySelectorAll(".sba-distance > p");
  	//communes
  	var communes = document.querySelectorAll(".sba-commune");
  	//difficultes
  	var difficultes = document.querySelectorAll(".sba-difficulte");
  
 	for (var i=0;i<distancesObject.length;i++) {
      	var dist = parseInt(distancesP[i].innerHTML);
      	var loco=2;
      	if (locomotions[i].innerHTML == "À pied") loco = 0;
      	else loco = 1;
      	if (valueSelect == '') valueSelect = 2;
		if ((dist <= parseInt(sliders[1].value) && dist >= parseInt(sliders[0].value)) && ((loco == valueSelect) || (valueSelect==2)) && ((communes[i].innerHTML==commune) || (commune == 'Sélection commune')) && ((difficultes[i].innerHTML == note) || (note == ''))) {
        	distancesObject[i].style.display = "initial";
        } else {
        	distancesObject[i].style.display = "none";
        }
    }
}
		</script>
		<!-- end Simple Custom CSS and JS -->
