										<h2 class="grve-element grve-align-left grve-title-no-line grve-h2 titre titre-bleu titre-agenda" style="margin-bottom: 15px;"><span>Agenda</span></h2>
										<div class="grve-element grve-text vc_custom_1564130532223">
											<div class="carousel-agenda">Pas d'évènements prévus pour le moment.</div>
										</div>
										<div class="wpb_raw_code wpb_raw_js">
											<div class="wpb_wrapper">
												<script type="text/javascript">

(function($) {
  var agenda_list = $('.ecs-event-list li');
  var ul_height = $('.ecs-event-list').outerHeight();
  $('.ecs-event-list').append(agenda_list.clone());

  var i = 0;
  (function displayAgenda(i){
    setTimeout(function(){
      if( $('.ecs-event-list').css('top') == (-1 * ul_height) + 'px'){
        $('.ecs-event-list').css('top', '0');
      }
      var li_height = $(agenda_list[i]).outerHeight();
      $('.ecs-event-list').animate({
        top: '-=' + li_height + 'px'}, 1500);
      if( i == agenda_list.length - 1){
        i = 0;
      }else{
        i++;
      }
      displayAgenda(i);
      
    }, 3000);
  })(i);
  
})(jQuery);
												</script>
											</div>
