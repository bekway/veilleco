				$("body").prepend("<div id=\"cookiesck\" />");
				
				$("#cookiesck").append("<span class=\"cookiesck_inner\">En visitant ce site, vous acceptez l'utilisation de cookies afin de vous proposer les meilleurs services possibles.")
					.append("<a href=\"/cookies\"  target=\"\" id=\"cookiesck_readmore\">En savoir plus")
