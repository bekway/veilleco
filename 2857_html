      $.fn.socialSharePrivacy.settings.order = ['facebook', 'twitter', 'viadeo'];
      $.fn.socialSharePrivacy.settings.img_path_prefix = '/cookies/img/'; <!--very important to put the /-->
      $.fn.socialSharePrivacy.settings.css_path_prefix = '/cookies/css/'; <!--very important to put the /-->
      $.fn.socialSharePrivacy.settings.js_path_prefix = '/cookies/javascript/'; <!--very important to put the /-->
