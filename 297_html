	var acte_code_magic = 'cd593f4ebce3f28bc1458028';
	var urlListDelib = '/apps/portail_ged/liste_acte.php?collectivite=AGGLO&type=DELIB&CODEMAGIC='+acte_code_magic;
	var urlGetFichier = '/apps/portail_ged/download_acte.php?collectivite=AGGLO&type=DELIB&uid=--uid--&CODEMAGIC=cd593f4ebce3f28bc1458028';
	var mode_acte = "delib";
