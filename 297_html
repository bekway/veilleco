var localizationobj;

var getLocalization = function () {
	return localizationobj;
};

// Fonction de formatage des données ("format" JQX avant passation au tableau)
function FormatListeActes(liste_acte){
	var listeOK = Array();
	for(ii=0; ii<liste_acte.length; ii++){
		var acte_en_cours = liste_acte[ii];
		
		var urlLienActe = urlGetFichier.replace("--uid--", acte_en_cours.uid);
		acte_en_cours.lien_acte = "<a target='_blank' href='"+urlLienActe+"'><img src='/ressources//portail_ao_delib/images/fileicons/pdf.png' class='icon_rh_acte' />";
			if(acte_en_cours.collectivite == collectivite){
			listeOK[listeOK.length] = acte_en_cours;
		}
	}
	
	
	var JQsource =
	{
		root: "actes",
		localData: listeOK,
		dataType: "array",   
		dataFields:
		[
			{ name: 'numero_acte', type: 'string' },
			{ name: 'uid', type: 'string' },
			{ name: 'uidsecret', type: 'string' },
			{ name: 'lien_acte', type: 'string' },
			{ name: 'objet_acte', type: 'string' },
			{ name: 'transfert_controle', type: 'date', format: "yyyy-MM-dd"}
		],
		sortcolumn: 'transfert_controle',
		sortdirection: 'desc'
	};
		
	return JQsource;
}


function afficheDataActes(liste_acte){
	JQsource = FormatListeActes(liste_acte);
	
	$("#liste_acte").jqxDataTable(
	{
		width: "100%",
		pageable: true,
		autoRowHeight: true,             
		sortable: false,
		localization: getLocalization(),
		rowDetails: false,
		// initRowDetails: initRowActeDetails,         
		pagerButtonsCount: 20,
		pageSize: 20,
		source: JQsource,                   
		filterable: false,             
		filtermode: 'advanced',
		columnsResize: true,
		ready: function () {
			// expand the first details.
		},
		columns: [
		  { text: "N° Acte", dataField: 'numero_acte',  cellClassName: ''  ,  width: 130},
		  { text: "Objet", dataField: 'objet_acte',  cellClassName: ''  ,  width: "auto"},
		  { text: 'Date d\'affichage', dataField: 'transfert_controle',  cellClassName: ''  ,  cellsFormat: 'dd-MM-yyyy', width: 130},
		  { text: 'Télécharger', dataField: 'lien_acte',  cellClassName: 'align_center'  ,  width: 130}
		]
	});
	
	// $('#liste_acte').on('rowDoubleClick', detailDocument);
	
}

	
