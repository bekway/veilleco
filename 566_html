		<script type="text/javascript">
		 $(document).ready(function () {
		 
				 var taille = 12;
				 var augmentation = 3;
				 var tailleMax = 20;
				 var tailleMin= 12;
				 
				 $('#agrandir').click(function () {
				 taille +=augmentation;
				 if (taille >= tailleMax)
				 {
				 taille = tailleMax;
				 }
				 $('#motdupresident').stop().animate({fontSize: taille+"px"},300);
				 });
				 $('#diminuer').click(function () {
				 taille -=augmentation;
				 if (taille <= tailleMin)
				 {
				 taille = tailleMin;
				 }
				 $('#motdupresident').stop().animate({fontSize: taille+"px"},300);
				 });
		 
		 });
	
	 
		</script>
