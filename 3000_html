function afficheDescURL(toThis) 
  {
  if (document.getElementById)
    {
    document.getElementsByClassName("moreinfos").innerHTML = toThis;
    }
  else if (document.all) 
    {
    document.all["moreinfos"].innerHTML = toThis;
    }
  }

base = " Vous êtes sur la page d'accueil ";
afficheDescURL(base);

