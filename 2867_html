	if($(".message").length === 0){
		$("#bandeaux_urgence").hide();
		$(".message").hide();
	}

	function bandeaux_clear(){
		if($(".message").length === 1){
			//a.className = 'hide';
			//c.className = 'hide';
			$("#bandeaux_urgence").hide();
			$(".message").hide();
			document.cookie = "bandeau=false";
		}
	}
