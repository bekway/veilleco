	function test(input, checked, indexCat){
		// var idCat = $(this).data("input");
		deleteMarkers();
		if (checked){
			// console.log( input + " checked (index categorie = "+indexCat+")" );
			if (infowindow){
				infowindow.close();
			}
			setMarker(map, datas_maps[indexCat], indexCat);
		}else{
			// console.log( input + " unchecked (index categorie = "+indexCat+")" );
			if (infowindow){
				infowindow.close();
			}
			clearMarkers(datas_markers[indexCat],indexCat);
		}
	}

