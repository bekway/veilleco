					<script>
	var datas_maps = [];
	var datas_markers = [];
	var marker_loc = [];
	var markers_categories = [];
	var map = null;
	var infowindow;
	var shape = {
		coords: [1, 1, 1, 20, 18, 20, 18, 1],
		type: 'poly'
	};	
    function initialize() {
		var latlng = new google.maps.LatLng(48.862725, 2.287592000000018);
		var myOptions = {
			zoom: 13,
			center: latlng,
			mapTypeId: google.maps.MapTypeId.ROADMAP
		};
		//console.log("Map");
		map = new google.maps.Map(document.getElementById('gmap_canvas'), myOptions);
		var j = 1;
					datas_maps[j]  = [{"location_id":"1","location_title":"Rue des Dauphins","location_address":"8 Rue des Dauphins, 34470 P\u00e9rols, France","location_latitude":"43.56727000993539","location_longitude":"3.941534918315938","location_messages":""}];	
			markers_categories[j] = "https:\/\/www.paysdelor.fr\/wp-content\/uploads\/2019\/01\/pict-ggl-map.png";	
			datas_markers[j] = [];
			j += 1;
					datas_maps[j]  = [];	
			markers_categories[j] = "https:\/\/www.paysdelor.fr\/wp-content\/uploads\/2019\/01\/pict-ggl-map.png";	
			datas_markers[j] = [];
