            // Generate map
            var map = L.map('widget-map-canvas-1', { fullscreenControl: true }).setView([50.476060, 1.698867], 10),
                listMarkers = {};

            L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', {
                attribution: '© <a href="http://osm.org/copyright">OpenStreetMap'
            }).addTo(map);

            // Add markers
                                                // Generate index
                                                                
                    if(listMarkers["1"] === undefined){
                        listMarkers["1"] = {marker: [], popup: []};
                    }

                    // Add marker
