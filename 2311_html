                    marker.addTo(map);

                    // Add marker popup
                    marker.bindPopup("\u003Cbr\u003E\u003Cb\u003EZone\u0020d\u2019Activit\u00E9\u0020Opalopolis\u003C\/b\u003E\u003Cbr\u003E\u003Cbr\u003ERue\u0020Edouard\u0020L\u00E9v\u00EAque,\u003Cbr\u003E\u002062630\u0020Etaples\u002Dsur\u002DMer"); // Je ne mets pas de texte par défaut
                    var popup = marker.getPopup();
                    listMarkers["8"]['popup'].push(
                        marker
                    );

                                    // Generate index
                                                                
                    if(listMarkers["8"] === undefined){
                        listMarkers["8"] = {marker: [], popup: []};
                    }

                    // Add marker
                    var icon = L.icon({
                        iconUrl: "/bundles/bercksurmercmscore/img/map-icon/png/map-marker-icon-purple.png",
                        shadowUrl: false,

                        iconSize:     [32, 32], // size of the icon
                        shadowSize:   [32, 32], // size of the shadow
                        iconAnchor:   [16, 32], // point of the icon which will correspond to marker's location
                        shadowAnchor: [4, 62],  // the same for the shadow
                        popupAnchor:  [16, 0] // point from which the popup should open relative to the iconAnchor
                    });
                    var marker = L.marker([50.444366, 1.7646879999999783], {icon: icon});
                    listMarkers["8"]['marker'].push(
                        marker
                    );
                    marker.addTo(map);

                    // Add marker popup
                    marker.bindPopup("\u003Cp\u003E\u003Cbr\u0020\/\u003E\u003Cstrong\u003EZone\u0020d\u0026rsquo\u003BActivit\u0026eacute\u003B\u0020de\u0020Campigneulles\u002Dles\u002DPetites\u003C\/strong\u003E\u003Cbr\u0020\/\u003E\u003Cbr\u0020\/\u003E\u002062170\u0020Campigneulles\u002Dles\u002DPetites\u003C\/p\u003E"); // Je ne mets pas de texte par défaut
                    var popup = marker.getPopup();
                    listMarkers["8"]['popup'].push(
                        marker
                    );

                                    // Generate index
                                                                
                    if(listMarkers["8"] === undefined){
                        listMarkers["8"] = {marker: [], popup: []};
                    }

                    // Add marker
                    var icon = L.icon({
                        iconUrl: "/bundles/bercksurmercmscore/img/map-icon/png/map-marker-icon-purple.png",
                        shadowUrl: false,

                        iconSize:     [32, 32], // size of the icon
                        shadowSize:   [32, 32], // size of the shadow
                        iconAnchor:   [16, 32], // point of the icon which will correspond to marker's location
                        shadowAnchor: [4, 62],  // the same for the shadow
                        popupAnchor:  [16, 0] // point from which the popup should open relative to the iconAnchor
                    });
                    var marker = L.marker([50.41059505116808, 1.6540897861693793], {icon: icon});
                    listMarkers["8"]['marker'].push(
                        marker
                    );
                    marker.addTo(map);

                    // Add marker popup
                    marker.bindPopup("\u003Cp\u003E\u003Cbr\u0020\/\u003E\u003Cstrong\u003EZone\u0020d\u0026rsquo\u003BActivit\u0026eacute\u003B\u0020Champ\u0020Gretz\u003C\/strong\u003E\u003Cbr\u0020\/\u003E\u002062180\u0020Rang\u002Ddu\u002DFliers\u003C\/p\u003E\r\n\u003Cp\u003EContact\u003A\u0020Service\u0020D\u0026eacute\u003Bveloppement\u0020Economique\u002003.21.90.94.59\u003C\/p\u003E"); // Je ne mets pas de texte par défaut
                    var popup = marker.getPopup();
                    listMarkers["8"]['popup'].push(
                        marker
                    );

                            
                    }
    
											</script>
