		<script type="text/javascript">
	function favoris() 
	{
		if ( navigator.appName != 'Microsoft Internet Explorer' )
		{ 
			window.sidebar.addPanel('Site de la Communauté de Communes du Pays de Colombey Sud Toulois',"https://www.pays-colombey-sudtoulois.fr/",""); 
		}else 
		{ 
			window.external.addfavorite('https://www.pays-colombey-sudtoulois.fr/','Site de la Communauté de Communes du Pays de Colombey Sud Toulois')
		}
	}

	$(window).load(function() {
			$('#slider').nivoSlider();
		});

	$(document).ready(function() {
	
		//arrondi
		$('.arrondi').corner('right 7px');
		
		//calendrier 1
		var date = new Date();
		var d = date.getDate();
		var m = date.getMonth()+1;
		var y = date.getFullYear();
		
		$('#calendar').fullCalendar({
			header: {
				left: 'prev,next',
				center: 'title',
				right: 'month,basicWeek,basicDay'
			},
			firstDay:1,
			editable: true,
		
			events: "lib/php/calendrier/json-events.php?m="+m+"&y="+y
			
		});
		//end:calendrier 1
		
			});
		
		</script>
