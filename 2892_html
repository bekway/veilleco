/**
*	Gestion des menus/sous-menus
*/
	/**
	*	Function appearMenu
	*	Fait apparaitre les sous menus du menu sélectionné
	*	@param marginleft : positionnement du menu par rapport au coté gauche de l'écran
	*/
	function appearMenu(id_menu)
	{
		/**
		*	Génere le contenu du sous menu selon le menu sélectionné
		*/
		generateMenu(id_menu);
		
		/**
		*	On fait apparaitre le container de sous menus
		*/
		document.getElementById(containerName).style.display = '';	
		
		/**
		*	La position de la souris change, on est sur un menu
		*/
		onMenu = 1;
	}
	
	/**
	*	Function quitMenu
	*	La souris quitte le menu, mise à jour des variables de position
	*	et lancement de la fonction prepareToCloseMenu qui fermera le menu si il faut
	*/
	function quitMenu()
	{
		onMenu = 0;
		onSsMenu = 0;
		prepareToCloseMenu();
	}
	
	/**
	*	Function initSsMenu
	*	La souris passe sur un sous menu, mise à jour des variables de position
	*/
	function initSsMenu()
	{
		onMenu = 0;
		onSsMenu = 1;	
	}
	
	/**
	*	Function quitSsMenu
	*	La souris quitte le sous menu, mise à jour des variables de position
	*	et lancement de la fonction prepareToCloseMenu qui fermera le menu si il faut
	*/
	function quitSsMenu()
	{
		onMenu = 0;
		onSsMenu = 0;
		onSsSsMenu = 0;
		prepareToCloseMenu();
	}
		
	/**
	*	Function prepareToCloseMenu
	*	Temporisateur avant la fermeture effective du sous menu
	*	Lance la fonction de fermeture de sous menu après timeBeforeClosing millisecondes
	*/
	function prepareToCloseMenu()
	{
		setTimeout("closeMenu()", timeBeforeClosing);
	}
	
	/**
	*	Function closeMenu
	*	Fait disparaitre le sous menu si la souris n'est ni sur un menu, ni sur un sous menu
	*	Fait disparaitre le sous sous menu si la souris n'est pas dessus
	*/
	function closeMenu()
	{		
		if( (onSsMenu == 0)
			   &&(onMenu == 0)
			   		&&(onSsSsMenu == 0) )
			document.getElementById(containerName).style.display = 'none';
		
		if( (onSsSsMenu == 0) )
			document.getElementById(ssContainerName).style.display = 'none';
	}
	
	/**
	*	Function generateMenu
	*	génère le sous menu correspondant à la position de la souris
	* 	@param marginleft : positionnement du menu par rapport au coté gauche de l'écran
	*/
	function generateMenu(id_menu)
	{
		var ssMenuComplet = ''; 
		/*
		*	Selon le menu selectionné, on charge le sous menu correspondant
		*/		
		for(var i = 0; i < ssMenu[id_menu].length; i++)
			ssMenuComplet += ssMenu[id_menu][i][0];
			
		document.getElementById(containerName).innerHTML = ssMenuComplet;
		
		/**
		*	Placement et redimentionnement du sous menu
		*/
		switch(id_menu)
		{
			case 'm_1':
				document.getElementById(containerName).style.marginLeft = '0px';
				break;

			case 'm_2':
				document.getElementById(containerName).style.marginLeft = '116px';
				break;
				
			case 'm_3':
				document.getElementById(containerName).style.marginLeft = '232px';
				break;
				
			case 'm_4':
				document.getElementById(containerName).style.marginLeft = '348px';
				break;
				
			case 'm_5':
				document.getElementById(containerName).style.marginLeft = '464px';
				break;
			
			case 'm_6':
				document.getElementById(containerName).style.marginLeft = '580px';
				break;
		
			case 'm_7':
				document.getElementById(containerName).style.marginLeft = '696px';
				break;
				
			case 'm_8':
				document.getElementById(containerName).style.marginLeft = '633px';
				break;
				
			default:
				break;
		}
	}
/*
*	Fin de gestion menus/sous-menus
*/

/**
*	Gestion sous-menus/sous-sous-menus
*/

	/**
	*	Function initSsSsMenu
	*	La souris passe sur un sous sous menu, mise à jour des variables de position
	*/
	function initSsSsMenu()
	{
		onMenu = 0;
		onSsMenu = 0;
		onSsSsMenu = 1;
	}
	
	/**
	*	Function quitSsSsMenu
	*	La souris quitte le sous sous menu, mise à jour des variables de position
	*	et lancement de la fonction prepareToCloseMenu qui fermera le menu si il faut
	*/
	function quitSsSsMenu()
	{
		onMenu = 0;
		onSsMenu = 0;
		onSsSsMenu = 0;
		prepareToCloseMenu();
	}
	
	/**
	*	Function appearSsMenu
	*	Fait apparaitre les sous sous menus du sous menu sélectionné
	*	@param marginleft : positionnement du menu par rapport au coté gauche de l'écran
	*/
	function appearSsMenu(id_menu)
	{
		/**
		*	Génere le contenu du sous sous menu selon le sous menu sélectionné
		*/
		generateSsMenu(id_menu);
		
		/**
		*	On fait apparaitre le container de sous menus
		*/
		document.getElementById(ssContainerName).style.display = '';
		
		onSsSsMenu = 1;
	}
	
	/**
	*	Function generateSsMenu
	*	génère le sous-sous menu correspondant à la position de la souris
	*/
	function generateSsMenu(id_menu)
	{
		// On décompose l'id du menu pour trouver le bon tableau
		var explode_id = new Array();
		explode_id = id_menu.split('_');
		
		var racine = explode_id[0] + '_' + explode_id[1]; // premiere dimension du tableau
		var dim = explode_id[2]; // position du sous-sous menu
		
		var ssSsMenuComplet = ''; 
		/*
		*	Selon le menu selectionné, on charge le sous menu correspondant
		*/		
		for(var i = 1; i < ssMenu[racine][dim].length; i++)
			ssSsMenuComplet += ssMenu[racine][dim][i];
		
		document.getElementById(ssContainerName).innerHTML = ssSsMenuComplet;
		
		var padding = 32;
		
		if(navigator.appName=="Microsoft Internet Explorer")
			padding = 32;
		
		var pos_top = 162 + padding*(dim-1);
		/**
		*	Placement et redimentionnement du sous menu
		*/
		switch(racine)
		{
			case 'm_1':
				document.getElementById(ssContainerName).style.marginLeft = '284px';
				break;

			case 'm_2':
				document.getElementById(ssContainerName).style.marginLeft = '484px';
				break;
				
			case 'm_3':
				document.getElementById(ssContainerName).style.marginLeft = '539px';
				break;
				
			case 'm_4':
				document.getElementById(ssContainerName).style.marginLeft = '682px';
				break;
				
			case 'm_5':
				document.getElementById(ssContainerName).style.marginLeft = '304px';
				break;
				
			case 'm_6':
				document.getElementById(ssContainerName).style.marginLeft = '362px';
				break;
				
			case 'm_7':
				document.getElementById(ssContainerName).style.marginLeft = '464px';
				break;
				
			case 'm_8':
				document.getElementById(ssContainerName).style.marginLeft = '410px';
				break;
				
			default:
				break;
		}
		
		document.getElementById(ssContainerName).style.marginTop = pos_top + 'px';
	}
	
	function appearCommune()
	{
		/**
		*	On fait apparaitre le container de sous menus
		*/		
		$('#listCommunes').fadeIn('slow', function() {
        // Animation complete
      	});
		
		/**
		*	La position de la souris change, on est sur un menu
		*/
		onCommune = 1;
	}
	
	function quitCommune()
	{
		onCommune = 0;
		onMCommune = 0;
		prepareToCloseCommune();
	}
	
	function initCommune()
	{
		onCommune = 0;
		onMCommune = 1;	
	}
	
	function prepareToCloseCommune()
	{
		setTimeout("closeCommune()", timeBeforeClosing);
	}
	
	function closeCommune()
	{		
		if( (onMCommune == 0) &&(onCommune == 0) )
			$('#listCommunes').fadeOut(400, function() {
			// Animation complete
			});
	}
		</script>
