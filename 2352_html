						<script language="javascript">
var m = 0;
var a = 0;
window.onload = function(){

var xhr_object; 
	xhr_object = new XMLHttpRequest();
	xhr_object.open("GET", "agenda/fct/calendar.php?aj_m="+m+"&aj_a="+a+"", true);
	xhr_object.onreadystatechange = function() { 
   if(xhr_object.readyState == 4) {           
   		document.getElementById('calendrier').innerHTML = xhr_object.responseText;		           
   	} 
   }
var data = "./agenda/fct/calendar.php?aj_m=3&aj_a=2009";
xhr_object.send(data);
}


function calendar(vm,va){ 
 var xhr_object; 
	xhr_object = new XMLHttpRequest();
	xhr_object.open("GET", "agenda/fct/calendar.php?aj_m="+vm+"&aj_a="+va+"", true);
	xhr_object.onreadystatechange = function() { 
   if(xhr_object.readyState == 4) {           
   		document.getElementById('calendrier').innerHTML = xhr_object.responseText;		           
   	} 
   }
var data = "./agenda/fct/calendar.php?aj_m="+vm+"&aj_a="+va+"";
xhr_object.send(data);
}
						
