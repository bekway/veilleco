$(window).load(function(){   

$("a.lien-meteo").fancybox({
			type: "iframe",
			autoSize : true,
			width : "60%",
			maxWidth: 270,
			maxHeight: 300
		});

resize_submenu();

});

$( window ).resize(function() {
resize_submenu();
});

function resize_submenu(){
$(".menu-principal-horizontal-submenu").each(function(){
   $(this).css({"display":"block",'visibility': 'hidden'});
   var sizeMe = child_size(this);

   if($(this).height() < sizeMe)
      $(this).height(sizeMe);

   $(this).css({"display":"none",'visibility': 'visible'});
});
}


function child_size(submenu){
   var menu_gauche = $(submenu).find(".menu-gauche");
   var content = $(menu_gauche).find(".menu-gauche-content");
   return (content.height());
}
		
