		</script>
		<script type="text/javascript" charset="utf-8">
  $(window).load(function() {
	  	
		$( "ul#listedescommunes a" ).click(function() {
		  $('g#situation').removeClass('visible').addClass('invisible');
		  $('g#contours').removeClass('invisible').addClass('visible');
		  $(this).toggleClass('off on');
		  var id= this.id;
		  $("#contours path#z"+id).toggleClass('off on');
		  $("#titres g#b"+id).toggleClass('off on');
		});
		
		
		var open_item=$('empty');
		$('ul.infos').hide();
		$('ul#listedescommunes a.off').click(function(){
			var li = $(this.parentNode);
			if ($('ul.infos',li).is(':hidden')){
				$('ul.infos',open_item).slideUp('slow');
				$('ul.infos',li).slideDown("slow");
				open_item=li;
			}
			else{
				$('ul.infos',open_item).slideUp('slow');
				open_item=$('empty');
			}
			return false;
			});
		$('ul#listedescommunes a.on').click(function(){
			var li = $(this.parentNode);
			if ($('ul.infos',li).is(':hidden')){
				$('ul.infos',open_item).slideUp('slow');
				$('ul.infos',li).slideDown("slow");
				open_item=li;
			}
			else{
				$('ul.infos',open_item).slideUp('slow');
				open_item=$('empty');
			}
			return false;
			});

});
		</script>
