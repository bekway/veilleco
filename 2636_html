			<script type="text/javascript">
  function getInternetExplorerVersion(){
  // Returns the version of Internet Explorer or a -1 for other browsers.
          
    var rv = -1;
    if(navigator.appName == 'Microsoft Internet Explorer')    {
      var ua = navigator.userAgent;
      var re  = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
      if(re.exec(ua) != null){
        rv = parseFloat( RegExp.$1 );
      }
    }
    return rv;
  }
  
  function checkSupportNavigateur() {
    var notSupport=false;
    var versionIe = getInternetExplorerVersion();
    //c'est un IE
    if(versionIe!=-1){
      if(versionIe<=8){
        notSupport=true;
      }
    }
    return notSupport;
  }
  
  if(checkSupportNavigateur()){
    var newLocation = window.location.href;
    
    // On retire les paramètres si on en a
    var parameterBeginIndex = newLocation.indexOf("?");
    if( parameterBeginIndex != -1) newLocation = newLocation.substr(0, parameterBeginIndex);
    
    // Ajout du paramètre portal=xxx
    window.location = newLocation + "?portal=p2_56366";
  };
