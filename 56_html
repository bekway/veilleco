$(document).ready(function () {openData.faq();
var mapf = L.map('mapf');
            mapf.zoomControl.setPosition('bottomright');
            L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png').addTo(mapf);
            var m = tmpm = L.marker([47.390714, 0.688753]).addTo(mapf).bindPopup('<div class=\'\'textInfoBulle\'\'><h3>Mairie Centrale - Hôtel de Ville<span>1 à 3 rue des Minimes  37926 TOURS CEDEX 9<span><a href=https://www.tours.fr/annuaire/695/9-mairie-centrale-hotel-de-ville.htm class=\'btn-primary\'>En savoir plus');
            tmpm.setIcon(L.icon({iconUrl:'/images/GBI_TOURS/marker-map.png',iconSize:[23,34],iconAnchor:[11,34],popupAnchor:[0,-34]}));
            tmpm.on('mouseover',function(){this.setIcon(L.icon({iconUrl:'/images/GBI_TOURS/marker-map-hover.png',iconSize:[23,34],iconAnchor:[11,34],popupAnchor:[0,-34]}));});
            tmpm.on('mouseout',function(){this.setIcon(L.icon({iconUrl:'/images/GBI_TOURS/marker-map.png',iconSize:[23,34],iconAnchor:[11,34],popupAnchor:[0,-34]}));}); 
            ;
            mapf.setView(m.getLatLng(),13);
            
tagAnalyticsCNIL.CookieConsent.start();
});
