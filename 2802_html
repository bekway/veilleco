																					<script>
			jQuery(document).ready(function($){
				var dates_options_html = $('#bloc-recherche-deliberations [name="date"]').html();
				$('#bloc-recherche-deliberations [name="annee"]').on('change',function(){
					var annee = $(this).val();
					$('#bloc-recherche-deliberations [name="date"]').html(dates_options_html);
					$('#bloc-recherche-deliberations [name="date"] option').each(function(){
						if ($(this).attr('annee') && $(this).attr('annee')!=annee)
							$(this).remove();
					});
				}).trigger('change');
			});
		
