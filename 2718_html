				<script type="text/javascript">

    function select_cat(_select)
    {
        var id = _select.find('option:selected').val();
        
        $('#map .cursor').each(
            function()
            {
                var cat = $(this).attr('rel').split('-');
                var val = false;
                
                for(var i = 0; i < cat.length; i++)
                {
                    if(Number(cat[i]) == id)
                    {
                        val = true;
                        break;
                    }
                }
                
                $(this).css('display', val ? 'block' : 'none');
            }
        );
    }
				</script>
