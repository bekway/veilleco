	        var beforeShowDay9337567617436473 = function(date)
	        {
	            var events = events9337567617436473;
	            var eventsCount = 0;
	            for (var i = 0; i < events.length; i++)
	            {
	                if (isEvent(date, events[i]))
	                {
	                    eventsCount++;
	                }
	            }
	            return [eventsCount > 0, '', 'Événements : ' + eventsCount];
	        }
	        
	        var onSelect9337567617436473 = function(dateText, inst)
	        {
	            var events = events9337567617436473;
	            
	            var date = $j.datepicker.parseDate('yy-mm-dd', dateText);
	
	            if (samePage9337567617436473)
	            {
	                var html = '';
	                var eventsToShow = [];
	                
	                for (var i = 0; i < events.length; i++)
	                {
	                    var month = $j.datepicker.formatDate('yy-mm', date);
	                    
	                    if (isEvent(date, events[i]))
	                    {
	                        var id = 'event-' + month + '-9337567617436473-' + events[i].eventId;
	                        eventsToShow.push("<li>" + $j('#' + id).html() + "");
	                    }
	                }
	                // Limit to 3 events
	                var max = eventsToShow.length > 3 ? 3 : eventsToShow.length;
	                for (var i=0; i < max; i++) {
	                	html += eventsToShow[i];
	                }
	                
	                if (html != '')
	                {
	                    $j("#day-events-9337567617436473 .date").html($j.datepicker.formatDate('d MM yy', date));
	                    $j("#day-events-9337567617436473 .list").html(html);
	                    $j('.events').show();
	                }
	                else
	                {
	                    $j('.events').hide();
	                }
	            }
	            else
	            {
	                var dateStr = $j.datepicker.formatDate('yy/mm/dd', date);
	                var eventsUrl = '/fr/_plugins/calendar/page/' + dateStr + '/' + encodeURIComponent(encodeURIComponent('zoneItem://1f5606d8-e5ec-4ba5-9e88-145eb1faf09f')) + '/events_1.3.html';
	                
	                          document.location.href = eventsUrl;
	                    
	            }
	        }
	        
	        var isEvent = function(date, event)
	        {
	            var isEvent = false;
	            
	            var start = $j.datepicker.parseDate('yy-mm-dd', event.start);
	            var end = $j.datepicker.parseDate('yy-mm-dd', event.end);
	            
	            if (start == null && end != null)
	            {
	                isEvent = date.getFullYear() == end.getFullYear() && date.getMonth() == end.getMonth() && date.getDate() == end.getDate();
	            }
	            else if (start != null && end == null)
	            {
	                isEvent = date.getFullYear() == start.getFullYear() && date.getMonth() == start.getMonth() && date.getDate() == start.getDate();
	            }
	            else if (start != null && end != null)
	            {
	                isEvent = (date.getTime() >= start.getTime() && date.getTime() <= end.getTime());
	            }
	            
	            return isEvent;
	        }
	        // Display last events
   			onSelect9337567617436473($j.datepicker.formatDate("yy-mm-dd", new Date()));
        
    	$j(function() {
            $j("#all-events-9337567617436473").hide();
            $j("#calendar-9337567617436473").datepicker({
                
                minDate: $j.datepicker.parseDate('yy-mm-dd', '2020-06-01'),
                maxDate: $j.datepicker.parseDate('yy-mm-dd', '2022-08-01'),
                
                
                showOtherMonths: false,
                
                selectOtherMonths: true,
                dayNamesMin: [ "Dim", "Lun", "Mar", "Mer", "Jeu", "Ven", "Sam" ],
                
                dateFormat: 'yy-mm-dd',
                
                beforeShowDay: beforeShowDay9337567617436473,
                onSelect: onSelect9337567617436473
            });
            // Remove unwanted class
            $j('.ui-datepicker-calendar > tbody > tr > td').each(function(key, value) {
            	if ($j(value).hasClass('ui-state-disabled')) {
            		$j(value).removeClass('ui-state-disabled');
            	}
           	});
           	// Add wanted class
           	$j('.ui-datepicker-calendar td').each(function(key, value) {
           		if ($j(value).data('event') === 'click') {
           			$j(value).addClass('event');
           		}
           	});
        });
    
// -->
		</script>
		<script type="text/javascript"><!--

			$j(function() {
				// Limit the function to others pages than homepage
				if ($j('body.home').length <= 0)
					trunkAbstract();
				
				$j('.pagination .page input').on('click', function () {
					trunkAbstract();
				});
				$j('.pagination .nav-button input').on('click', function () {
					trunkAbstract();
				});	
			});
			
			function trunkAbstract() {
				
				if ($j('.filtered-contents-link.service').length) {
					$j('p.content-abstract').trunk8({lines: 1});
				}
				else if ($j('.filtered-contents-link.article.faq'.length)) {
					$j('p.content-abstract').trunk8({lines: 2});
				}
				else if ($j('.article.faq'.length)) {
					$j('p.content-abstract').trunk8({lines: 3});  
				} else if ($j('.article'.length)) {
					$j('p.content-abstract').trunk8({lines: 2});
				} 
				
				// Crop more text if title is on 2 lines
				$j('.summary').each(function() {
					if ($j(this).height() > 30) {
						$j(this).parent().parent().find('p.content-abstract').trunk8({lines: 2});
					} else
						$j(this).parent().parent().find('p.content-abstract').trunk8({lines: 3});
				});
				
				$j('p.content-abstract').addClass('trunk8');
			}
		
// -->
		</script>
		<script type="text/javascript"><!--
            
            $j(document).ready( function() {
                $j('#carousel-6122879748822729').carousel( {
                 
                    // Is pagination available
                    pagination: true,
                    // The position of the pagination : outsite or inside
