		<script type="text/javascript">

$(function(){

$("a.minicine").mouseover(function(){

if($(this).attr("rel")=="") return false;

$("body").append('<span class="infobulle">');
var bulle=$(".infobulle");
//bulle.append($(this).attr("title"));

bulle.append('<img src="'+$(this).attr("rel")+'" />');

var postop= $(this).offset().top+$(this).height()+5;
var posleft=$(this).offset().left-$(this).width();

bulle.css({
left:posleft,
top:postop,
opacity:0
});


bulle.animate({
opacity:0.99
},1000);


});




$(".minicine").mouseout(function(){


var bulle=$(".infobulle").remove();;

});



});
