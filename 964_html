								<script type="text/javascript">
var urlLoadingAutoComplete = "l-agglo/decider-et-agir/le-conseil-communautaire-meuse-grand-sud/deliberations-du-conseil-communautaire.html?tx_cimsearch_displaybandeau%5Baction%5D=loadingAutocomplete&tx_cimsearch_displaybandeau%5Bcontroller%5D=Search&tx_cimsearch_displaybandeau%5Bformat%5D=html";
var timeOutVar;
jQuery(document).ready(function($) {
	
	 $('#tx_cimsearch_form_general input[type="text"]').autocomplete({
		source: function( request, response ) {
			$.ajax({
				url: urlLoadingAutoComplete+"&"+$('#tx_cimsearch_form_general').serialize(),
				type: 'GET',
				data: {
						"tx_cimsearch_displaybandeau[other_field][title,description]": request.term
					},
				success: function( data ) {
					data= $.parseJSON(data)

					response( data );
				}
			});
		},
		minLength: 3,
		select: function( event, ui ) {

			$(this).val(ui.item.label);
			
		},
		open: function() {
			$(this).autocomplete('widget').css('z-index', 1000);
			$( this ).removeClass( "ui-corner-all" ).addClass( "ui-corner-top" );
		},
		close: function() {
			$( this ).removeClass( "ui-corner-top" ).addClass( "ui-corner-all" );
		}
	});

});
								</script>
