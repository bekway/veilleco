	$(document).ready(function() {
		
		$('.banniereTitrePage .banniereWrapper1')
			.delay(500)
			.css('opacity', 0)
			.animate(
				{ opacity: 1 },
				{ duration: 1000 }
				);
							
		$('.banniere .banniereWrapper1')
			.delay(500)
			.css('opacity', 0)
			.animate(
				{ opacity: 1 },
				{ duration: 1000 }
				);
								
		$('.banniere .banniereWrapper2')
			.delay(1500)
			.css('opacity', 0)
			.animate(
				{ opacity: 1 },
				{ duration: 1000 }
				);		
				
		$('.banniere .banniereWrapper3')
			.delay(2000)
			.css('opacity', 0)
			.animate(
				{ opacity: 1 },
				{ duration: 1500 }
				);

				
		$('.banniereWrapperTexteInner span.scroll').on('click', function(){
			var winH = $(window).height();
			$('html,body').animate({scrollTop: +winH},1500);
		  });
		
		});	
