		<!-- fin pour lien 1er niveau -->
		<!-- menu gauche -->
		<script type="text/javascript">
			jQuery(function($){
				$("#icon-menu-gauche").click(function(e){
					e.preventDefault();
					var $left = $("#gauche").position().left;
					if($left == -380){
						$("#gauche").css('left',0);
					}else{
						$("#gauche").css('left',-380);
					}
					
				});
			});
		</script>
		<!-- menu gauche -->
