		<!-- script Antrenas -->
		<script>

function appearAntrenas() {
	
	
	var infosCommunes=document.getElementsByClassName("modInfosCommunes ");
	var i;

	for (i=0;i<infosCommunes.length;i++){
		infosCommunes[i].classList.add("noDisplayCommunes");
	}
	
	var antrenasVisible=document.getElementsByClassName("infosAntrenas");
	antrenasVisible[0].classList.remove("noDisplayCommunes");
	
	
}
		</script>
		<!-- script Bourgs sur Colagne -->
