	for (i=0;i<infosCommunes.length;i++){
		infosCommunes[i].classList.add("noDisplayCommunes");
	}
	
	var recouleFumasVisible=document.getElementsByClassName("infosRecoulesFumas");
	recouleFumasVisible[0].classList.remove("noDisplayCommunes");
	
	
}
		</script>
		<!-- script Saint-Bonnet de Chirac  -->
		<script>

function appearStBonnetChirac() {
	
	
	var infosCommunes=document.getElementsByClassName("modInfosCommunes ");
	var i;

	for (i=0;i<infosCommunes.length;i++){
		infosCommunes[i].classList.add("noDisplayCommunes");
	}
	
	var stBonnetChiracVisible=document.getElementsByClassName("infosStBonnetChirac");
	stBonnetChiracVisible[0].classList.remove("noDisplayCommunes");
	
	
}
		</script>
