								<script type="text/javascript">

$(document).ready(function() {
	/* BDXFR-1352 : Affichage ou pas des liens réseaux sociaux. */
    if ($("#partageReseauxSociauxAMasquer").val()){
        $("#partageReseauxSociauxAAfficher").hide();
    }
});
$(document).ready(function() {
    $("A.bdx_share_button_facebook").each(function() {
        this.href = "https://www.facebook.com/sharer/sharer.php?u=" + encodeURIComponent(window.location.href);
    });
    $("A.bdx_share_button_twitter").each(function() {
        var text = $("meta[name='twitter:title']").attr("content");
        //if (!text) { text = $("title").text(); }
        var href = "https://twitter.com/intent/tweet/?url=" + encodeURIComponent(window.location.href);
        if (text) { href += "&text=" + encodeURIComponent(text); }
        this.href = href;
    });
    $("A.bdx_share_button_mail").each(function() {
		var ogTitlePourMel = $("meta[property='og:title']").attr("content");
		this.href = "mailto:?subject=bordeaux.fr - " + ogTitlePourMel + "&body=%0D%0A\"" + ogTitlePourMel + "\" sur bordeaux.fr : " + encodeURIComponent($("link[rel='canonical']").attr("href"));
    });
    $("A.bdx_share_button").each(function() {
        this.onclick = function() { window.open(this.href, "Partager", "height=400,width=600"); return false; }
    });
});
								</script>
