        function triggerMapLoaded() {
            /* Event polyfill on IE <= 11 */
            (function () {
                function CustomEvent(e, params) {
                    params = params || { bubbles: false, cancelable: false, detail: undefined };
                    const evt = document.createEvent('CustomEvent');
                    evt.initCustomEvent(e, params.bubbles, params.cancelable, params.detail);
                    return evt;
                }

                CustomEvent.prototype = window.Event.prototype;

                window.CustomEvent = CustomEvent;
            })();
            // Create a new event
            var event = new CustomEvent('google:mapLoaded');
            // Dispatch the event
            document.dispatchEvent(event);
        }
    
		</script>
		<script>
        $(function () {
            // Accessibility
            var fontSize = 0;
            var contrast = 0;

            if (fontSize != 0) $("html").addClass("s" + fontSize);
            if (contrast != 0) $("html").addClass("c" + contrast);

            // chat bot
            var $chatBotButton = $('.chatbot-btn');

            $chatBotButton.click(function () {
                if ($("#Pauline").attr('src') === undefined) {
                    $("#Pauline").attr('src', 'https://pauline.pau.fr');
                }
                $chatBotButton.hide();
                $("html, body").addClass("overflow-hidden-mobile");
                $("meta[name=viewport]").attr('content', 'width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0');
                $('#bot').show();
                $('#bot-close-btn').click(closeChat);
            });

            function closeChat() {
                $("html, body").removeClass("overflow-hidden-mobile");
                $("meta[name=viewport]").attr('content', 'width=device-width, initial-scale=1.0, , shrink-to-fit=no');
                $chatBotButton.show();
                $('#bot').hide();
            }

            // Load menu asychronously
            $('.nav-link-menu').click(function () {
                var url = '/Menu/GetMenu' + '/' +  $(this).attr('data-menu-index');
                if ($(this).attr('data-menu-loaded') === 'false') {
                    $('#' + $(this).attr('id') + 'Content').load(url, function () {
                        // mega menu mobile accordions // ALL PAGES
                        if (matchMedia) {
                            var mq = window.matchMedia("(min-width: 768px)");
                            mq.addListener(WidthChange);
                            WidthChange(mq);
                        }
                    });
                }
            });

            // mega menu show/hide // ALL PAGES
            var $navBarButton = $('#navbarButton'),
                $navBarCloseButton = $('#navbarCloseButton'),
                $megaMenu = $('#megaMenu'),
                $body = $('body');
            $navBarButton.click(function () {
                $body.prepend('<div class="overlay">');
                $('.overlay').animate({
                    width: $(window).width()
                }, 200, 'easeOutQuad', function () {
                    $megaMenu.show().animate({
                        opacity: 1
                    }, 200);
                });
                $body.addClass('mega-menu-open');
                $('.nav-link-menu.active').click();
            });
            $navBarCloseButton.click(function () {
                $body.removeClass('mega-menu-open');
                $megaMenu.animate({
                    opacity: 0
                }, 200, 'easeOutBounce', function () {
                    $megaMenu.hide();
                    $('.overlay').animate({
                        width: 0
                    }, 200, function () {
                        $('.overlay').remove();
                    });
                });
            });

            function WidthChange(mq) {
                if (mq.matches) {
                    $(".mega-menu h3 a").addClass('prevent-click');
                    $(".mega-menu h3 + ul").removeClass("collapse");
                } else {
                    $(".mega-menu h3 a").removeClass('prevent-click');
                    $(".mega-menu h3 + ul").addClass("collapse");
                }
            }

            // mega menu resize on window resize // ALL PAGES
            function resizeMegaMenu() {
                if ($('.mega-menu-open').length) {
                    $('.overlay').css('width', $(window).width());
                }
            }
            window.addEventListener("resize", function () {
                setTimeout(function () {
                    resizeMegaMenu();
                }, 50);
            });

            // sticky nav toggle // ALL PAGES
            var $header = $('.site-header');
            var $stickyNav = $('.navbar');
            $(window).scroll(function (event) {
                var scroll = $(window).scrollTop();
                if (scroll > $stickyNav.height()) {
                    $stickyNav.addClass('sticky');
                } else {
                    if (!$header.hasClass('expand')) {
                        $stickyNav.removeClass('sticky');
                    }
                }
            });
        });
        function goBack() {
            window.history.back();
        }
        function showSatisfactionPauline() {
            $('#popinSatisfaction').load('/satisfactionPopin', function () {
                $('#popinSatisfaction').modal('show');
            });
        }
    
		</script>
		<script>
            // sticky search toggle on mobile // ALL PAGES BUT SEARCH PAGE
            let $stickySearch = $('#collapsibleSearch'),
                $siteWrapper = $('.site-wrapper');
            $(window).scroll(function (event) {
                let scroll = $(window).scrollTop();
                if (scroll > $stickySearch.height() && $(document).width() < 768) {
                    $stickySearch.addClass('sticky');
                    $siteWrapper.addClass('sticky-shown');
                } else {
                    $stickySearch.removeClass('sticky');
                    $stickySearch.removeClass('show');
                    $siteWrapper.removeClass('sticky-shown');
                }
            });
        
		</script>
	</body>
</html>
