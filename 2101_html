		<script>
        var elem = document.querySelector('.infos-pratiques .show .masonry');

        if (elem != null) {
            var msnry = new Masonry(elem, {
                itemSelector: '.item',
                gutter: 0
            });
        }

        $('.infos-pratiques .collapse').on('shown.bs.collapse', function () {
            $(this).find('.masonry').masonry({
                itemSelector: '.item',
                gutter: 0,
                stagger: 0
            });
