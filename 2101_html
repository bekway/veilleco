											<script type="text/javascript">
    $(function () {
        $("#btn-like-787").on("click", function () {
            var btn = $(this);
            var articleId = btn.attr('data-id');
            //Si le navigateur accepte les cookies et que articleId est renseigné
            if (navigator.cookieEnabled && articleId != '') {
                //disabled button
                btn.attr("disabled", "disabled");
                $.ajax({
                    type: "POST",
                    url: '/article/like/' + articleId,
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {
                        if (data.total != null) {
                            $("#btn-like-787 .like-count").html(data.total);
                            var icon = (data.action == "add") ? "icon liked-icon" : "icon like-icon";
                            $("#btn-like-787 .icon").removeClass().addClass(icon);
                        }
                        btn.removeAttr("disabled");           
                    },
                    error: function () {
                        //alert("Error while inserting data");
                    }
                });
                return false;
            }
        });
    });
											</script>
