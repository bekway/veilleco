    $(".btn-all").on('click', function (e) {
        e.preventDefault();
        e.stopImmediatePropagation();
        LaunchSearch();
    });

    $('#expandSearch').on('keypress', function (e) {
        e.stopImmediatePropagation();
        var keyCode = e.keyCode || e.which;
        if (keyCode === 13) {
            LaunchSearch()
            return false;
        }
    });

    $('#collapsibleSearch').on('shown.bs.collapse', function () {
        $("#txt-search").focus();
    })

    function LaunchSearch() {
        var search = $('#txt-search').val().replace(/[^\w\sÀ-ÿ-'’]/gi, ' ').trim();
        search = search.replace(/\s\s+/g, ' ');
        if (search && search.length >= 3) {
            window.location.href = '/rechercher/' + search + "?_source=user";
        }
    }

    //
    function LoadData(data) {
        $("#search-result-loader").hide();
        $("#search-result-container").html('');
        $(".count").html(data.total);
        $.each(data.articles, function (i, item) {
            var url;
            var target = "_self";
            var type = "<span>" + item.type + "";
            switch (item.type) {
                case "Cartes":
                    url = "/cartes/" + item.id;
                    break;
                case "Association":
                    url = "/associations/" + item.url;
                    type = "<span style='background-color:#7f2fa5; color:#ffffff;'>" + item.type + "";
                    break;
                case "Site_externe":
                    var site = externalSites.filter(function (value) { return value.Name == item.category; })[0];
                    url = item.url;
                    target = "_blank";
                    type = "<span style='background-color:"+ site.ColorTag +"; color:"+site.ColorText+"; padding: 0 1rem;'>" + item.category + "";
                    break;
                default:
                    url = "/article/" + item.url;
                    break;
            }
            var rows = "<li class='list-group-item'>"
                + "<a onclick='saveUserSearch(" + data.score + "," + data.median + ")' href='" + url + "' target='" + target +"'>" + item.title + ""
            + type
            + "";
            $('#search-result-container').append(rows);
            $('.search-result').show();
        });
    }

    function saveUserSearch(score, median) {
        if (termSearch != "") {
            $.ajax({
                type: "GET",
                url: 'https://www.pau.fr/article/savesearch/' + termSearch + '/' + score + '/' + median + '/',
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                success: function (data) {
                   // Console.log("OK");
                },
                error: function () {
                   // Console.log("Error");
                }
            });
        }
    }

    $("#txt-search").on('keyup', function () {
        var search = $('#txt-search').val().replace(/[^\w\sÀ-ÿ-'’]/gi, ' ').trim();
        if (search && search.length >= 3) {
            if (termSearch != search) {
                termSearch = search;
                $("#search-result-loader").show();
                $('.search-result').hide();
                $.ajax({
                    type: "GET",
                    url: 'https://api.pau.fr/api/article/search/' + search,
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {
                        LoadData(data);
                    },
                    error: function () {
                        //alert("Error while inserting data");
                    }
                });
            }
        }
        else {
            $('.search-result').hide('fast');
            $(".count").html('0');
            $("#search-result-container").html('');
        }
    });
						</script>
