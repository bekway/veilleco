/* js pour que on capte le premier mot et qu'on le mette entre des span */

jQuery('body.tm-dropcap .big-title h3.uk-panel-title , body.home .uk-article h2,body.home .uk-article h3,body.home .uk-article h4, .uk-article h1, h1,.list-conseil h3').each(function(index) {
    //get the first word
    var firstWord = jQuery(this).text().split(' ')[0];

    //wrap it with span
    var replaceWord = "<span class='title-custo-accueil'>" + firstWord + "";

    //create new string with span included
    var newString = jQuery(this).html().replace(firstWord, replaceWord);

    //apply to the divs
    jQuery(this).html(newString);
});



/* Tableaux responsive Boostrap pour pages article */
/* ajouter class "table" au tableau */
jQuery( "article.uk-article .tm-article .cck-f100.cck-pad-8 .cck-line-body table" ).addClass( "table" );
/* envelopper tableau dans div avec class "table-responsive" */
jQuery( "article.uk-article .tm-article .cck-f100.cck-pad-8 .cck-line-body table" ).wrap( "<div class='table-responsive'>" );

