	jQuery(".list-videos-small, .list-videos-small img, .list-videos-small div").click(function(e) {
		
		jQuery( ".list-videos div:not(.slick-current)  iframe" ).each(function( index ) {
			var src = jQuery( this ).attr("src");
			jQuery( this ).attr("src", '');
			jQuery( this ).attr("asrc", src);
			
			var asrc = jQuery( this ).attr("asrc");
			jQuery( this ).attr("src", asrc);
			jQuery( this ).attr("asrc", '');
		});
		
