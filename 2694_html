				  $( "#cookies-warning" ).hide();
				});

				$("#accepteCookies" ).click(function() {

					$.cookie('acceptedCookie', 'true');
				  	
				  	$("#cookies-warning" ).hide();
				});
			});
		
		</script>
	</body>
</html>
