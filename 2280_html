		<script>
        var coord = [];
        
                            var coordCurrent = {
                    "type" : "Feature",
                    "properties": {
                        "message": "43 rue de la République 88260 Darney \n 03 29 09 43 16 \n contact@vosgescotesudouest.fr",
                        "iconSize": [50, 50]
                    },
                    "geometry": {
                        "type": "Point",
                        "coordinates": [
                            6.046138499999984, 48.0855433
                        ]
                    }
                };
                coord.push(coordCurrent);
                    
        var geojson = {
            "type": "FeatureCollection",
            "features": coord
        }
        var icons = "http://www.vosgescotesudouest.fr/Views/images/markerDefault.png";

        mapboxgl.accessToken = 'pk.eyJ1IjoiZHNvZnQiLCJhIjoiY2psaHB1NnduMDdmMjN3b2U2d2F2YXdjayJ9.mFimkT2vfL1VQ94KoI7BdA';
        var map = new mapboxgl.Map({
            container: 'map',
            style: 'mapbox://styles/mapbox/streets-v10',
            center: [6.046138499999984,48.0855433],
            zoom: 10.5
        });

        geojson.features.forEach(function(marker) {
            // create a DOM element for the marker
            var el = document.createElement('div');
            el.className = 'marker';
            el.style.backgroundImage = 'url('+icons+')';
            el.style.width = marker.properties.iconSize[0] + 'px';
            el.style.height = marker.properties.iconSize[1] + 'px';
            el.addEventListener('click', function() {
                window.alert(marker.properties.message);
            });
            // add marker to map
            new mapboxgl.Marker(el)
                .setLngLat(marker.geometry.coordinates)
                .addTo(map);
        });
    
		</script>
