		<script>
							
			

			// gestion des tableaux de données en responsive
			var headertext = [],
			headers = document.querySelectorAll("table.table-data thead tr th, table.table-data thead tr td"),
			tablerows = document.querySelectorAll("table.table-data th"),
			tablebody = document.querySelector("table.table-data tbody");

			if (headers !== null && tablebody !== null) {
				for(var i = 0; i < headers.length; i++) {
					var current = headers[i];
					headertext.push(current.textContent.replace(/\r?\n|\r/,""));
				}
				for (var i = 0, row; row = tablebody.rows[i]; i++) {
					for (var j = 0, col; col = row.cells[j]; j++) {
						if (headertext[j] !== undefined) {
							col.setAttribute("data-th", headertext[j]);
						}
					}
				}
			}

			// config variables
			var delay = 10000, // DOM load breakpoint in millisecond
			  pxratio = 2; // pixel-ratio breakpoint

			if (window.addEventListener) {
			  window.addEventListener('load', domload);
			}

			function domload() {
			  // navigation timing begins
			  var perf = window.performance.timing;

			  if (perf !== undefined) {
				var timing = perf.domComplete - perf.domLoading,
				  imgs, i;
				if (timing <= delay) {
				  // now pixel density detect
				  //q = "(-webkit-min-device-pixel-ratio: " + pxratio + "), (min-resolution: " + pxratio * 96 + "dpi), (min-resolution: " + pxratio + "dppx)";
				  //if (("matchMedia" in window) && window.matchMedia(q).matches) {
					// Everything OK ? Go Go Go !
					// Get src value and change it
					$('img, a[href$=".jpg"], a[href$=".jpeg"], a[href$=".JPG"], a[href$=".JPEG"]').each(function() {
						if ($(this).is('img')) {
							var imgUrl = $(this).attr('src');
							var type = 'src';
						} else {
							var imgUrl = $(this).attr('href');
							var type = 'href';
						}

						$(this).attr(type, imgUrl.replace('-lq', '-hq'));
					});
				  //}
				}
			  }
			}

			function widgetIcon($icon) {
				var $widget = $icon.closest('.widget');
				if ($widget.find('.widget-content > ul').is(':hidden')) {
					$icon.removeClass('fa-chevron-up fa-chevron-down');
					$icon.addClass('fa-chevron-up');
				} else {
					$icon.removeClass('fa-chevron-down fa-chevron-up');
					$icon.addClass('fa-chevron-down');
				}
			}

			$('.widget-type-brief .widget-header, .widget-type-pgevevent .widget-header').click(function(){
				var $icon = $(this).find('.icon-right');
				widgetIcon($icon);
				$(this).closest('.widget').find('.widget-content > ul').slideToggle('fast');
			});

			// Collections
			$('.slideshow-collection').slick({
				infinite: false,
				prevArrow: '<button type="button" data-role="none" class="slick-prev slick-arrow" aria-label="Précédent" role="button"><i class="fa fa-caret-left">',
				nextArrow: '<button type="button" data-role="none" class="slick-next slick-arrow" aria-label="Suivant" role="button"><i class="fa fa-caret-right">',
				speed: 300,
				slidesToShow: 5,
				slidesToScroll: 5,
				adaptiveHeight: true,
				responsive: [
				  {
					breakpoint: 1200,
					settings: {
					  slidesToShow: 4,
					  slidesToScroll: 4
					}
				  },
				  {
					breakpoint: 768,
					settings: {
					  slidesToShow: 3,
					  slidesToScroll: 3
					}
				  },
				  {
					breakpoint: 480,
					settings: {
					  slidesToShow: 2,
					  slidesToScroll: 2
					}
				  }
				]
			});

			function urlExists(url)
			{
				var http = new XMLHttpRequest();
				http.open('HEAD', url, false);
				http.send();
				return http.status === 200;
			}

			// Lightbox
			$('.slideshow-collection a').simpleLightbox({'showCounter': false});
			$('.slideshow-webcams a').simpleLightbox({'showCounter': false});

			$('#main article img, .widget-type-text .widget-content img').each(function(i, e){
				if (!$(this).parents('a').length) {
					var imgSrc = $(this).attr('src');
					var testImg = imgSrc.match('\/uploads\/(([0-9]+)\-(hq|lq))\/');
					var i = 4;
					var urlFinal = imgSrc;

					if (testImg) {
						var isEnd = false;

						while (!isEnd && i > 0) {
							var urlPart = testImg[0].replace(eval('/([0-9])/'), i.toString());
							var url = imgSrc.replace(testImg[0], urlPart);

							if (!urlExists(url)) {
								i--;
							} else {
								urlFinal = url;
								isEnd = true;
							}


						}
					}

					$(this).wrap('<a href="'+urlFinal+'" class="lightbox">');
				}
			});

			$('#main article a.lightbox, .widget-type-text .widget-content a.lightbox').simpleLightbox({'nav': false,
																										'showCounter': false});
		
		</script>
