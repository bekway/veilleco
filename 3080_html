						<script src="http://static.reseaudesintercoms.fr/default/scripts/interactive_map.js"></script>
						<script>
			var map_cities = [
				'La Fontenelle',
				'Le Poislay',
				'Droué',
				'La Chapelle-Vicomtesse',
				'Bouffry',
				'Ruan-sur-Egvonne',
				'Romilly-du-Perche',
				'Chauvigny-du-Perche',
				'Fontaine-Raoul',
				'Villebout',
				'Busloup',
				'Saint-Hilaire la Gravelle',
				'Saint-Jean Froidmentel',
				'Lisle',
				'Pezou',
				'Lignières',
				'Fréteval',
				'Morée',
				'Brévainville',
				'Renay',
				'La Chapelle-Enchérie',
				'Moisy',
				'Ouzouer-le-Doyen'
			];
			
			var map_config = {
				'default':{
					'borderColor': '#BEC4CB',
					'mapShadow':'#fff',
					'shadowOpacity':'35',
					'hoverShadow':'#666666',
					'lakesColor':'#66CCFF',
					'namesColor':'#919191'
				},
			};
			
			for (var i = 1, max = map_cities.length; i <= max; i++) {
				map_config['map_' + i] = {
					'hover': map_cities[i-1],
					'upColor':'#ffffff',
					'overColor':'#77b752',
					'downColor':'#77b752',
					'enable':true,
				};
				addEvent('map_' + i);
			}
			
			var pin_config = {
				'default':{
					'pinShadow':'#000',
					'pinShadowOpacity':'50',
				},
				'points':[]
			}
		
						</script>
