//Affichage d'un seul point avec les informations provenant du SIG
function genere_affichage_unitaire(urlsingle, div_detail, couch ) {

      jQuery.support.cors = true;
	  $.ajax({
		  cache:true,
	  	  url:urlsingle,
		  dataType:'json',
		  crossDomain:true,
		  xhrFields: {
			   withCredentials: false
		  },
		  contentType: 'text/plain; charset=utf-8',
		  type:'GET',
		  success:function( dataa ) {

			  $.each( dataa, function( keyy, vall ) {//Revoir optimisation
				  rs='relatedRecordGroups';
				  if(couch==0 || sgl){
					  rs='features';
				  }

				  if(keyy==rs){
					  its.push( vall );
				  }
				  else if(keyy=='fields'){
					  fls.push(vall);
				  }
			  });


			 if (couch==0 || sgl) {
				 tb = its[0];
			 }
			 else {
			 	 tb=its[0][0].relatedRecords;
			 }


			 tablo.push(tb[0].attributes);
			 var vals=['CODE_POS','CODE_INSEE','SOUS_TYPE_CAC','ID_EQ_BUREAU_VOTE','ID_EQ_MAIRIE','ID_EQ_ECOLE','ID_EQ_CONS_QUA','ID_EQ_CULTUREL','ID_EQ_CCAS','ID_EQ_MONUMENT','ID_EQ_SERVICE','ID_EQU','GlobalID','OBJECTID','IDENTIFIANT','NOM','FAMILLE','SOUS_TYPE','ABREVIATION','CLASSE_TEXTE','ID_EQU_BAT','VISIBILITE_WEB','VISIBILITE_CARTO','CLASSE_WEB','DATE_CREA','ETIQUETTE','COORD_X_L93','COORD_Y_L93','CODE_WEB','URL_WEB'];
			 var lt=[];
			  for(var i=0;i<fls[0].length;i++){
				  var st=fls[0][i].name;
				  var val = String(eval("tablo[0]."+st));
				  var st2=fls[0][i].alias;

				//console.log(st);
				//Récupération des informations littérales du SIG et masque celles que l'on ne souhaite pas faire apparaitre (revoir optimisation)
				if (val !== "null" && $.inArray(st,vals)==-1)

					{
						//Ajout des informations dans le div
						var vl=String(eval("tablo[0]."+fls[0][2].name));

						if($('.search').length==0){$('a.hding h1 .rgg p.nomarge,.singl .articleTitre h1,.finall .obj').html(vl.toLocaleString());}
						//Date de mise à jour du point
						if (st.indexOf('DATE') !== -1) {
							val = new Date(parseInt(val));
							div_detail.append( '<!--'+st+'\/\/--> <b>'+st2+': '+val.toLocaleString()+'<br/>');
						}
						else {

							if(st.indexOf('COORD_')!==-1){
								lt.push('<!--'+st+'\/\/--> <b>Coordonnées '+st2+': '+val+'<br/>');
							}else{
								div_detail.append( '<!--'+st+'\/\/--> <b>'+st2+': '+val+'<br/>');
							}

						}

					}

			  }
			  for (var k=0;k<lt.length;k++){
				 div_detail.append(lt[k]);
			  }
			  $('.loadingsgl').hide().remove();


	}
});
}

	  }
	  });



		}
});
	  });
		</script>
	</body>
</html>
