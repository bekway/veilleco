			<script src="https://code.jquery.com/jquery-1.12.4.min.js" integrity="sha256-ZosEbRLbNQzLpnKIkEdrPv7lOy9C27hHQ+Xp8a4MxAQ=" crossorigin="anonymous"></script>
			<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js" integrity="sha256-VazP97ZCwtekAsvgPBSUwPFKdrwD3unUfSGVYrahUqU=" crossorigin="anonymous"></script>
			<script>
  var redirectURL = "http://www.agglo-lepuyenvelay.fr";
  function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+ d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
  }
  function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
  }
  
  $( function() {
    var visited = getCookie('visited');
    if( visited != 1 ) {
      $( "#dialog" ).dialog({
        dialogClass: "no-close",
        draggable: false,
        modal: true,
        width: 800,
        buttons: [
          {
            text: "Continuer vers le site de la CC du Langeadois",
            click: function() {
              setCookie('visited', 1, 1);
              $( this ).dialog( "close" );
            }
          },
          {
            text: "Site de la CC du Pays de Paulhaguet",
            click: function() {
			  window.open('http://www.cc-paysdepaulhaguet.fr', '_blank');
			  setCookie('visited', 1, 1);
			  $( this ).dialog( "close" );
            }
          },
          {
            text: "Site de la CC du Pays de Saugues",
            click: function() {
        window.open('https://www.paysdesaugues.com', '_blank');
        setCookie('visited', 1, 1);
        $( this ).dialog( "close" );
            }
          }
        ]
      });
    } else {
        $('#dialog').css('display','none');
    }
  } );
  
			</script>
