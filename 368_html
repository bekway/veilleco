var jqMMob = jQuery.noConflict();
jqMMob( document ).ready(function() {
	jqMMob('#burger-mobile').click(function() {
		jqMMob('#menu-cache').toggle(400);
		jqMMob('#menu-mobile').toggle(400);
	   // Déplacement du block recherche
	   if(jqMMob('#search-block-form').parent() != jqMMob('#recherche-mobile')){
			// le menu est visible
			 jqMMob('#recherche-mobile').append(jqMMob('#search-block-form'));
			 jqMMob('#edit-search-block-form--2').css('width','100%');
			 jqMMob('#edit-search-block-form--2').css('height','28px');
			 jqMMob('#edit-search-block-form--2').css('border','none');
	   }else{
			 jqMMob('#block-search-form').find('.content').append(jqMMob('#search-block-form'));
			 jqMMob('#edit-search-block-form--2').css('width','285px');
			 jqMMob('#edit-search-block-form--2').css('height','35px');
			 jqMMob('#edit-search-block-form--2').css('border','1px solid #d1d2d2');
	   }
	});
	jqMMob('.bt-agr-menu').click(function() {
		jqMMob(this).parent().nextUntil('.parent','.child').toggle(400);
	});    
	jqMMob('.bt-fermer-menu').click(function() {
		jqMMob('#menu-cache').toggle(300);
		jqMMob('#menu-mobile').toggle(300);
	});    
    jqMMob('#menu-cache').click(function() {
		jqMMob('#menu-cache').hide(300);
		jqMMob('#menu-mobile').hide(300);
	});    
});
											</script>
