								<div id="bt-BAI" ng-open-modal="fstnModal" ng-template-url="" class="pointer bt-social-2"></div><a href="http://www.facebook.com/MontpellierMediterraneeMetropole" class="bt-social-2" target="_blank" title="Partager sur facebook."> <img src="/sites/all/themes/zen_agglo/images/icon_facebook.png" alt="Facebook"> </a> <a href="https://twitter.com/Montpellier3m" class="bt-social-2" target="_blank" title="Partager sur twitter."> <img src="/sites/all/themes/zen_agglo/images/icon_twitter.png" alt="Twitter"> </a>
								<script type="text/javascript">
var jq12 = jQuery.noConflict();
jq12( document ).ready(function() {
	jq12('#bt-BAI').click(function() {
		jq12('#bai-page1').show();
		jq12('#bai-page2').hide();
		jq12('#dial-BAI').show();
		jq12('#cont-BAI').attr('style','position: fixed;top: 0px;left:0px;z-index: 5000;width:100%;height:100%;;overflow:scroll;background-color: rgba(169, 169, 169, 0.8);');
	});
	jq12('#BAI-MOB').click(function() {
                jq12('#menu-cache').hide();
		jq12('#menu-mobile').hide();
		jq12('#bai-page1').show();
		jq12('#bai-page2').hide();
		jq12('#dial-BAI').show();
		jq12('#cont-BAI').attr('style','position: fixed;top: 0px;left:0px;z-index: 5000;height:100%;;overflow:scroll;height:300%;background-color: rgba(169, 169, 169, 0.8);');
                jq12('.helpForm').hide();
	});
	jq12('.bt-bai-fermer').click(function() {
		jq12('#dial-BAI').hide();
		jq12('#cont-BAI').attr('style','');
	});
	jq12('#bt-bai-suggerer').click(function() {
		// vérifications
		var ok = true;
		if(jq12('#title').val()==''){
			ok = false;
			alert('Vous devez saisir un titre pour votre suggestion');
			jq12(this).addClass('ng-invalid-required');
		}
		if(jq12('#description').val()==''){
			jq12(this).addClass('ng-invalid-required');
			if(ok){
				alert('Vous devez saisir une description');
			}
			ok = false;
		}
		if(ok && jq12('#prenom').val()==''){
			jq12(this).addClass('ng-invalid-required');
			if(ok){
				alert('Vous devez saisir un prénom ou un pseudo');
			}
			ok = false;
		}
		if(ok){
			var t = jq12('#title').val();
			var n = jq12('#prenom').val();
			var d = jq12('#description').val();
			var c = jq12('#category').val();
			var m = jq12('#mail').val();
			jq12.ajax({
				url:"/sites/all/themes/zen_agglo/boiteAidee.php",
				method: "POST",
				data:{ titre: t, nom: n,description:d,categorie:c,mail:m }
			}).success(function(data) {
				jq12('#bai-page1').hide();
				jq12('#bai-message').html(data);
				jq12('#bai-page2').show();
			}).error(function(resultat, statut, erreur){
				alert(erreur);
				jq12('#bai-message').html("Votre suggestion n'a pas pu être prise en compte.");
			});
			
		}
	});
});
								</script>
