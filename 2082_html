			<script>

        //consentement cookies
        var okMatomo = lireCookie('matomo');

        if (okMatomo == 'non') {
            $('#okMatomo').prop('checked', false);
        }
        else {
            $('#okMatomo').prop('checked', true);

            var _paq = window._paq || [];
            _paq.push(['trackPageView']);
            _paq.push(['enableLinkTracking']);
            (function() {
              var u="//www.cchautesvosges.fr/stat/";
              _paq.push(['setTrackerUrl', u+'matomo.php']);
              _paq.push(['setSiteId', '1']);
              var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
              g.type='text/javascript'; g.async=true; g.defer=true; g.src=u+'matomo.js'; s.parentNode.insertBefore(g,s);
            })();
        }

        function checkCookie(){
    		if (document.getElementById('okMatomo').checked == true){
    			document.cookie = 'matomo=oui;max-age=157680000‬';
    			$('#activiteMatomo').html('Matomo actif');

                var _paq = window._paq || [];
                _paq.push(['trackPageView']);
                _paq.push(['enableLinkTracking']);
                (function() {
                  var u="//www.cchautesvosges.fr/stat/";
                  _paq.push(['setTrackerUrl', u+'matomo.php']);
                  _paq.push(['setSiteId', '1']);
                  var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
                  g.type='text/javascript'; g.async=true; g.defer=true; g.src=u+'matomo.js'; s.parentNode.insertBefore(g,s);
                })();
    		}
    		else {
    			document.cookie = 'matomo=non;max-age=157680000‬';
    			$('#activiteMatomo').html('Matomo inactif');
    		}
    	}

        //sauvegarde Contraste

        var contraste = 0;

        var cookieContraste = lireCookie('contraste');

        if (cookieContraste == 'oui') {
            $('body').addClass('contraste');
            contraste = 1;
        }


    
			</script>
			<script>

        $.noConflict();

        jQuery(document).ready(function($) {

            //affichage sans sondage
            // if (document.getElementById('sondage')) {
            //     $('#bloc-large').addClass('sondage');
            // }

            //menu deroulant

            function fermerMenu() {
                $('#principal > .parent > ul').slideUp(0);
                $('#principal > .parent').removeClass('deroule');
                $('#principal .parent, #principal a').removeClass('opacite');
                $('#principal > .parent').removeClass('opacite2');
            }

            $('#principal > .parent').click(function() {
                if ($(this).hasClass('deroule')) {
                    $(this).removeClass('deroule');
                    $(' > ul', this).slideUp(0);

                    $('#principal .parent, #principal a').removeClass('opacite');
                    $(this).removeClass('opacite2');
                } else {
                    fermerMenu();

                    $(' > ul', this).slideDown(0);
                    $(this).addClass('deroule');

                    $('#principal a, #principal .parent').addClass('opacite');
                    $(this).addClass('opacite2');
                }
            });

            $('main, header, footer').click(function() {
                fermerMenu();
            });

            $(document).keyup(function(touche) {
                if (touche.keyCode == 27) { //echap
                    fermerMenu();
                }
            });

            //burger
            $('#burger').click(function() {
                $('#principal').slideToggle();
                $(this).toggleClass("croix");
            });

            //recherche
            $('#loupe').click(function() {
                $('#recherche form').slideToggle();
            });
            $('#fermer-recherche').click(function() {
                $('#recherche form').slideUp();
            });



            //position en fonction de la colonne
            // if ($('#colonne > div').hasClass('tag') | $('#colonne > div').hasClass('custom')) {
            //     $('.aussi').addClass('decale3');
            // }


            //Anim scroll

            function position() {

                //var fen = $(window).height();
                var pos = $(document).scrollTop();

                if (pos > 7) {
                    $('#logo').addClass('petit-logo');
                } else {
                    $('#logo').removeClass('petit-logo');
                }

                if(pos > 300){
        			$('#retour-haut').addClass('retour');
        		}
        		else{
        			$('#retour-haut').removeClass('retour');
        		}

            }

            position();

            $(window).scroll(function() {
                position();
            });


            //contraste

            // $('.augmenter').click(function() {
            //     $('body').addClass('contraste');
            //     $(this).removeClass('augmenter');
            //     $(this).addClass('baisser');
            //     document.cookie = 'contraste=oui;max-age=31104000‬';
            // });
            //
            // $('.baisser').click(function() {
            //     $('body').removeClass('contraste');
            //     $(this).removeClass('baisser');
            //     $(this).addClass('augmenter');
            //     document.cookie = 'contraste=non;max-age=31104000‬';
            // });



            $('#contraste').click(function() {

                if (contraste == 0) {
                    document.cookie = 'contraste=oui; max-age=31104000‬; path=/';
                    $('body').addClass('contraste');
                    contraste = 1;
                }
                else {
                    document.cookie = 'contraste=non; max-age=31104000; path=/';
                    $('body').removeClass('contraste');
                    contraste = 0;
                }

            });



            //bouton partager
            $('#partager').click(function() {
                $('#bloc-partager').slideToggle(200);
            });

            //accordeon

            if (document.getElementById('formulaire')) {

            }

            else if (document.getElementById('corps-article')) {
                var art = $('#corps-article').html();

            	acc1 = art.replace(/\[\[/g, '<h3 class="accordeon"><span>');
            	acc2 = acc1.replace(/\|\|/g, '<div class="panneau">');
            	acc3 = acc2.replace(/\]\]/g, '<br><br><br>');

            	$('#corps-article').html(acc3);

            }


        	var acc = document.getElementsByClassName("accordeon");
        	var i;

        	for (i = 0; i < acc.length; i++) {
        		acc[i].addEventListener("click", function() {

        			this.classList.toggle("ouvert");

        			var panneau = this.nextElementSibling;

        			if (panneau.style.maxHeight){
        				panneau.style.maxHeight = null;
        			}
        			else {
        				panneau.style.maxHeight = panneau.scrollHeight + "px";
        			}
        		});
        	}

            //accordeon blog

            $('.lire-plus button').click(function() {
                $(this).parent().parent().toggleClass('voir');
                $(this).html() == 'Montrer plus' ? $(this).html('Montrer moins') : $(this).html('Montrer plus');
            });

            $('.item-image').parent().parent().toggleClass('avec-photo');


            //miniature photo
            $('.pull-center').click(function() {
                $(this).toggleClass('pleine');
            });

            //nav clavier
            $(document).keyup(function(touche) {
                if (touche.keyCode == 9) {
                    $('*').addClass('outline');
                }
            });

            //pause animation
            $('#anim-femme').click(function() {
                $('#jambe1, #jambe2, #bulle').toggleClass('pause');
            });

            $('.bloc-feat').click(function() {
                $(this).toggleClass('pause');
            });

            //scrolling doux
            $('.doux').on('click', function() {
                var page = $(this).attr('href');
                $('html, body').animate({
                    scrollTop: $(page).offset().top
                }, 400);
                return false;
            });

        });
    
			</script>
			<script>

        jQuery(document).ready(function($) {

            //cacher tous les pointeurs
            callHideAllGroup();

            //afficher pointeur CC
            callToggleGroup(5);

            //Me géolocaliser
            $('.geoloc').click(function() {

                if (navigator.geolocation) {
                    navigator.geolocation.getCurrentPosition(
                        succesGeoloc,
                        erreurGeoloc, {
                            enableHighAccuracy: true,
                            maximumAge: 300000
                        },
                    );
                } else {
                    alert("Votre navigateur ne prend pas en compte la géolocalisation");
                }
            });

            function succesGeoloc(position) {

                lat1 = position.coords.latitude;
                lon1 = position.coords.longitude;

                var marqueur = L.icon({
                    iconUrl: '/administrator/components/com_zhosmmap/assets/icons/marqueur-neutre.png',
                    iconAnchor: [21, 67],
                });

                map.flyTo([lat1, lon1], 11);

                L.marker([lat1, lon1], {
                    icon: marqueur
                }).addTo(map);

            };

            function erreurGeoloc(error) {
                switch (error.code) {
                    case error.POSITION_UNAVAILABLE:
                        alert("Votre emplacement n'a pas pu être déterminé");
                        break;
                }
            };

            //centrer la carte
            $('.centrer').click(function() {
                map.flyTo([48.0372, 6.8314], 11);
            });

            //ouvrir version texte
            $('.txt-carte').click(function() {
                $('#OMapsMarkerListMain').slideToggle();
            });

            //GPS Itineraire
            /*var gps = $('.iti-google').html();

            itiGPS = gps.replace(/dir\/\//g, 'dir/Gérardmer');
            $('.iti-google').html(itiGPS);*/

        });

        
			</script>
