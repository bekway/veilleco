						<script type="text/javascript">
				
			var events_E14CC12EA1E446ADA2B87CDA2207C394 = undefined;	
					
			function getEvent_E14CC12EA1E446ADA2B87CDA2207C394(day, month,year){	
					var empty = ["false","",""];
					if(events_E14CC12EA1E446ADA2B87CDA2207C394 != undefined)			
						return events_E14CC12EA1E446ADA2B87CDA2207C394[year][month+1][day];		
					else 
						return empty; 										
				}
		
				selectedDay_E14CC12EA1E446ADA2B87CDA2207C394 = function(month,day, year) {					
						return "/modules/calendrier/calendrier_view.php?id-template=1&date="+year+(month < 10 ? "0"+month : month)+(day < 10 ? "0"+day : day)
				}
		
				function load_month_E14CC12EA1E446ADA2B87CDA2207C394(year, month){						
						events_E14CC12EA1E446ADA2B87CDA2207C394 = undefined;
						$.ajax({
								url: "/modules/actualite/agenda_calendrier.php",
								type: "POST", 
								data: {
									action : "getMonth",
									month : month,									
									year : year
									},									
								success: function(data) {					
									 events_E14CC12EA1E446ADA2B87CDA2207C394 = jQuery.parseJSON(data);	
									 $("#almanach-E14CC12EA1E446ADA2B87CDA2207C394").datepicker("refresh");
							}		 		
					});
				}
								 		
	
				$(document).ready(function(){
			
								 		
						
					$("#almanach-E14CC12EA1E446ADA2B87CDA2207C394").datepicker({
										numberOfMonths: 1,
										stepMonths: 1,
										nextText: "Suivant",
										prevText: "Précédent",
										dayNamesMin: ["Di", "Lu", "Ma", "Me", "Je", "Ve", "Sa"],
										monthNames: ["Janvier","Février","Mars","Avril","Mai","Juin","Juillet","Août","Septembre","Octobre","Novembre","Décembre"],
										firstDay: 1,
										beforeShowDay: function(date) {									 												
											return getEvent_E14CC12EA1E446ADA2B87CDA2207C394(date.getDate(), date.getMonth(), date.getFullYear());							
										},
										onChangeMonthYear : function (year, month, inst) {
											load_month_E14CC12EA1E446ADA2B87CDA2207C394(year, month);
										},	
										onSelect: function(dateText) {
											var day = parseInt(dateText.slice(3,5),10);	
         									var month = parseInt(dateText.slice(0,2),10);
											var year = parseInt(dateText.slice(6),10);
											
											var href_to_open = selectedDay_E14CC12EA1E446ADA2B87CDA2207C394(month,	day,year);
													
											var event = events_E14CC12EA1E446ADA2B87CDA2207C394[year][month][day];
													
											if(event[0] == "true")
												$.fancybox( {type : "iframe", href : href_to_open}  );
										}
						
									});
							
									/*
									* Bricolage indispensable pour colorer les dates choisies
									* chaque fois que le datepicker se redessine !
									*/
									$("#almanach-E14CC12EA1E446ADA2B87CDA2207C394").bind("mouseenter", function(){
										if ($("td.important > a", this).eq(0).css("color") != "#FF0000") {
											$("td.important > a", this).css("color","#FF0000");
								
										}
									});			
							
					
				});	//fin document ready
		
				load_month_E14CC12EA1E446ADA2B87CDA2207C394(2020, 07)	;									
													
				$(window).load(function(){
					$(".calendrier-test").mouseenter();
				});	//fin window ready
		
			
						</script>
