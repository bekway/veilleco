							<script type="text/javascript">
$( document ).ready(function() {
    $('#nav-communes').css('left','236px');
    $('#communes-handle').click(function() {
        $('#nav-communes ul').css('overflow','hidden');
        if (parseInt($('#nav-communes').css('left')) == 236) {
            $('#nav-communes').css('background', '#e9e9e9');
        } else {
            $('#nav-communes').css('background', 'none');
        }
        var a = (parseInt($('#nav-communes').css('left')) <236)?'236px':0;
        $('#nav-communes').animate({left: a}, 500 ,function(){$('#nav-communes ul').css('overflow','auto');});
    });
});
							</script>
