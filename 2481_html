							jQuery(function()
							{
								// Box Search
								// clic sur box
								$('.box-search').on("click", function()
								{
									// on affiche la boxe
									$(this).addClass('active');
								});

								$(".ico-close").click(function(e)
								{
									$('.box-search').removeClass('active');

									// on stoppe l'éxecution des autres clic sur la box
									e.stopImmediatePropagation();
								});


								/* On déplace l'élément ".box-vous-etes-search" dans l'élément ".box-header-1" */
								if( $(window).width() <= 1025 ) // QUE pour tablette
								{ 
									spe.tools.move(".box-vous-etes-search", ".box-header-1", "inner:first");//, "inner:before"
								} // Résolution desktop


								// Box Vous êtes
								// clic sur box
								$('.box-vous-etes').on("click", function()
								{
									// on affiche la boxe
									$(this).toggleClass('active');
								});
							});
						
