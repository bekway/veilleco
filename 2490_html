			                function in_array (needle, haystack, argStrict) {
		var key = '', strict = !!argStrict;
		if (strict) {
			for (key in haystack) {
				if (haystack[key] === needle) {
					return true;
				}
			}
		} else {
			for (key in haystack) {
				if (haystack[key] == needle) {
					return true;
				}
			}
		}
		return false;
	}
	                jQuery(document).ready(function() {
		                jQuery("#calendarPicker").datepicker({
			                dateFormat:'yy-mm-dd',
			                			                minDate:0,
			                firstDay:1,
                            url:'/resource/agenda.html',			                onSelect: function(dateText, inst) { 
				                document.adminForm2.filter_5.value = Base64.encode(dateText);
				                document.adminForm2.filter5.value = Base64.encode(dateText);
                                document.adminForm2.submit();
			                 },
			                 beforeShowDay: function(date) {
				arr = new Array(
				'1289862000000','1292367600000'            
				);
				if(in_array( date.getTime(), arr )) {
					return [true,'activedate'];
				}
				else {
					return [true,''];
				}
			}
                });
		                
	                });    
	                
