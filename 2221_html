		    var timer_resize_bloc_13 = 0;
		
			$( window ).resize(function() {				
				
			    if(timer_resize_bloc_13 != 0){
				    clearTimeout(timer_resize_bloc_13);
				    timer_resize_bloc_13= 0;
				}
								
				timer_resize_bloc_13= setTimeout(resize_bloc_13,1000);
			    
			});
		
			$(window).load(function(){				
				resize_bloc_13();
							
				timer_resize_bloc_13= setTimeout(resize_bloc_13,1000);
				
				
			});
			
			
			function resize_bloc_13(){
			   equalheightForAll(".module-bloc-13 .module-bloc-item-image");
				equalheightForAll(".module-bloc-13 .module-bloc-item-titre");
				equalheightForAll(".module-bloc-13 .module-bloc-item-commentaire");
				equalheightForAll(".module-bloc-13 .module-bloc-item");
			}
			
			
			
