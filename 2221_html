					<div class="calendrier clearfix">
						<script type="text/javascript">
				
			var events_BEE5723F186B4639B5C3D85036580DE7 = undefined;	
					
			function getEvent_BEE5723F186B4639B5C3D85036580DE7(day, month,year){	
					var empty = ["false","",""];
					if(events_BEE5723F186B4639B5C3D85036580DE7 != undefined)			
						return events_BEE5723F186B4639B5C3D85036580DE7[year][month+1][day];		
					else 
						return empty; 										
				}
		
				selectedDay_BEE5723F186B4639B5C3D85036580DE7 = function(month,day, year) {					
						return "/modules/calendrier/calendrier_view.php?id-template=1&date="+year+(month < 10 ? "0"+month : month)+(day < 10 ? "0"+day : day)
				}
		
				function load_month_BEE5723F186B4639B5C3D85036580DE7(year, month){						
						events_BEE5723F186B4639B5C3D85036580DE7 = undefined;
						$.ajax({
								url: "/modules/actualite/agenda_calendrier.php",
								type: "POST", 
								data: {
									action : "getMonth",
									month : month,									
									year : year
									},									
								success: function(data) {					
									 events_BEE5723F186B4639B5C3D85036580DE7 = jQuery.parseJSON(data);	
									 $("#almanach-BEE5723F186B4639B5C3D85036580DE7").datepicker("refresh");
							}		 		
					});
				}
								 		
	
				$(document).ready(function(){
			
								 		
						
					$("#almanach-BEE5723F186B4639B5C3D85036580DE7").datepicker({
										numberOfMonths: 1,
										stepMonths: 1,
										nextText: "Suivant",
										prevText: "Précédent",
										dayNamesMin: ["Di", "Lu", "Ma", "Me", "Je", "Ve", "Sa"],
										monthNames: ["Janvier","Février","Mars","Avril","Mai","Juin","Juillet","Août","Septembre","Octobre","Novembre","Décembre"],
										firstDay: 1,
										beforeShowDay: function(date) {									 												
											return getEvent_BEE5723F186B4639B5C3D85036580DE7(date.getDate(), date.getMonth(), date.getFullYear());							
										},
										onChangeMonthYear : function (year, month, inst) {
											load_month_BEE5723F186B4639B5C3D85036580DE7(year, month);
										},	
										onSelect: function(dateText) {
											var day = parseInt(dateText.slice(3,5),10);	
         									var month = parseInt(dateText.slice(0,2),10);
											var year = parseInt(dateText.slice(6),10);
											
											var href_to_open = selectedDay_BEE5723F186B4639B5C3D85036580DE7(month,	day,year);
													
											var event = events_BEE5723F186B4639B5C3D85036580DE7[year][month][day];
													
											if(event[0] == "true")
												$.fancybox( {type : "iframe", href : href_to_open}  );
										}
						
									});
							
									/*
									* Bricolage indispensable pour colorer les dates choisies
									* chaque fois que le datepicker se redessine !
									*/
									$("#almanach-BEE5723F186B4639B5C3D85036580DE7").bind("mouseenter", function(){
										if ($("td.important > a", this).eq(0).css("color") != "#FF0000") {
											$("td.important > a", this).css("color","#FF0000");
								
										}
									});			
							
					
				});	//fin document ready
		
				load_month_BEE5723F186B4639B5C3D85036580DE7(2020, 07)	;									
													
				$(window).load(function(){
					$(".calendrier-test").mouseenter();
				});	//fin window ready
		
			
						</script>
