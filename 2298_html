    jQuery(document).ready(function($){     
      $('#om-maximenu-menu-button li.om-leaf .om-maximenu-content').removeClass('om-maximenu-content-nofade');
      $('#om-maximenu-menu-button li.om-leaf .om-link').click(function(ev) {
        ev.stopPropagation();
        if($(this).parent().hasClass('open')) {
          $(this).parent().removeClass('open');
        }
        else {
          $('#om-maximenu-menu-button li.om-leaf').removeClass('open');
          $(this).parent().addClass('open');
        }  
        
        $(this).parent().siblings().children('.om-maximenu-content').slideUp('slow');        
        $(this).siblings('.om-maximenu-content').toggle('slow');       
        return false;
      });
      $('#om-maximenu-menu-button .om-maximenu-content').click(function(ev) {
        ev.stopPropagation();
      });        
      $('html').click(function() {
        // Close any open menus.
        $('#om-maximenu-menu-button .om-maximenu-content:visible').each(function() {
          $(this).parent().removeClass('open');
          $(this).hide();
        });
      });      
    }); 
    
