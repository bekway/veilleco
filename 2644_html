					{ contenu: c, titre: escape("") , css: 'css/1027380_3.css', keyword : ''},
						function(data){					
							window.open('https://www.cdcla.fr//index.php/pageToPDF/display/'+t+'.pdf');
							return false;
					}
				);
			});

