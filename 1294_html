                      slidesToShow: 3,
                      slidesToScroll: 1,
                      asNavFor: '.slider-for',
                      dots: true,
                      centerMode: true,                                    
                      autoplay: true,
                    autoplaySpeed: 7000,
                      centerPadding: '60px',
                      focusOnSelect: true
                    });
                  
