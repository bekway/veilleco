						$(document).ready(function () {
				$(".gotoPage").bind('change',function (event) {
					if (event.target.value)
						document.location=event.target.value;
				});
				if ("Bandeau-CC2M-logo" != "") {
					$(".img_font").css('background', "url('/library/Bandeau-CC2M-logo') no-repeat")
				} else {
