				/*----------------------------------------------
					Soumission formulaire de recherche
				---------------------------------------------*/
				$('#form-search').submit(function(e){
					e.preventDefault();
					
					value_action 	= $(this).attr('action');
					value_keywords 	= $('#form-search-keywords').val();

					/*** Si la recherche comporte au moin 3 caratères ***/
					if(value_keywords.length >= 3){
						window.location.href = value_action+'?recherche='+value_keywords;	
						
					/*** Sinon afficher un message d'erreur ***/
					}else{			
						message_erreur = "<p>La recherche doit contenir au minimum 3 caractères";
						
						$('#message-flash .container').append(message_erreur);
						var animation_flash	= new TimelineLite();
						
						/*** Animation ***/
						animation_flash.to('#message-flash', 0.5, {left: '0%', zIndex: 100});
						animation_flash.play();
						
						/*** Supprimer le message après x durée ***/
						setTimeout(function(){
							animation_flash.reverse();
							
							setTimeout(function(){
								$('#message-flash .container').html('');
							},2500);
							
							
							
						},6000);
					}
				});
				
				/*----------------------------------------------
					Menu interne | Gauche
				---------------------------------------------*/
				
				$('.menu-interne').on('click', 'svg', function(){
				
					//console.log($(this).parent().parent());
					var svg = $(this);
					var obj = $(this).parent().parent();					
					
					animate_menu(svg, obj)

				});
				
				
				$(window).load( function(){
					var obj = $('.menu-interne .current-page');
					var svg = '';
					//li-current-page-lvl-3
					//li-current-page-lvl-2
					
					if( obj.hasClass('lvl-1')){
						svg = obj.find('.svg').not('.lvl-2 svg');	
											
						animate_menu(svg, obj);
					}
					
					
					if( obj.hasClass('lvl-2')){
						// Parent
						obj_parent = obj.parent().parent().parent();
						svg_parent = obj_parent.find('.svg').not('.lvl-2 svg');		
						
						animate_menu(svg_parent, obj_parent);
						// Cible
						
						svg = obj.find('.svg').not('lvl-1 svg').not('lvl-3 svg');							
						animate_menu(svg, obj);
					}
					
					
					if( obj.hasClass('lvl-3')){
						// Parent
						obj_parent = obj.parent().parent();
						svg_parent = obj_parent.find('.svg').not('.lvl-3 svg');		

						animate_menu(svg_parent, obj_parent);
						// Parent TOP
						
						obj = obj_parent.parent().parent().parent();							
						svg = obj.find('.svg').not('.lvl-2 svg');	
						

						animate_menu(svg, obj);				
					}

					
				});
				
				
				function animate_menu(svg, obj){
					
					
					if( obj.hasClass('lvl-1') ){ nav = obj.find('.lvl-2'); }
					if( obj.hasClass('lvl-2') ){ nav = obj.find('.lvl-3'); }
					
					animateTime = 400;
					if(nav.height() === 0){
												
						svg.find('#vertical').css({'transform-origin' : '50% 50%' });
						svg.find('#vertical').animate({
							'rotate' : '+=90deg'
						}, 400);
						
						autoHeightAnimate(nav, animateTime);
						if(obj.hasClass('lvl-1')){ obj.find('span').css({'border-bottom-color' : 'transparent'}) }
					} else {
						nav.stop().animate({ height: '0', }, animateTime, function(){
							if(obj.hasClass('lvl-1')){ obj.find('span').css({'border-bottom' : ''}) }	
						});
						// Fermeture svg
						svg.find('#vertical').css({'transform-origin' : '50% 50%' });
						svg.find('#vertical').animate({
							'rotate' : '-=90deg'
						}, 400, function(){
							svg.find('#vertical').css({'transform-origin' : '' });
						});
						
					}
				}
								
				function autoHeightAnimate(element, time){
				  	var curHeight = element.height(), // Get Default Height
					autoHeight = element.css('height', 'auto').height(); // Get Auto Height
					element.height(curHeight); // Reset to Default Height
					element.stop().animate(
						{ height: autoHeight }, 
						parseInt(time), 
						function(){
							element.css('height', 'auto');	
						}
					); // Animate to Auto Height		
				}
				
				
				
				
				
				$(window).load(function(){
					
					var obj = $('.menu-interne li.current-page');
					//console.log(obj);
					if (obj.length === 0)
					{
						obj = $('.menu-interne li.current-page-lvl-2');
						//console.log(obj.parent().parent().parent());
						
						obj = obj.parent().parent().parent();
					}	
					
					
					
					
					obj.addClass('open');
					//animation_ouverture_menu_interne(obj);
					
					
				});
				
				
				
				
				
				/*----------------------------------------------
					Taille input FORMULAIRE CONTACT
				---------------------------------------------*/	
				$(document).ready(function(){
					form_width_input();
					search_current_top_menu();
					slider();
					//croix_menu();
					mise_en_forme_tableau();
					
					
					
				});
				
				$(window).resize(function(){
					form_width_input();
				});
				
				function mise_en_forme_tableau(){
					/*** Mise en forme des tableaux ***/
					if( $('body').hasClass('page-id-56')){
						table_horaire();
					}else{
						var le_tableau = $('#content table tr:first-child td');
						le_tableau.css({ width : (le_tableau.length / 100)+'%' });
					}
				}
				
				function form_width_input(){
					var form 				= $('.formulaire-type');

					var form_width			= form.find('.ligne-form').innerWidth();
					var form_label_width 	= form.find('label').innerWidth();
										
					form.find('.ligne-form input, .ligne-form .dropdown').innerWidth( form_width - form_label_width );

				}
				/*
				function croix_menu(){
					var les_liens = $('.menu-interne .lvl-1 span');
					var height_croix = $(les_liens[0]).find('svg').height();
				
					
					les_liens.each( function(){
						$(this).find('svg').css({
							top :  ( ($(this).outerHeight() - height_croix ) / 2)+'px'
						})
					})
				}
				*/
				function table_horaire(){
					$('table td').each(function(index, value){
						if( $(value).html().length == 0 )
						{
							$(value).addClass('vide');
							$(value).html('Fermé');
						}	
					});
				}
				
				function search_current_top_menu(){
					obj = $('body');
					
					if( obj.hasClass('parent-pageid-340') )
					{
						$('#menu .id-340').addClass('active');
					}
					
					if( obj.hasClass('parent-pageid-342') )
					{
						$('#menu .id-342').addClass('active');
					}
					
					if( obj.hasClass('parent-pageid-344') )
					{
						$('#menu .id-344').addClass('active');
					}
				}
			})(jQuery);
						
			function slider()
			{
				var nbImages			= jQuery('.content-slider');
				
				if(nbImages.length >= 2)
				{
					nbImages.css({
						position : 'absolute',
						top:		0,
						left:		0,
						right:		0,
						bottom:		0,
						opacity:	0
					});
					
					jQuery.each(nbImages, function(key, value){
						jQuery(this).attr('id', 'slide-'+key);
					});
					
					jQuery(nbImages[0]).addClass('current-slider').css({
						opacity: 1
					});
										    
				    slider_text();
				    setInterval("slider_next()", 4500);
				}
					
			}
			
			function slider_text()
			{
				a = jQuery('.content-slider');
				jQuery.each(a, function(key, value){
					
					obj = jQuery(this).find('.texte-slide');
					
					if ( obj.hasClass('v-center') ){
						
						obj.css({
							'top' 			:'50%',
							'margin-top'   : '-'+ $(obj).height()/2 +'px'
						});
					}
					
					if( obj.hasClass('h-center') ){
						obj.css({
							'left' 			:'50%',
							'margin-left'   : '-'+ $(obj).width()/2 +'px'
						});
					}
					
					
				});
			}
			
			function slider_next()
			{
				// Init
				lesSlides 		= jQuery('.content-slider');
				leSlideCourant  = jQuery('.current-slider');
				nbTotalIndex   	= lesSlides.length -1;
				nbCurrentIndex 	= 0;
				nbFuturIndex   	= 0;
				
				// Calcul					
				nbCurrentIndex = parseInt(leSlideCourant.attr('id').replace('slide-', ''));
												
				// Futur slider a afficher, retour au début si arrive à la fin
				if(nbCurrentIndex == nbTotalIndex)
				{
					nbFuturIndex = 0
				}else{
					nbFuturIndex = nbCurrentIndex + 1;
				}
				
				// Debug
				/*				
				console.log('Slide Actuel : '+ nbCurrentIndex);
				console.log('Nombre total de slide : '+ nbTotalIndex);
				console.log('Futur Slide : '+ nbFuturIndex);
				*/
				
				
				// Animation
				jQuery(lesSlides[nbCurrentIndex]).fadeTo(800, 0);
				jQuery(lesSlides[nbFuturIndex]).fadeTo(800, 1);
				
				
				// gestion des classes courantes
				leSlideCourant.removeClass('current-slider');
				jQuery(lesSlides[nbFuturIndex]).addClass('current-slider');
			}
		
		</script>
		<script>				 
			$(window).bind('touchmove, scroll', function(){
				var position_header = $('header').offset();
				var position_menu   = $('#menu-un-click').offset();
				
				//console.log('position header :'+position_header.top);
				//console.log('position menu :'+position_menu.top);
				
				
				breakLine = $('#slider-content').offset().top + $('#slider-content').outerHeight();
				
				if(position_header.top >= breakLine){
					if( !$('body').hasClass('menu-effect-open'))
					{
						animation_header_tablette_play();	
					}
					
				}else{
					animation_header_tablette_reverse();
				}
			});	
			
			function animation_header_tablette_play(){
				//console.log('play');

				$('body').addClass('menu-effect-open');					
				top_menu1Click = $('header').innerHeight();
				$('#menu-un-click').css({'top' : top_menu1Click});
				
			}
			
			function animation_header_tablette_reverse(){
				$('body').removeClass('menu-effect-open');
				$('#menu-un-click').css({'top' :0});				
			}
		
		</script>
