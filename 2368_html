		<script>
$(document).ready( function () {

  $('#actus .img ').each(function() {
    if(0 === $(this).has('img').length) {
      $(this).remove();
    }
  });

  $('#actus a img').each(function(){
    $(this).removeAttr('width');
    $(this).removeAttr('height');
    $(this).addClass('img-responsive img-thumbnail');
  });
  $('#actus .more').each(function(){
    $(this).css('text-align','center');
  });
  $('#actus .more a').each(function(){
    $(this).addClass('btn btn-default');
  });
  $('#actus .col-md-6').each(function(i) {
    if (i % 2 == 0) {
      $(this).nextAll().andSelf().slice(0,2).wrapAll('<div class="row">');
    }
  });

  $('a[href$=".pdf"]').attr('target','_blank');
});
		</script>
