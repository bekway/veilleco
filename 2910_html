		jQuery(function() {
		
			var el;
			
			jQuery("select")
				.each(function() {
					el = jQuery(this);
					el.data("origWidth", el.outerWidth()) // IE 8 will take padding on selects
				})
			  .mouseenter(function(){
			   jQuery(this).css("width", "auto");
			  })
