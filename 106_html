            function getCookie(name) {
                var value = "; " + document.cookie;
                var parts = value.split("; " + name + "=");
                if (parts.length == 2) return decodeURIComponent(parts.pop().split(";").shift());
            }
            var colorMode = getCookie('color-mode');
            if (colorMode) {
                colorMode = JSON.parse(colorMode);
                if (colorMode.isActive) {
                    document.getElementById('prerender').classList.remove('is-light');
                    document.getElementById('prerender').classList.add('is-' + colorMode.value);
                }
            }
            </script>
