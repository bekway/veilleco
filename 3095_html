var temp = $(".template-header").height();
$(".template-header").height(temp);

var gmapToShow = null;
$( document ).ready(function() {
$("#liens-google-map-69-5").toggle();
gmapToShow = $("#liens-google-map-69-5").find(".google-map-bloc");
if(gmapToShow.exists())
gmapToShow = gmapToShow.attr("id").replace("map", "initialize_")+"();";
});

function show_map(){
$("#liens-google-map-69-5").toggle("slow",function(){eval(gmapToShow);});
}

$(".fancybox-articles").fancybox({
			type: "iframe",
			autoSize : true,
