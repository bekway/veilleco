                       $("#agenda-older-79").height(height_breve);
	
                       $("#agenda-newer-79").html("");
            
                       $("#agenda-older-79").animate({
                          opacity: 0
                        }, 500, "linear", function() {
                        $("#agenda-older-79").html("");
                        $("#agenda-older-79").css("opacity","1");
                       
                        });
                  
                        index--;
                        if(index < 0)
                             index = module_article_breve_79.length;
               
