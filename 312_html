		<script type="text/javascript">
		jQuery(document).ready(function(){
			var sudoSlider4 = jQuery("#home_social_network_slider").sudoSlider({
				responsive: true,
				prevNext: true,
				numeric: false,
				continuous: true,
				slideCount: 4,
                moveCount: 2
			});
			jQuery(window).on("resize focus", function () {
                    var width = jQuery("#home_social_network_slider").width();
                    var orgSlideCount = sudoSlider4.getOption("slideCount");
                    var slideCount;
                    var moveCount;
                    if (width >= 920) {
                        slideCount = 4;
                        moveCount = 2;
                    } 
                    else if (width >= 545) {
                        slideCount = 3;
                        moveCount = 1;
                    }
                    else {
                        slideCount = 1;
                        moveCount = 1;
                    } 
                    if (slideCount != orgSlideCount) {
                        sudoSlider4.setOption("slideCount", slideCount);
                        sudoSlider4.setOption("moveCount", moveCount);
                    }
            }).resize();
		});
		</script>
