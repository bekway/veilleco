		<script type="text/javascript">
	$(".social_wall_single_tw_content").html(function(){
	  	var text= $(this).text().trim().split(" ");
	  	var last = text.pop();
	  	return text.join(" ") + (text.length > 0 ? " <span class='social_wall_single_tw_content_span'>" + last + "" : last);
	});
