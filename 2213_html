			<script>
	var cpt=0;
	setInterval(function() {
		var cpt_prec;
		var decalage;
		cpt++;
		if(cpt==4) cpt=0;
		cpt_prec=cpt-1;
		if(cpt_prec==-1) cpt_prec=3;
		decalage=0.5+(101*cpt);
		if(decalage>303.5) decalage=0.5;
		jQuery(".post-ref-"+cpt_prec).removeClass("is-selected");
		jQuery(".post-ref-"+cpt).addClass("is-selected");
		jQuery(".flickity-page-dots li:eq("+cpt_prec+")").removeClass("is-selected");
		jQuery(".flickity-page-dots li:eq("+cpt+")").addClass("is-selected");
		jQuery(".flickity-slider").css("transform","translateX(-"+decalage+"%)");
		//alert("10s"+cpt);
	}, 6 * 1000); 
			</script>
