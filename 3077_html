					<section id="content">
						<div class="article-flex">
							<div class="articleBody" style="width: 100%;"><span id="c8385" class="anchor"></span>
								<div id="d8385" class="content colPos0 setPos isHidden0">
									<div class="rte">
										<h3>Retransmission</h3>
									</div>
									<div class="videos">
