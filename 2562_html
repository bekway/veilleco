		function textZoom( value )
		{
			var fontSize = parseInt( jQuery('body').css('font-size') ) * value;
			jQuery('body').css('font-size', fontSize + 'px');
			// jQuery.cookie( 'fontSize', fontSize );
		}
		
		jQuery(document).ready(function() {
			// if( $.cookie('fontSize') )
			// {
				// var fontSize = $.cookie('fontSize');
				// $('body').css('font-size', fontSize + 'px');
			// }
		
			jQuery(".zoomIn").click(function(){
				textZoom( 1.2 );
			});
			jQuery(".zoomOff").click(function(){
				textZoom( 1 );
			});
			jQuery(".zoomOut").click(function(){
				textZoom( 0.9 );
			}); 
			
			
			jQuery('#homeFlexsliderContent').flexslider({
				directionNav	: false,
				manualControls	: '#homeFlexsliderNav ul li',
				slideshowSpeed: 5500,
				animationSpeed: 0,
				start: function(slider) {
					jQuery('#homeFlexsliderNav ul li')
					.mouseover(function(){
						var activeSlide = 'false';
						if (jQuery(this).hasClass('flex-active')){  
							activeSlide = 'true';                       
						}
						if (activeSlide == 'false'){
							jQuery(this).trigger("click"); 
						}
					})
					.click(function(){
						if (jQuery(this).hasClass('flex-active')){  
							var index 	= jQuery('#homeFlexsliderNav ul li').index(this);
							var url 	= jQuery('#homeFlexsliderContent ul li:eq('+ index +') a').attr('href');
							window.location.href = url;
						}
					});
				},
				// before: function(slider) {
					// jQuery('#homeFlexsliderNav ul li').removeClass('flex-active');
					// jQuery('#homeFlexsliderNav ul li:eq('+ slider.animatingTo +')').addClass('flex-active');
				// },
			});
			
