			function newsletter_validation_E5F1FAA8EADA41CB87842A8447988B26() {
				var email = $("#inputNewsletter-E5F1FAA8EADA41CB87842A8447988B26").val();
				var modele = /^[a-zA-Z0-9\.\-_]+@[a-zA-Z0-9\-]+[a-zA-Z0-9\.\-]+\.[a-zA-Z]{2,5}$/i;
	
				if (!modele.test(email))
				{
				custom_alert ("Veuillez saisir une adresse e-mail valide svp");
			
				} else {
					//code ajax d enregistrement
	
				$.ajax ({
					url: "/modules/newsletter/newsletter_client-ajax.php",
					type: "POST",
					data: {
						action : "insert",
						mail : $("#inputNewsletter-E5F1FAA8EADA41CB87842A8447988B26").val(),
						type : $("#selectNewsletter-E5F1FAA8EADA41CB87842A8447988B26").val(),
						token : $("#token").val()
					},
					success: function(data) {
					custom_alert (data);
						$("#inputNewsletter-E5F1FAA8EADA41CB87842A8447988B26").val("");
					},
				});
	
				}
			}
			
					</script>
