function msieversion() {

   var ua = window.navigator.userAgent;
   var msie = ua.indexOf("MSIE ");

   if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./))  // If Internet Explorer, return version number
   {
       return true;
   }
   else  // If another browser, return 0
   {
       return false;
   }

   return false;
}
$(document).ready(function() {
           if (msieversion()) {
               $('.svg-content-active svg').each(function() {
                   this.style.height = "750px";
               });
           }

           var $win = $(window);
var $headrBloc = $('.template-header-content div.header-left');
           $headrBloc.css('transition', "all .3s ease");
           $win.scroll(function() {
                       if ($win.scrollTop() >= 40) {
                         $headrBloc.css('transform','translateY(-16%) translateX(-50px) scale(0.7)');
                         } else {
$headrBloc.css('transform','none');
;}
}
                      )
           ;
});
		</script>
