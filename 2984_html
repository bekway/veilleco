				<section id="block-views-actualit-s-block" class="block block-views clearfix">
					<div class="view view-actualit-s view-id-actualit_s view-display-id-block view-dom-id-6dd1129af0116ca8b3046ec31142e64b">
						<div class="view-header">
							<h2>Actualités</h2>
							<div class="tous-link"><a href="/actualites">Toutes les actualités</a></div>
						</div>
						<div class="view-content">
