  $('#lien_ancre').click(function() {
    $('html,body').animate({scrollTop: 0}, 'slow');
  });

  $(window).scroll(function(){
     if($(window).scrollTop()<50){
        $('#lien_ancre').fadeOut();
     }else{
        $('#lien_ancre').fadeIn();
     }
  });
});
