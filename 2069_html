							<script>$(function() {
					var $list = $('#js-section-map-list'),
						$map = $('#js-section-territory-svg');
					$list
						.on('mouseenter', 'a', function () {
							var city = $(this).data('path');
							$map.find('#' + city + '> path').addClass('active');
							$map.find('#' + city + '> polygon').addClass('active');
						})
						.on('mouseleave', 'a', function () {
							var city = $(this).data('path');
							$map.find('#' + city + '> path').removeClass('active');
							$map.find('#' + city + '> polygon').removeClass('active');
						});
					$map
						.on('mouseenter', 'path, polygon', function () {
							var city = $(this).parent().attr('id');
							$list.find('#list-' + city).parent().addClass('active');
						})
						.on('mouseleave', 'path, polygon', function () {
							var city = $(this).parent().attr('id');
							$list.find('#list-' + city).parent().removeClass('active');
						});
				});
							</script>
