		<script type="text/javascript">
			$( document ).ready(function() {
			    if($.cookie('acceptedCookie') == 'true' ){
			    	$( "#cookies-warning" ).hide();
			    }else{
			    	$( "#cookies-warning" ).show();
			    	$( "#cookies-warning" ).css("display" , "flex");
			    }

			    $("#closeWarningCookie" ).click(function() {
				  $( "#cookies-warning" ).hide();
				});

				$("#accepteCookies" ).click(function() {

					$.cookie('acceptedCookie', 'true');
				  	
				  	$("#cookies-warning" ).hide();
				});
			});
		
