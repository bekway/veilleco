		<script type="text/javascript">jQuery(document).ready(function( $ ){
    
  $('.avia_textblock:has(.bloc-lien-page-parent)').css("border", "1px solid #007099");
  
  $('.avia_textblock:has(.bloc-lien-page-parent)').mouseenter(function() {
      $( this ).css("background-color","#007099");
      $( this ).css( 'cursor', 'pointer' );
      $( this ).find("img").css("filter","brightness(0) invert(1)");
      $( this ).find("p:last-child").css("filter","brightness(0) invert(1)");
  });

   $('.avia_textblock:has(.bloc-lien-page-parent)').mouseleave(function() {
      $( this ).css("background-color","");
	  $( this ).css( 'cursor', '' );
      $( this ).find("img").css("filter","brightness(1) invert(0)");
      $( this ).find("p:last-child").css("filter","brightness(1) invert(0)");
  });
  

   $('.avia_textblock:has(.bloc-lien-page-parent)').click(function() {
     	var pathname = window.location.pathname;
     	if(pathname.indexOf('espace-de-travail') == -1){
            var addressValue = $(this).find("a").attr("href");
            window.location.href = addressValue;  
   		}
  });

});
