		<script type="text/javascript">jQuery(document).ready(function( $ ){
  
      var types = ['enfouissement','gravats','dechets-verts',
                   'cartons','deee','piles---accumulateurs',
                   'bois','ameublement','lampes---neons','textiles',
                   'cartouches-d-encre','huiles-de-vidange',
                   'huiles-de-friture','dechets-dangereux-des-menages',
                   'batteries','metaux','capsules-de-cafe','polystyrene',
                   'plastique-dur','radiographies','collecte-selective',
                   'reutilisation','pneumatiques'];
                 
             

      function affiche($type) {

        for(var i= 0; i < types.length; i++){

            $(".".concat(types[i])).each(function( index ) {

                var classList = $( this ).attr('class').split(/\s+/);

                if($.inArray($type, classList) >= 0){	
                    $(this).css({"display": "inline"}); 
                    $(this).removeClass("first");
                }else{
                    $(this).css({"display": "none"}); 
                }

            });
        }
        
      }

  
  
      $("#choix-type-dechet").change(function(){
        
          var choix = $(this).children("option:selected").val();

          if(choix == "Tout"){
            
              for(var i= 0; i < types.length; i++){

                  $(".".concat(types[i])).each(function( index ) {
                     $(this).css({"display": "inline"}); 
                  });
                
              }
		
          }else{
               // Pour normaliser le choix en classe CSS existante
               $(affiche(choix.toLowerCase().split(' ').join('-').split('/').join('-').split('\'').join('-').normalize('NFD').replace(/[\u0300-\u036f]/g, "")));
          }

      });
  
  
});
		</script>
