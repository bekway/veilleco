		<!-- Menu Interne Mobile (deplct Menu droite)  -->
		<script type="text/javascript"> 
 jQuery.noConflict();
 // affiche ou cache le menu interne mobile 
 // la premiere fois - deplacement du menu interne mobile de droite sous le chemin
 var v_menu_interne_mobile = 0;
 jQuery("#BT_MENU_INTERNE_MOBILE").click(function() {
      if (v_menu_interne_mobile == 0 )
      {
        v_menu_interne_mobile = 1;
        jQuery( "#MENU_INTERNE_MOBILE").append(jQuery("#MENU_DROITE"));            
      }  
      
      jQuery( "#MENU_DROITE" ).toggleClass( "affiche" );

  });
  // affiche ou cache le menu interne droite
 jQuery("#BT_MENU_INTERNE").click(function() {
      jQuery( "#L_MENU_DROITE" ).toggleClass( "affiche" );
  });
		</script>
