								<!--  CONTENT ELEMENT, uid:24984/page_php_content_pi1 [begin] -->
								<!--<script type="text/javascript" src="http://www.jesoutiensnimes.fr/templates/t3_bs3_blank/js/jquery.countTo.js"></script>-->
								<script type="text/javascript" src="https://www.nimes.fr/fileadmin/v2/js/jquery.countTo.js"></script>
								<script type="text/javascript">
	    var nbre_soutiens = 49898;
	    jQuery(document).ready(function() {					
		        jQuery('#count').addClass('visible');
		        jQuery('#count').text('0');
						jQuery('p:empty').hide();
		    });
		    
		    jQuery(window).load(function() {
		        
		        /************* COUNT TO **************/

		        jQuery('#count').countTo({
		            from: 0, 
		            to: nbre_soutiens, 
		            speed: 2000, 
		            refreshInterval: 20,
		            formatter: function (value, options) {
		                return value.toFixed(options.decimals).replace(/\B(?=(?:\d{3})+(?!\d))/g, ' ');
		            },
		            onUpdate: function (value) {
		                jQuery('#count').addClass('visible');
		            }
		        });					
						deldivunesco();
	    });
		
								</script>
