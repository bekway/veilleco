    });

    if (-1 != -1) {
	    $('img').click(function () {
	    	setTimeout(function () {
	    		correction();
	    	}, 2000);
	    });
	    function correction() {
	     	$('img').each(function () {
	       		if($(this).attr("src").indexOf('lightbox-btn') != -1) {
	       			$(this).attr("src","../../"+$(this).attr("src"));
	       		}
	        });
	    };
    }

	$("a[href$='.png']").lightBox();
	$("a[href$='.jpg']").lightBox();
	$("a[href$='.gif']").lightBox();
 	$("a[href$='.swf']").lightBox();
 	$('.eb_carrousel').diaporousel();
