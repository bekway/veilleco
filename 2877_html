	     	{
	     		veureyHideAllWidgets();
	     	}
	        jQuery("#veurey_shortlink_contact_slider").slideToggle();
	     });
	});
	function veureyHideAllWidgets()
	{
		jQuery("#veurey_shortlink_tag_slider:visible").hide();
		jQuery("#veurey_shortlink_meteo_slider:visible").hide();
		jQuery("#veurey_shortlink_partager_slider:visible").hide();
		jQuery("#veurey_shortlink_contact_slider:visible").hide();
	}
