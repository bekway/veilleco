				<script>
    $(document).ready(function(){
        $('#pm-menu').multilevelpushmenu({
          container: $('#pm-menu'),
          containersToPush: [$('#pm-content')],
          collapsed: true,
          menuWidth: 300,
          backText: 'Retour',
          mode: 'cover',
          preventItemClick: false,
          fullCollapse: true
        });

        // Toggle button
        $( '#pm-togglebtn' ).click(function(){
          var menuExpanded = $( '#pm-menu' ).multilevelpushmenu( 'menuexpanded' , $( '#pm-menu' ).multilevelpushmenu( 'findmenusbytitle' , 'CC Muse et Raspes' ).first() );
          if( menuExpanded ){
            $( '#pm-menu' ).multilevelpushmenu( 'collapse' );
            $( '#pm-togglebtn' ).removeClass( 'collapsable' );
          }
          else {
            $( '#pm-menu' ).multilevelpushmenu( 'expand' );
            $( '#pm-togglebtn' ).addClass( 'collapsable' );
          }
        });

        // Search button
        $( '#pm-searchbtn' ).click(function(){
          $( '.togglesearch' ).slideToggle('fast',function() {
				    if ($(this).is(':visible'))
					   $( '#pm-searchbtn' ).addClass('collapsable');
            else
             $( '#pm-searchbtn' ).removeClass('collapsable');
			     });
        });

        // Desktop Search
        $( '#searchbtn' ).click(function(){
          $( '.togglesearch' ).slideToggle('fast',function() {
				    if ($(this).is(':visible'))
					   $( '#searchbtn' ).addClass('collapsable');
            else
             $( '#searchbtn' ).removeClass('collapsable');
			     });
        });

    });
    
